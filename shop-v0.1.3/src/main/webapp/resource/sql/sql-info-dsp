  `DLVY_COMPLETE_DT` datetime DEFAULT NULL COMMENT '배송완료시간',
  `DLVY_UNSUCC_CD` char(2) DEFAULT '00' COMMENT '배송실패사유(코드)',
  `DLVY_DESC` varchar(511) DEFAULT NULL COMMENT '배송상세정보',
  `PASSPORT_NO` varchar(30) DEFAULT NULL,
  `RECIPIENT_NM` varchar(30) DEFAULT NULL COMMENT '수령인',
  `RECIPIENT_NM_EN` varchar(30) DEFAULT NULL COMMENT '수령인(영문)',
  `RECIPIENT_TEL` varchar(30) DEFAULT NULL COMMENT '수령인 전화번호',
  `RECIPIENT_DESC` varchar(255) DEFAULT NULL COMMENT '상세객실정보',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`DLVY_GRP_SEQNO`),
  KEY `FK_TB_PLM_DLVY_RECV_ADDR_MST_TO_TB_ORDER_DLVY_PROD_DELIVERY_GRP` (`DLVY_RECV_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_ORDER_DLVY_PROD_DELIVERY_GRP` (`UNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_ORDER_DLVY_PROD_DELIVERY_GRP` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_ORDER_DLVY_PROD_DELIVERY_GRP` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_PLM_DLVY_RECV_ADDR_MST_TO_TB_ORDER_DLVY_PROD_DELIVERY_GRP` FOREIGN KEY (`DLVY_RECV_SEQNO`) REFERENCES `TB_PLM_DLVY_RECV_ADDR_MST` (`DLVY_RECV_SEQNO`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_ORDER_DLVY_PROD_DELIVERY_GRP` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=1013 DEFAULT CHARSET=utf8 COMMENT='주문_배송상품 배송(Delivery)정보 - 그룹';

