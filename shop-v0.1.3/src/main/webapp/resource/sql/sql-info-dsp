-- Table structure for TB_PLM_LOGI_DLVY_TR_INFO
-- ----------------------------
DROP TABLE IF EXISTS `TB_PLM_LOGI_DLVY_TR_INFO`;
CREATE TABLE `TB_PLM_LOGI_DLVY_TR_INFO` (
  `LOGI_DLVY_TR_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '배송(운송장)SEQ',
  `JOB_STATUS_CD` char(2) DEFAULT '00' COMMENT '10(작업확정/회수요청/교환접수/반품접수/출고대기) 20(피킹/출고확인/패킹대기) 30(패킹완료) 31(패킹반려) 40(상차) 50(배송완료/회수완료/반품완료/교환완료) 60(배송실패/회수실패/반품실패/교환실패) ',
  `RECEIPT_STATUS_CD` char(2) DEFAULT '00' COMMENT '11(반품입고) 12(교환입고) 13(패킹반려) 20(재입고완료) 30(입고파기) 40(입고반품)',
  `LOGI_DLVY_TRACKING_NO` varchar(30) DEFAULT NULL COMMENT '13자리 = DSP + 타입(1) + YYMM + MOD(SEQ, 100001)',
  `LOGI_DLVY_TRACKING_BRCD` varchar(30) DEFAULT NULL COMMENT 'DSP + 타입(1) + YYMM + MOD(SEQ, 100001)',
  `LOGI_DLVY_TY` char(2) DEFAULT '00' COMMENT '60(배송) 70(반품) 80(교환) 90(긴급배송) 91(긴급회수)',
  `LOGISTICS_REG_YN` char(1) DEFAULT 'N' COMMENT '작업등록여부',
  `LOGISTICS_PROC_CD` char(2) DEFAULT '00' COMMENT '00(대기) 10(상차/확정)',
  `LOGI_DLVY_QTY` int(11) DEFAULT NULL COMMENT '(배송)수량',
  `DLVY_AREA_CODE` int(11) DEFAULT NULL COMMENT '배송권역코드(SEQNO)',
  `DELIVERER_SEQNO` int(11) DEFAULT NULL COMMENT '배송직원SEQ',
  `COURIER_CAR_SEQNO` int(11) DEFAULT NULL COMMENT '배송차량SEQ',
  `LOGI_DLVY_LADE_CAR_DT` datetime DEFAULT NULL COMMENT '상차시간',
  `LOGI_DLVY_UNLADE_CAR_DT` datetime DEFAULT NULL COMMENT '하차시간',
  `DLVY_PROC_CD` char(2) DEFAULT '00' COMMENT '배송상태:00(물품준비) 10(배송준비) 11(배송중/쿠폰발송) 20(배송완료) 30(배송실패) 31(지연실패) ',
  `DLVY_DATE_TZ_CD` varchar(20) DEFAULT NULL COMMENT 'TB_CODE(DLY-TZ)',
  `DLVY_COMPLETE_YN` char(1) DEFAULT 'Y' COMMENT '배송완료여부: Y,N',
  `DLVY_COMPLETE_DT` datetime DEFAULT NULL COMMENT '배송완료시간',
  `DLVY_UNSUCC_CD` char(2) DEFAULT '00' COMMENT '배송실패:00(없음) 11(부재중) 12(수령거부) 21(교환) 31(반품) 41(결제취소) 90(지연)',
  `DLVY_DELAY_CD` char(2) DEFAULT '00' COMMENT '00(없음) 10, 20, 30, ',
  `DLVY_MEMO` varchar(255) DEFAULT NULL COMMENT '배송메모',
  `RECIPIENT_NM` varchar(30) DEFAULT NULL COMMENT '수령인',
  `RECIPIENT_NM_EN` varchar(30) DEFAULT NULL COMMENT '수령인(영문)',
  `RECIPIENT_TEL` varchar(30) DEFAULT NULL COMMENT '수령인 전화번호',
  `DLVY_RECV_SEQNO` int(11) NOT NULL COMMENT '배송지정보(우편번호, 배송주소1~3)',
  `RECIPIENT_DESC` varchar(255) DEFAULT NULL COMMENT '상세객실정보',
  `LOGI_DLVY_DATE` varchar(8) DEFAULT NULL COMMENT '(배송)배송일자',
  `RETURN_TY` char(2) DEFAULT '00' COMMENT '회수사유 10(업체요청), 20(내부이슈)',
  `RETURN_MEMO` varchar(255) DEFAULT NULL COMMENT '회수메시지',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`LOGI_DLVY_TR_SEQNO`),
  KEY `FK_TB_PLM_COURIER_CAR_INFO_TO_TB_PLM_LOGI_DLVY_TR_INFO` (`COURIER_CAR_SEQNO`),
  KEY `FK_TB_DLVY_AREA_MST_TO_TB_PLM_LOGI_DLVY_TR_INFO` (`DLVY_AREA_CODE`),
  KEY `FK_TB_COURIER_DELIVERER_INFO_TO_TB_PLM_LOGI_DLVY_TR_INFO` (`DELIVERER_SEQNO`),
  CONSTRAINT `FK_TB_COURIER_DELIVERER_INFO_TO_TB_PLM_LOGI_DLVY_TR_INFO` FOREIGN KEY (`DELIVERER_SEQNO`) REFERENCES `TB_COURIER_DELIVERER_INFO` (`DELIVERER_SEQNO`),
  CONSTRAINT `FK_TB_DLVY_AREA_MST_TO_TB_PLM_LOGI_DLVY_TR_INFO` FOREIGN KEY (`DLVY_AREA_CODE`) REFERENCES `TB_DLVY_AREA_MST` (`DLVY_AREA_CODE`),
  CONSTRAINT `FK_TB_PLM_COURIER_CAR_INFO_TO_TB_PLM_LOGI_DLVY_TR_INFO` FOREIGN KEY (`COURIER_CAR_SEQNO`) REFERENCES `TB_PLM_COURIER_CAR_INFO` (`COURIER_CAR_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=52 DEFAULT CHARSET=utf8 COMMENT='물류-배송기본정보';

-- ----------------------------
-- Table structure for TB_PLM_LOGI_DLVY_TR_ISSUE
-- ----------------------------
DROP TABLE IF EXISTS `TB_PLM_LOGI_DLVY_TR_ISSUE`;
CREATE TABLE `TB_PLM_LOGI_DLVY_TR_ISSUE` (
  `LOGI_DLVY_TR_SEQNO` int(11) NOT NULL COMMENT '배송(운송장)SEQ',
  `PLM_PROD_SEQNO` int(11) NOT NULL COMMENT 'PLM_상품정보SEQ',
  `ORDER_SEQNO` int(11) NOT NULL COMMENT 'ORDER_SEQNO',
  `STOCK_ISSUE_CFM_QTY` tinyint(4) DEFAULT '0' COMMENT '출고확인수량',
  `STOCK_ISSUE_YN` char(1) DEFAULT 'N' COMMENT '출고확인여부',
  `STOCK_ISSUE_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '출고확인시간',
  `STOCK_ISSUE_QTY` tinyint(4) DEFAULT '0' COMMENT '출고수량',
  PRIMARY KEY (`LOGI_DLVY_TR_SEQNO`,`PLM_PROD_SEQNO`,`ORDER_SEQNO`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_LOGI_DLVY_TR_ISSUE` (`PLM_PROD_SEQNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_PLM_LOGI_DLVY_TR_ISSUE` (`ORDER_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='물류-배송 출고내역';

-- ----------------------------
-- Table structure for TB_PLM_LOGI_DLVY_TR_JOB_LIST
-- ----------------------------
DROP TABLE IF EXISTS `TB_PLM_LOGI_DLVY_TR_JOB_LIST`;
CREATE TABLE `TB_PLM_LOGI_DLVY_TR_JOB_LIST` (
  `JOB_ORDER_SEQNO` int(11) NOT NULL COMMENT '작업할당SEQNO',
  `LOGI_DLVY_TR_SEQNO` int(11) NOT NULL COMMENT '배송(운송장)SEQ',
  `JOB_ORDER_REQ_DATE` varchar(8) DEFAULT NULL COMMENT '20160301 (X)20160302',
  `LOGI_DLVY_TY` char(2) DEFAULT '00' COMMENT '60(배송) 70(반품/회수) 80(교환) 91(긴급회수)',
  `LOG_DLVY_REQ_DATE` varchar(8) DEFAULT NULL COMMENT '20160303  (I) 20160305',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORDER_SEQNO',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`JOB_ORDER_SEQNO`,`LOGI_DLVY_TR_SEQNO`),
  KEY `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_LOGI_DLVY_TR_JOB_LIST` (`LOGI_DLVY_TR_SEQNO`),
  CONSTRAINT `FK_TB_PLM_JOB_ORDER_LIST_TO_TB_PLM_LOGI_DLVY_TR_JOB_LIST` FOREIGN KEY (`JOB_ORDER_SEQNO`) REFERENCES `TB_PLM_JOB_ORDER_LIST` (`JOB_ORDER_SEQNO`),
  CONSTRAINT `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_LOGI_DLVY_TR_JOB_LIST` FOREIGN KEY (`LOGI_DLVY_TR_SEQNO`) REFERENCES `TB_PLM_LOGI_DLVY_TR_INFO` (`LOGI_DLVY_TR_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='물류-작업확정-작업요청내역';

