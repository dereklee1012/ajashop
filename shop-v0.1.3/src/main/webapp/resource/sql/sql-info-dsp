DROP TABLE IF EXISTS `TB_BRAND_MST`;
CREATE TABLE `TB_BRAND_MST` (
  `BRAND_CODE` char(10) NOT NULL DEFAULT '00000' COMMENT '브랜드코드',
  `COMPANY_CODE` char(10) DEFAULT NULL COMMENT '업체코드',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `LEVEL` tinyint(4) NOT NULL DEFAULT '0' COMMENT '레벨',
  `UPPER_BRAND_CODE` char(10) DEFAULT NULL COMMENT '상위 브랜드코드',
  `BRAND_TY` char(1) NOT NULL DEFAULT 'D' COMMENT 'D(배송상품) C(쿠폰상품)',
  `BRAND_NAME_EN` varchar(30) DEFAULT NULL COMMENT 'BRAND_명(영어)',
  `BRAND_NAME_KO` varchar(30) DEFAULT NULL COMMENT 'BRAND_명(한국어)',
  `BRAND_NAME_ZH` varchar(30) DEFAULT NULL COMMENT 'BRAND_명(간체)',
  `BRAND_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '브랜드이미지PATH',
  `BRAND_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '브랜드이미지 화일명칭',
  `BRAND_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '브랜드이미지(원)화일명칭',
  `BRAND_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '브랜드이미지 사이즈_H',
  `BRAND_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '브랜드이미지 사이즈_V',
  `BANNER_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '[브랜드배너이미지경로]',
  `BANNER_IMG_NM` varchar(128) DEFAULT NULL COMMENT '[브랜드배너이미지명]',
  `BANNER_ORG_IMG_NM` varchar(128) DEFAULT NULL COMMENT '[브랜드배너(원)이미지명]',
  `BANNER_IMG_SIZE_H` decimal(4,0) DEFAULT NULL COMMENT '[브랜드배너 사이즈_H2',
  `BANNER_IMG_SIZE_V` decimal(4,0) DEFAULT NULL COMMENT '[브랜드배너 사이즈_V2',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출 여부',
  `DISP_ORDER` tinyint(4) NOT NULL DEFAULT '1' COMMENT '노출순서',
  `MNMNU_EXPSR_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '메인메뉴 노출여부',
  `SALE_PER` varchar(10) DEFAULT NULL COMMENT '50% -> 50',
  `NAKJEON_DD_RATE` decimal(6,3) DEFAULT NULL COMMENT '낙전(딩동 %)',
  `NAKJEON_SUPPLIER_RATE` decimal(6,3) DEFAULT NULL COMMENT '낙전(공급사 %)',
  `TURNOVER_TY` char(1) DEFAULT NULL COMMENT 'S(공급가) E(업체판매가)',
  `RELEVANT_WORDS` varchar(1023) DEFAULT NULL COMMENT '관련주제 키워드, 해시태그',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`BRAND_CODE`),
  KEY `FK_TB_COMPANY_INFO_TO_TB_BRAND_MST` (`COMPANY_CODE`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_BRAND_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_BRAND_MST` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_COMPANY_INFO_TO_TB_BRAND_MST` FOREIGN KEY (`COMPANY_CODE`) REFERENCES `TB_COMPANY_INFO` (`COMPANY_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='TB_BRAND_MST(브랜드 정보)';

---------------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_CART_PROD_COUPON_INFO`;
CREATE TABLE `TB_CART_PROD_COUPON_INFO` (
  `CART_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'CART_SEQNO',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `PRODUCT_UUID` int(11) NOT NULL COMMENT '상품UUID',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `PROD_UUID` int(11) DEFAULT NULL COMMENT 'PROD_COUPON_UUID or PORD_DLVY_UUID',
  `PROD_CODE` char(10) NOT NULL COMMENT 'D: Delivery C: Coupon L:Local coupon',
  `BRAND_CODE` char(10) DEFAULT NULL COMMENT '브랜드코드',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `SUPLER_GOODS_CODE` varchar(32) DEFAULT NULL COMMENT '공급사상품코드',
  `WAREHOUSING_TY` char(3) DEFAULT NULL COMMENT '입고방식 PRE(선구매) PST(후발주)',
  `PUT_ITEM_CART_DT` datetime DEFAULT NULL COMMENT '카드 보관 일시',
  `PROD_SALE_END_DT` datetime DEFAULT NULL COMMENT '판매 종료일시',
  `SHELF_LIFE_DT` datetime DEFAULT NULL COMMENT '식품유효기간',
  `ITEM_QTY` int(11) DEFAULT '0' COMMENT '물품 수량',
  `SELLING_PRICE` decimal(13,0) DEFAULT '0' COMMENT '가격(판매가*수량+옵션)',
  `SELLING_PRICE_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '가격(판매가*수량+옵션) (위안화)',
  `RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '시중 판매가/소비자가',
  `RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '시중 판매가/소비자가(위안화)',
  `DISCOUNT_RATE` decimal(6,3) NOT NULL DEFAULT '0.000' COMMENT '할인율(시중가대비)',
  `PROD_NAME_EN` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(영어)',
  `PROD_NAME_KO` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(한국어)',
  `PROD_NAME_ZH` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(간체)',
  `PROD_OPTION_YN` char(1) DEFAULT 'N' COMMENT '상품옵션 유무',
  `PROD_OPTION_LEVEL` tinyint(4) DEFAULT '0' COMMENT '상품옵션 LEVEL',
  `GENUINE_MARK_PROD_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '정품인증마크 적용여부',
  `ODER_PROD_YN` char(1) NOT NULL DEFAULT 'N' COMMENT 'ORDER_PROD_YN	주문상품여부 - 카드에서 주문할 상품여부(Y,N)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`CART_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_CART_PROD_COUPON_INFO` (`UNO`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_CART_PROD_COUPON_INFO` (`PRODUCT_UUID`),
  KEY `FK_TB_PROD_COUPON_INFO_TO_TB_CART_PROD_COUPON_INFO` (`PROD_UUID`),
  KEY `FK_TB_BRAND_MST_TO_TB_CART_PROD_COUPON_INFO` (`BRAND_CODE`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_CART_PROD_COUPON_INFO` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_BRAND_MST_TO_TB_CART_PROD_COUPON_INFO` FOREIGN KEY (`BRAND_CODE`) REFERENCES `TB_BRAND_MST` (`BRAND_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_CART_PROD_COUPON_INFO` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_CART_PROD_COUPON_INFO` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_PROD_COUPON_INFO_TO_TB_CART_PROD_COUPON_INFO` FOREIGN KEY (`PROD_UUID`) REFERENCES `TB_PROD_COUPON_INFO` (`PROD_COUPON_UUID`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_CART_PROD_COUPON_INFO` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=593 DEFAULT CHARSET=utf8 COMMENT='사용자-쿠폰(Coupon)상품_카트 정보';

