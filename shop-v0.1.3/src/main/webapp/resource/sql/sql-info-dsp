  `NW_ITEM_THUMBNAIL_SIZE_H` int(11) DEFAULT NULL COMMENT '주요아이템 썸네일 사이즈_H',
  `NW_ITEM_THUMBNAIL_SIZE_V` int(11) DEFAULT NULL COMMENT '주요아이템 썸네일 사이즈_V',
  `NW_ITEM_THUMBNAIL_FILE_NM` varchar(128) DEFAULT NULL COMMENT '주요아이템 썸네일 파일명',
  `NW_ITEM_THUMBNAIL_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '주요아이템 썸네일 (원)파일 명칭',
  `EXPSR_MBER_SCOPE_CODE` char(1) DEFAULT 'A' COMMENT '노출회원범위코드 A(전체) M(회원) N(비회원)',
  `MVMN_SETUP` char(1) DEFAULT 'D' COMMENT '이동설정 D:배송 C:쿠폰 S:스페셜 E:이벤트 T:테마 S:매장',
  `LINK_URL` varchar(512) DEFAULT NULL COMMENT '링크URL',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드',
