  `COURIER_DLVY_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '배송비용SEQ',
  `ORDER_SEQNO` int(11) NOT NULL COMMENT 'ORDER_SEQNO',
  `COURIER_DLVY_FEE_SEQNO` int(11) DEFAULT NULL COMMENT '배송비용 요금SEQ',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `PAY_COURIER_DLVY_FEE_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '배송비_USD',
  `PAY_COURIER_DLVY_FEE_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '배송비_KRW',
  `PAY_COURIER_DLVY_FEE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '배송비_CNY',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`COURIER_DLVY_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_ORDER_COURIER_DLVY_FEE` (`UNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_ORDER_COURIER_DLVY_FEE` (`ORDER_SEQNO`),
  KEY `FK_TB_COURIER_DLVY_FEE_INFO_TO_TB_ORDER_COURIER_DLVY_FEE` (`COURIER_DLVY_FEE_SEQNO`),
  CONSTRAINT `FK_TB_COURIER_DLVY_FEE_INFO_TO_TB_ORDER_COURIER_DLVY_FEE` FOREIGN KEY (`COURIER_DLVY_FEE_SEQNO`) REFERENCES `TB_COURIER_DLVY_FEE_INFO` (`COURIER_DLVY_FEE_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_ORDER_COURIER_DLVY_FEE` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_ORDER_COURIER_DLVY_FEE` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='주문_배송요금 정보';

