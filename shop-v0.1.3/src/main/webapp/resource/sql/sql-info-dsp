DROP TABLE IF EXISTS `TB_PLM_STOCK_MOVEMENT_HST`;
CREATE TABLE `TB_PLM_STOCK_MOVEMENT_HST` (
  `STOCK_MOVEMENT_INFO` int(11) NOT NULL AUTO_INCREMENT COMMENT '재고상품 이동SEQNO',
  `STOCK_SEQNO` int(11) DEFAULT NULL COMMENT '재고관리SEQ',
  `WRHOUS_SEQNO` int(11) DEFAULT NULL COMMENT '(변경전)물류창고SEQNO',
  `WRHOUS_CMPRT_SEQNO` int(11) DEFAULT NULL COMMENT '(변경전)창고구획SEQNO',
  `TO_WRHOUS_SEQNO` int(11) DEFAULT NULL COMMENT '(변경후)물류창고SEQNO',
  `TO_WRHOUS_CMPRT_SEQNO` int(11) DEFAULT NULL COMMENT '(변경후)창고구획SEQNO',
  `MOVEMENT_RSN_CD` char(2) DEFAULT '00' COMMENT 'TB_CODE(STMOVE) 10(재배치)',
  `MOVEMENT_RSN_DESC` varchar(255) DEFAULT NULL COMMENT '변경사유-상세',
  `MOVEMENT_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '이동수량',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`STOCK_MOVEMENT_INFO`),
  KEY `FK_TB_PLM_WRHOUS_COMPART_INFO_TO_TB_PLM_STOCK_MOVEMENT_HST` (`WRHOUS_CMPRT_SEQNO`),
  KEY `FK_TB_PLM_WAREHOUSE_MST_TO_TB_PLM_STOCK_MOVEMENT_HST` (`WRHOUS_SEQNO`),
  KEY `FK_TB_PLM_STOCK_INFO_TO_TB_PLM_STOCK_MOVEMENT_HST` (`STOCK_SEQNO`),
  KEY `FK_TB_PLM_WAREHOUSE_MST_TO_TB_PLM_STOCK_MOVEMENT_HST2` (`TO_WRHOUS_SEQNO`),
  KEY `FK_TB_PLM_WRHOUS_COMPART_INFO_TO_TB_PLM_STOCK_MOVEMENT_HST2` (`TO_WRHOUS_CMPRT_SEQNO`),
  CONSTRAINT `FK_TB_PLM_STOCK_INFO_TO_TB_PLM_STOCK_MOVEMENT_HST` FOREIGN KEY (`STOCK_SEQNO`) REFERENCES `TB_PLM_STOCK_INFO` (`STOCK_SEQNO`),
  CONSTRAINT `FK_TB_PLM_WAREHOUSE_MST_TO_TB_PLM_STOCK_MOVEMENT_HST` FOREIGN KEY (`WRHOUS_SEQNO`) REFERENCES `TB_PLM_WAREHOUSE_MST` (`WRHOUS_SEQNO`),
  CONSTRAINT `FK_TB_PLM_WAREHOUSE_MST_TO_TB_PLM_STOCK_MOVEMENT_HST2` FOREIGN KEY (`TO_WRHOUS_SEQNO`) REFERENCES `TB_PLM_WAREHOUSE_MST` (`WRHOUS_SEQNO`),
  CONSTRAINT `FK_TB_PLM_WRHOUS_COMPART_INFO_TO_TB_PLM_STOCK_MOVEMENT_HST` FOREIGN KEY (`WRHOUS_CMPRT_SEQNO`) REFERENCES `TB_PLM_WRHOUS_COMPART_INFO` (`WRHOUS_CMPRT_SEQNO`),
  CONSTRAINT `FK_TB_PLM_WRHOUS_COMPART_INFO_TO_TB_PLM_STOCK_MOVEMENT_HST2` FOREIGN KEY (`TO_WRHOUS_CMPRT_SEQNO`) REFERENCES `TB_PLM_WRHOUS_COMPART_INFO` (`WRHOUS_CMPRT_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='재고-상품-이동정보';

