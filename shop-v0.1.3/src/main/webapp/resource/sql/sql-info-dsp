DROP TABLE IF EXISTS `TB_PLM_DLVY_RECV_ADDR_MST`;
CREATE TABLE `TB_PLM_DLVY_RECV_ADDR_MST` (
  `DLVY_RECV_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '배송지리스트SEQNO',
  `DLVY_AREA_CODE` int(11) DEFAULT NULL COMMENT '배송권역코드(SEQNO)',
  `AREA_CODE` char(10) DEFAULT '0000000011' COMMENT '지역 코드',
  `ZONE_CODE` char(10) DEFAULT NULL COMMENT '존 코드',
  `ADDR_CTGRY_CODE` varchar(10) DEFAULT NULL COMMENT 'TB_CODE(RCVADR) -서울,공항,경기...',
  `RECV_ADDR_NM_KO` varchar(127) DEFAULT NULL COMMENT '배송지명_국문',
  `RECV_ADDR_NM_ZH` varchar(127) DEFAULT NULL COMMENT '배송지명_중문간체',
  `RECV_ADDR_NM_EN` varchar(127) DEFAULT NULL COMMENT '배송지명_영문',
  `RECV_ZIPCODE` varchar(7) DEFAULT NULL COMMENT '배송지 우편번호_국문',
  `RECV_ADDR_KO` varchar(511) DEFAULT NULL COMMENT '배송지 주소_국문',
  `RECV_ADDR_ZH` varchar(511) DEFAULT NULL COMMENT '배송지 주소_중문',
  `RECV_ADDR_EN` varchar(511) DEFAULT NULL COMMENT '배송지 주소_영문',
  `LAT` varchar(20) DEFAULT NULL COMMENT '위치 위도',
  `LNG` varchar(20) DEFAULT NULL COMMENT '위치 경도',
  `MEMO` varchar(1023) DEFAULT NULL COMMENT '메모',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`DLVY_RECV_SEQNO`),
  KEY `FK_TB_DLVY_AREA_MST_TO_TB_PLM_DLVY_RECV_ADDR_MST` (`DLVY_AREA_CODE`),
  KEY `FK_TB_ZONE_MST_TO_TB_PLM_DLVY_RECV_ADDR_MST` (`ZONE_CODE`),
  KEY `FK_TB_AREA_MST_TO_TB_PLM_DLVY_RECV_ADDR_MST` (`AREA_CODE`),
  CONSTRAINT `FK_TB_AREA_MST_TO_TB_PLM_DLVY_RECV_ADDR_MST` FOREIGN KEY (`AREA_CODE`) REFERENCES `TB_AREA_MST` (`AREA_CODE`),
  CONSTRAINT `FK_TB_DLVY_AREA_MST_TO_TB_PLM_DLVY_RECV_ADDR_MST` FOREIGN KEY (`DLVY_AREA_CODE`) REFERENCES `TB_DLVY_AREA_MST` (`DLVY_AREA_CODE`),
  CONSTRAINT `FK_TB_ZONE_MST_TO_TB_PLM_DLVY_RECV_ADDR_MST` FOREIGN KEY (`ZONE_CODE`) REFERENCES `TB_ZONE_MST` (`ZONE_CODE`)
) ENGINE=InnoDB AUTO_INCREMENT=835 DEFAULT CHARSET=utf8 COMMENT='배송관리_배송지리스트정보';

------------------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_PLM_JOB_ORDER_LIST`;
CREATE TABLE `TB_PLM_JOB_ORDER_LIST` (
  `JOB_ORDER_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '작업할당SEQNO',
  `ORG_JOB_ORDER_SEQNO` int(11) DEFAULT NULL COMMENT '(원)작업할당SEQNO',
  `JOB_ORDER_REQ_DATE` varchar(8) DEFAULT NULL COMMENT '20160301 (X)20160302',
  `JOB_ORDER_TY` char(2) DEFAULT 'N' COMMENT '10(작업대기) 31(배송실패)',
  `LOGI_DLVY_TY` char(2) DEFAULT '00' COMMENT '60(배송) 70(반품/회수) 80(교환) 91(긴급회수)',
  `JOB_ORDER_COM_YN` char(1) DEFAULT 'N' COMMENT '지시작업 완료여부',
  `LOGI_DLVY_PROC_CD` char(2) DEFAULT '00' COMMENT '00(대기) 10(처리완료) 20(배송일자변경) 30(입고지연)',
  `LOG_DLVY_REQ_DATE` varchar(8) DEFAULT NULL COMMENT '20160303  (I) 20160305',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORDER_SEQNO',
  `DLVY_AREA_CODE` int(11) DEFAULT NULL COMMENT '배송권역코드(SEQNO)',
  `DELIVERER_SEQNO` int(11) DEFAULT NULL COMMENT '배송직원SEQ',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  `LOGI_DLVY_TR_SEQNO` int(11) DEFAULT NULL COMMENT '배송(운송장)SEQ',
  PRIMARY KEY (`JOB_ORDER_SEQNO`),
  KEY `FK_TB_COURIER_DELIVERER_INFO_TO_TB_PLM_JOB_ORDER_LIST` (`DELIVERER_SEQNO`),
  KEY `FK_TB_DLVY_AREA_MST_TO_TB_PLM_JOB_ORDER_LIST` (`DLVY_AREA_CODE`),
  KEY `FK_TB_ORDER_INFO_TO_TB_PLM_JOB_ORDER_LIST` (`ORDER_SEQNO`),
  KEY `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_JOB_ORDER_LIST` (`LOGI_DLVY_TR_SEQNO`),
  CONSTRAINT `FK_TB_COURIER_DELIVERER_INFO_TO_TB_PLM_JOB_ORDER_LIST` FOREIGN KEY (`DELIVERER_SEQNO`) REFERENCES `TB_COURIER_DELIVERER_INFO` (`DELIVERER_SEQNO`),
  CONSTRAINT `FK_TB_DLVY_AREA_MST_TO_TB_PLM_JOB_ORDER_LIST` FOREIGN KEY (`DLVY_AREA_CODE`) REFERENCES `TB_DLVY_AREA_MST` (`DLVY_AREA_CODE`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_PLM_JOB_ORDER_LIST` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_JOB_ORDER_LIST` FOREIGN KEY (`LOGI_DLVY_TR_SEQNO`) REFERENCES `TB_PLM_LOGI_DLVY_TR_INFO` (`LOGI_DLVY_TR_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=229 DEFAULT CHARSET=utf8 COMMENT='PLM_작업지시리스트';

