DROP TABLE IF EXISTS `TB_PLM_LOGI_DLVY_TR_RECIPT`;
CREATE TABLE `TB_PLM_LOGI_DLVY_TR_RECIPT` (
  `LOGI_DLVY_TR_SEQNO` int(11) NOT NULL COMMENT '배송(운송장)SEQ',
  `PLM_PROD_SEQNO` int(11) NOT NULL COMMENT 'PLM_상품정보SEQ',
  `ORDER_SEQNO` int(11) NOT NULL COMMENT 'ORDER_SEQNO',
  `RECEIPT_STATUS_CD` char(2) DEFAULT '00' COMMENT '회상태코드 00(회수대기/지시) 10(회수성공) 20(회수실패)',
  `STOCK_RECEIPT_QTY` tinyint(4) DEFAULT NULL COMMENT '입고수량',
  `STOCK_RECEIPT_CFM_QTY` tinyint(4) DEFAULT NULL COMMENT '입고확인수량',
  `STOCK_RECEIPT_YN` char(1) DEFAULT 'N' COMMENT '입고확인여부',
  `STOCK_RECEIPT_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '입고확인시간',
  PRIMARY KEY (`LOGI_DLVY_TR_SEQNO`,`PLM_PROD_SEQNO`,`ORDER_SEQNO`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_LOGI_DLVY_TR_RECIPT` (`PLM_PROD_SEQNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_PLM_LOGI_DLVY_TR_RECIPT` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_PLM_LOGI_DLVY_TR_RECIPT` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_LOGI_DLVY_TR_RECIPT` FOREIGN KEY (`LOGI_DLVY_TR_SEQNO`) REFERENCES `TB_PLM_LOGI_DLVY_TR_INFO` (`LOGI_DLVY_TR_SEQNO`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_LOGI_DLVY_TR_RECIPT` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='물류-배송 입고내역';

