DROP TABLE IF EXISTS `TB_PROD_COUPON_DET`;
CREATE TABLE `TB_PROD_COUPON_DET` (
  `PROD_DET_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'PROD_DET_SEQNO',
  `PROD_COUPON_UUID` int(11) NOT NULL COMMENT 'PROD_COUPON_UUID',
  `COUPON_USE_TY` char(1) DEFAULT NULL COMMENT '유효기일 기준(1:유효기간 2:사용기한)',
  `AVAILABLE_PERIOD_STR_DT` datetime DEFAULT NULL COMMENT '쿠폰 유효기간(시작)',
  `AVAILABLE_PERIOD_END_DT` datetime DEFAULT NULL COMMENT '쿠폰 유효기간(종료)',
  `USABLE_TERMS_DAYS` int(11) DEFAULT NULL COMMENT '사용기한 - 일수',
  `COUPON_EXCHANGE` varchar(63) DEFAULT NULL COMMENT '[쿠폰교환처]',
  `COUPON_TERM` varchar(63) DEFAULT NULL COMMENT '[쿠폰사용기간]',
  `COUPON_RESEND_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '[쿠폰재발송가능여부]',
  `MEMO` varchar(1023) DEFAULT NULL COMMENT '[MEMO',
  `COUPON_TY` char(1) DEFAULT '0' COMMENT '0:사용 확인 불가, 1:실시간 인증 방식, 2:일단위 배치 방식)',
  `REWARD_PT` int(11) DEFAULT NULL COMMENT '[리워드포인트]',
  `CANCEL_AVAIL_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '[취소가능여부]',
  `PIN_GROUP_CODE` char(10) DEFAULT NULL COMMENT 'PIN그룹코드',
  `REWARD_PT_DAYS` int(11) DEFAULT NULL COMMENT '리워드포인트 사용 가능일',
  `EXPDATE_EXTN_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '유효기일 연장 여부',
  `EXPDATE_EXTN_MAX` int(11) NOT NULL DEFAULT '91' COMMENT '최대유효기간(일수)',
  `AUTH_TY` char(3) DEFAULT '111' COMMENT '사용/취소인증방법 (NNN, 바코드, 스탬프, 번호인증 순, 1 or 0)',
  `COUPON_FORM_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '쿠폰포맷 이미지 파일경로',
  `COUPON_FORM_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '쿠폰포맷 이미지 파일명',
  `COUPON_FORM_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '쿠폰포맷 이미지 (원)파일명',
  `COUPON_FORM_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '쿠폰포맷 이미지 파일 사이즈_H',
  `COUPON_FORM_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '쿠폰포맷 이미지 파일 사이즈_V',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PROD_DET_SEQNO`),
  UNIQUE KEY `UIX_TB_PROD_COUPON_DET` (`PROD_COUPON_UUID`),
  KEY `FK_TB_PIN_GROUP_TO_TB_PROD_COUPON_DET` (`PIN_GROUP_CODE`),
  CONSTRAINT `FK_TB_PIN_GROUP_TO_TB_PROD_COUPON_DET` FOREIGN KEY (`PIN_GROUP_CODE`) REFERENCES `TB_PIN_GROUP` (`PIN_GROUP_CODE`),
  CONSTRAINT `FK_TB_PROD_COUPON_INFO_TO_TB_PROD_COUPON_DET` FOREIGN KEY (`PROD_COUPON_UUID`) REFERENCES `TB_PROD_COUPON_INFO` (`PROD_COUPON_UUID`)
) ENGINE=InnoDB AUTO_INCREMENT=1026 DEFAULT CHARSET=utf8 COMMENT='상품_쿠폰상품_상세정보';

