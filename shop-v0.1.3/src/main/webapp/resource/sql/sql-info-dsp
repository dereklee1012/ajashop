DROP TABLE IF EXISTS `TB_ORDER_PREPARE`;
CREATE TABLE `TB_ORDER_PREPARE` (
  `TEMP_ORDER_NO` int(11) NOT NULL AUTO_INCREMENT COMMENT '임시주문번호',
  `UNO` int(11) DEFAULT NULL COMMENT 'UNO(사용자NO)',
  `PASSPORT_NO` varchar(30) DEFAULT NULL COMMENT '여권번호',
  `PASSPORT_NAME_EN` varchar(30) DEFAULT NULL COMMENT '여권-영문 이름',
  `PG_TY` char(1) NOT NULL DEFAULT 'A' COMMENT 'A(알리페이) W(위쳇페이) U(유니온페이)',
  `TAX_REFUND_SETL_TYP_CD` char(1) DEFAULT 'A' COMMENT 'C: 카드 / M: 현금 / E: 전자화폐 / A :현금+카드+전자화폐',
  `B2C_ORDER_TY` char(2) DEFAULT '00' COMMENT '00(B2C상품. 기존상품) 90(B2B2C상품. ex) 알리트립에 판매하는 동화면세점5만원상품권 등)',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'LC' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `ORDER_QTY` int(11) DEFAULT '0' COMMENT '주문 물품 수량',
  `ORDER_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '주문금액(판매가*수량+옵션)_원화',
  `ORDER_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '주문금액(판매가*수량+옵션)_위안화',
  `ORDER_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '주문금액(판매가*수량+옵션)_USD',
  `DC_COUPON_USE_YN` char(1) DEFAULT 'N' COMMENT '할인권(DC 쿠폰)사용여부',
  `PAY_DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '총 할인금액_KRW',
  `PAY_DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '총 할인금액_CNY',
  `PAY_DC_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '총 할인금액_USD',
  `CASH_USE_YN` char(1) DEFAULT 'N' COMMENT '결제시 캐시 사용 결제 여부',
  `PAY_CASH_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT 'ROUND(PAY_CASH_AMT_CNY * EHGT_RATE_CNY_TO_KRW)',
  `PAY_CASH_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '결제시 사용한 캐시 금액_CNY',
  `PAY_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액 - 캐시사용금액',
  `PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액 - 캐시사용금액',
  `PAY_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액 - 캐시사용금액',
  `TAX_REFUND_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '세금환급가능금액_KRW',
  `TAX_REFUND_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '세금환급가능금액_CNY',
  `TAX_REFUND_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '세금환급가능금액_USD',
  `PAY_COURIER_DLVY_FEE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '배송비 결제여부',
  `COURIER_DLVY_FEE_SEQNO` int(11) DEFAULT NULL COMMENT '배송비용 요금SEQ',
  `PAY_COURIER_DLVY_FEE_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '배송비_KRW',
  `PAY_COURIER_DLVY_FEE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '배송비_CNY',
  `PAY_COURIER_DLVY_FEE_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '배송비_USD',
  `CANCLE_PAY_TY` char(1) DEFAULT 'N' COMMENT 'N(정상) T(전체취소) P(부분취소)',
  `CANCLE_PAY_CASH_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액 05.11 Add',
  `CANCLE_PAY_CASH_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액 05.11 Add',
  `CANCLE_PAY_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `EHGT_SEQNO` int(11) NOT NULL COMMENT '환율SEQNO',
  `EHGT_SEQNO_USD` int(11) NOT NULL COMMENT 'USD환율SEQNO',
  `EHGT_RATE_KRW_TO_CNY` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '원화,위안화환율',
  `EHGT_RATE_CNY_TO_KRW` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,원화환율',
  `EHGT_RATE_CNY_TO_USD` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,USD환율',
  `USING_CART_ORDER_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '카트상품주문여부',
  `PAY_CURRENCY` char(3) NOT NULL DEFAULT 'CNY' COMMENT 'CNY(위안화) USD(달러) KRW(한화)',
  `ACC_CURRENCY` char(3) NOT NULL DEFAULT 'USD' COMMENT 'CNY(위안화) USD(달러) KRW(한화)',
  `TAX_YN` char(1) DEFAULT 'Y' COMMENT '과세 여부',
  `TAX_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '과세 RATE',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '적용가능상품이 있는지 여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '주문이 면세인지',
  `TOUR_AGENCY_CODE` char(10) DEFAULT NULL COMMENT '여행사 업체코드',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`TEMP_ORDER_NO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PREPARE` (`EHGT_SEQNO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PREPARE2` (`EHGT_SEQNO_USD`),
  KEY `FK_TB_COURIER_DLVY_FEE_INFO_TO_TB_ORDER_PREPARE` (`COURIER_DLVY_FEE_SEQNO`),
  KEY `FK_TB_TOUR_AGENCY_INFO_TO_TB_ORDER_PREPARE` (`TOUR_AGENCY_CODE`),
  KEY `FK_TB_USER_INFO_TO_TB_ORDER_PREPARE` (`UNO`),
  CONSTRAINT `FK_TB_COURIER_DLVY_FEE_INFO_TO_TB_ORDER_PREPARE` FOREIGN KEY (`COURIER_DLVY_FEE_SEQNO`) REFERENCES `TB_COURIER_DLVY_FEE_INFO` (`COURIER_DLVY_FEE_SEQNO`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PREPARE` FOREIGN KEY (`EHGT_SEQNO`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PREPARE2` FOREIGN KEY (`EHGT_SEQNO_USD`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_TOUR_AGENCY_INFO_TO_TB_ORDER_PREPARE` FOREIGN KEY (`TOUR_AGENCY_CODE`) REFERENCES `TB_TOUR_AGENCY_INFO` (`TOUR_AGENCY_CODE`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_ORDER_PREPARE` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=3722 DEFAULT CHARSET=utf8 COMMENT='주문_임시주문정보';

