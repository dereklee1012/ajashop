DROP TABLE IF EXISTS `TB_ORDER_PAYMENT`;
CREATE TABLE `TB_ORDER_PAYMENT` (
  `PAYMENT_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'PAYMENT_SEQNO',
  `ORG_PAYMENT_SEQNO` int(11) DEFAULT NULL COMMENT '(원)결제SEQNO',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORDER_SEQNO',
  `ORDER_UUID` varchar(20) DEFAULT NULL COMMENT '(D,C)+YYMMDDHHMISS(UTC+0) + (PAYMENT_SEQNO / 100,001 나머지)',
  `UNO` int(11) DEFAULT NULL COMMENT 'UNO(사용자NO)',
  `PG_TY` char(1) NOT NULL DEFAULT 'A' COMMENT 'A(알리페이) W(위쳇페이) U(유니온페이)',
  `PAY_TYPE` char(1) NOT NULL DEFAULT 'C' COMMENT 'C(카드)',
  `PAY_YN` char(1) DEFAULT 'Y' COMMENT '결제여부',
  `PAY_STATUS_CD` char(2) DEFAULT '00' COMMENT '00(결제요청) 01(요청완료) 20(결제완료) 50(결제취소요청) 51(요청완료) 60(취소완료) 90(결제실패) 92(결제취소실패)',
  `PAY_CNL_CD` char(2) DEFAULT '00' COMMENT '00(결제요청) 01(요청완료) 20(결제완료) 50(결제취소요청) 51(요청완료) 60(취소완료) 90(결제실패) 92(결제취소실패)',
  `EHGT_SEQNO` int(11) NOT NULL COMMENT '환율SEQNO',
  `PAY_CURRENCY` char(3) NOT NULL DEFAULT 'CNY' COMMENT 'CNY(위안화) USD(달러) KRW(한화)',
  `ACC_CURRENCY` char(3) NOT NULL DEFAULT 'USD' COMMENT 'CNY(위안화) USD(달러) KRW(한화)',
  `PROD_SELLING_PRICE` decimal(13,0) DEFAULT '0' COMMENT '가격(판매가*수량+옵션)',
  `PAY_CASH_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT 'TB_ORDER_INFO.PAY_CASH_AMT_KRW 05.11 Add',
  `PAY_CASH_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT 'TB_ORDER_INFO.PAY_CASH_AMT_CNY  05.11 Add',
  `PAY_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '결제금액_KRW',
  `PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '결제금액_CNY',
  `PAY_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '결제금액_USD',
  `CNY_EXCHANGE_RATE` decimal(13,6) DEFAULT NULL COMMENT '위안화 환율',
  `TAX_YN` char(1) DEFAULT 'Y' COMMENT '과세 여부',
  `TAX_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '과세 RATE',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Tax refund 적용가능여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '면세상품여부',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PAYMENT_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_ORDER_PAYMENT` (`UNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_ORDER_PAYMENT` (`ORDER_SEQNO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PAYMENT` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PAYMENT` FOREIGN KEY (`EHGT_SEQNO`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_ORDER_PAYMENT` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_ORDER_PAYMENT` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=3096 DEFAULT CHARSET=utf8 COMMENT='주문_결제정보';

