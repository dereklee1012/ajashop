DROP TABLE IF EXISTS `TB_PLM_LOGI_DELIVERY_INFO`;
CREATE TABLE `TB_PLM_LOGI_DELIVERY_INFO` (
  `LOGI_DLVY_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '물류-배송SEQ',
  `LOGI_DLVY_TR_SEQNO` int(11) DEFAULT NULL COMMENT '배송(운송장)SEQ',
  `LOGI_DLVY_DATE` varchar(8) DEFAULT NULL COMMENT '(배송)배송일자',
  `LOGI_DLVY_QTY` int(11) DEFAULT NULL COMMENT '(배송)수량',
  `DLVY_AREA_CODE` int(11) DEFAULT NULL COMMENT '배송권역코드(SEQNO)',
  `DELIVERER_SEQNO` int(11) DEFAULT NULL COMMENT '배송직원SEQ',
  `COURIER_CAR_SEQNO` int(11) DEFAULT NULL COMMENT '배송차량SEQ',
  `LOGI_DLVY_LADE_CAR_DT` datetime DEFAULT NULL COMMENT '상차시간',
  `LOGI_DLVY_UNLADE_CAR_DT` datetime DEFAULT NULL COMMENT '하차시간',
  `DLVY_PROC_CD` char(2) DEFAULT '00' COMMENT '배송상태:00(물품준비) 10(배송준비) 11(배송중/쿠폰발송) 20(배송완료) 30(배송실패) 31(지연실패) ',
  `DLVY_DATE_TZ_CD` varchar(20) DEFAULT NULL COMMENT 'TB_CODE(DLY-TZ)',
  `DLVY_COMPLETE_YN` char(1) DEFAULT 'Y' COMMENT '배송완료여부: Y,N',
  `DLVY_COMPLETE_DT` datetime DEFAULT NULL COMMENT '배송완료시간',
  `DLVY_UNSUCC_CD` char(2) DEFAULT '00' COMMENT '배송실패:00(없음) 11(부재중) 12(수령거부) 21(교환) 31(반품) 41(결제취소) 90(지연)',
  `DLVY_DELAY_CD` char(2) DEFAULT '00' COMMENT '00(없음) 10, 20, 30, ',
  `DLVY_MEMO` varchar(255) DEFAULT NULL COMMENT '배송메모',
  `RECIPIENT_NM` varchar(30) DEFAULT NULL COMMENT '수령인',
  `RECIPIENT_NM_EN` varchar(30) DEFAULT NULL COMMENT '수령인(영문)',
  `RECIPIENT_TEL` varchar(30) DEFAULT NULL COMMENT '수령인 전화번호',
  `DLVY_RECV_SEQNO` int(11) DEFAULT NULL COMMENT '배송지정보(우편번호, 배송주소1~3)',
  `RECIPIENT_DESC` varchar(255) DEFAULT NULL COMMENT '상세객실정보',
  `HANJIN_INVOICE_NO` varchar(20) DEFAULT NULL COMMENT '한진운송장번호',
  `HANJIN_INVOICE_CREATED_DT` datetime DEFAULT NULL COMMENT '한진운송장생성일시',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`LOGI_DLVY_SEQNO`),
  KEY `FK_TB_COURIER_DELIVERER_INFO_TO_TB_PLM_LOGI_DELIVERY_INFO` (`DELIVERER_SEQNO`),
  KEY `FK_TB_DLVY_AREA_MST_TO_TB_PLM_LOGI_DELIVERY_INFO` (`DLVY_AREA_CODE`),
  KEY `FK_TB_PLM_COURIER_CAR_INFO_TO_TB_PLM_LOGI_DELIVERY_INFO` (`COURIER_CAR_SEQNO`),
  KEY `FK_TB_PLM_DLVY_RECV_ADDR_MST_TO_TB_PLM_LOGI_DELIVERY_INFO` (`DLVY_RECV_SEQNO`),
  KEY `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_LOGI_DELIVERY_INFO` (`LOGI_DLVY_TR_SEQNO`),
  CONSTRAINT `FK_TB_COURIER_DELIVERER_INFO_TO_TB_PLM_LOGI_DELIVERY_INFO` FOREIGN KEY (`DELIVERER_SEQNO`) REFERENCES `TB_COURIER_DELIVERER_INFO` (`DELIVERER_SEQNO`),
  CONSTRAINT `FK_TB_DLVY_AREA_MST_TO_TB_PLM_LOGI_DELIVERY_INFO` FOREIGN KEY (`DLVY_AREA_CODE`) REFERENCES `TB_DLVY_AREA_MST` (`DLVY_AREA_CODE`),
  CONSTRAINT `FK_TB_PLM_COURIER_CAR_INFO_TO_TB_PLM_LOGI_DELIVERY_INFO` FOREIGN KEY (`COURIER_CAR_SEQNO`) REFERENCES `TB_PLM_COURIER_CAR_INFO` (`COURIER_CAR_SEQNO`),
  CONSTRAINT `FK_TB_PLM_DLVY_RECV_ADDR_MST_TO_TB_PLM_LOGI_DELIVERY_INFO` FOREIGN KEY (`DLVY_RECV_SEQNO`) REFERENCES `TB_PLM_DLVY_RECV_ADDR_MST` (`DLVY_RECV_SEQNO`),
  CONSTRAINT `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_LOGI_DELIVERY_INFO` FOREIGN KEY (`LOGI_DLVY_TR_SEQNO`) REFERENCES `TB_PLM_LOGI_DLVY_TR_INFO` (`LOGI_DLVY_TR_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=27 DEFAULT CHARSET=utf8 COMMENT='물류-배송상세정보';

