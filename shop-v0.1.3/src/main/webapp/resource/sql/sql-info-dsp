DROP TABLE IF EXISTS `TB_CS_INTRO_NOTI`;
CREATE TABLE `TB_CS_INTRO_NOTI` (
  `NTCN_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '알림일련번호',
  `NTCN_NM_EN` varchar(1023) DEFAULT NULL COMMENT '알림명칭(영어)',
  `NTCN_NM_KO` varchar(1023) DEFAULT NULL COMMENT '알림명칭(한국어)',
  `NTCN_NM_ZH` varchar(1023) DEFAULT NULL COMMENT '알림명칭(간체)',
  `BANNER_IMAGE_PATH` varchar(255) DEFAULT NULL COMMENT '배너이미지PATH',
  `BANNER_FILE_NM` varchar(128) DEFAULT NULL COMMENT '배너파일명',
  `BANNER_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '배너 (원)파일 명칭',
  `BANNER_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '배너 사이즈_H 사이즈_H',
  `BANNER_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '배너 사이즈_V 사이즈_V',
  `THUMNAIL_PATH` varchar(255) DEFAULT NULL COMMENT '썸네일 경로',
  `THUMNAIL_FILENAME` varchar(128) DEFAULT NULL COMMENT '썸네일 파일명',
  `THUMNAIL_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT 'NEW_FILENAME',
  `THUMNAIL_SIZE_H` decimal(4,0) DEFAULT NULL COMMENT '썸네일 이미지 사이즈_H',
  `THUMNAIL_SIZE_V` decimal(4,0) DEFAULT NULL COMMENT '썸네일 이미지 사이즈_V',
  `BANNER_LINK_URL_TY` char(1) NOT NULL DEFAULT 'O' COMMENT '이동설정 O:외부링크 I:내부링크',
  `BANNER_LINK_URL` varchar(512) DEFAULT NULL COMMENT '배너 내부링크 URL',
  `EXPSR_DUR_STR` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '노출기간(시작)',
  `EXPSR_DUR_END` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '노출기간(종료)',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출 여부',
  `EXPSR_ORDR` tinyint(4) DEFAULT NULL COMMENT '노출 순서',
  `EXPSR_MBER_SCOPE_CODE` char(1) DEFAULT NULL COMMENT '노출회원범위코드 A(전체) M(회원) N(비회원)',
  `MVMN_SETUP` char(1) DEFAULT NULL COMMENT '이동설정 E:이벤트 T:테마 S:매장 P:상품 M:Mall C:쿠폰상품 B:브랜드 O:외부링크 I:내부링크 N:Notice + G(쿠폰몰 카테) H(배송몰 카테)',
  `LINK_URL` varchar(512) DEFAULT NULL COMMENT '링크URL',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드',
  `BRAND_CODE` char(10) DEFAULT NULL COMMENT '브랜드코드',
  `PRODUCT_UUID` int(11) DEFAULT NULL COMMENT '상품UUID',
  `EVENT_SEQNO` int(11) DEFAULT NULL COMMENT '이벤트 일련번호',
  `THEME_SEQNO` int(11) DEFAULT NULL COMMENT '테마일련번호',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리코드 (이동설정이 G나 H고 NULL 이면 메인으로 이동)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`NTCN_SEQNO`),
  KEY `FK_TB_BRAND_MST_TO_TB_CS_INTRO_NOTI` (`BRAND_CODE`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_CS_INTRO_NOTI` (`CTGRY_CODE`),
  KEY `FK_TB_EVENT_INFO_TO_TB_CS_INTRO_NOTI` (`EVENT_SEQNO`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_CS_INTRO_NOTI` (`PRODUCT_UUID`),
  KEY `FK_TB_STORE_INFO_TO_TB_CS_INTRO_NOTI` (`STORE_CODE`),
  KEY `FK_TB_THEME_INFO_TO_TB_CS_INTRO_NOTI` (`THEME_SEQNO`),
  CONSTRAINT `FK_TB_BRAND_MST_TO_TB_CS_INTRO_NOTI` FOREIGN KEY (`BRAND_CODE`) REFERENCES `TB_BRAND_MST` (`BRAND_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_CS_INTRO_NOTI` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `FK_TB_EVENT_INFO_TO_TB_CS_INTRO_NOTI` FOREIGN KEY (`EVENT_SEQNO`) REFERENCES `TB_EVENT_INFO` (`EVENT_SEQNO`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_CS_INTRO_NOTI` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_STORE_INFO_TO_TB_CS_INTRO_NOTI` FOREIGN KEY (`STORE_CODE`) REFERENCES `TB_STORE_INFO` (`STORE_CODE`),
  CONSTRAINT `FK_TB_THEME_INFO_TO_TB_CS_INTRO_NOTI` FOREIGN KEY (`THEME_SEQNO`) REFERENCES `TB_THEME_INFO` (`THEME_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=26 DEFAULT CHARSET=utf8 COMMENT='서비스알림 정보';

