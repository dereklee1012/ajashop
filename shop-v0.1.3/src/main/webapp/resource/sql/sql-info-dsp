DROP TABLE IF EXISTS `TB_ORDER_PROD_DLVY_DET_HST`;
CREATE TABLE `TB_ORDER_PROD_DLVY_DET_HST` (
  `HST_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'HST_SEQNO',
  `HST_CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'HST_CREATED_AT',
  `HST_TY` char(1) NOT NULL DEFAULT 'I' COMMENT '히스토리생성사유(I, U, D)',
  `HST_CREATED_USER` varchar(30) NOT NULL DEFAULT '0' COMMENT 'HST_CREATED_USER',
  `ORDER_DLVY_DET_SEQNO` int(11) NOT NULL COMMENT '배송상품상세SEQ',
  `UNO` int(11) DEFAULT NULL COMMENT 'UNO(사용자NO)',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORDER_SEQNO',
  `PROD_SEQNO` int(11) DEFAULT NULL COMMENT 'PROD_SEQNO',
  `ORDER_DLVY_SEQNO` int(11) DEFAULT NULL COMMENT '배송요청 SEQ',
  `ORDER_TRADE_NO` varchar(23) DEFAULT NULL COMMENT 'ORDER_UUID+(SEQ/101 나머지)',
  `DLVY_PROC_CD` char(2) DEFAULT '00' COMMENT '배송상태:00(물품준비) 10(배송준비) 11(배송중/쿠폰발송) 20(배송완료) 30(배송실패) 31(지연실패) ',
  `DLVY_RECV_SEQNO` int(11) DEFAULT NULL COMMENT '배송지리스트SEQNO',
  `DLVY_REQ_DAY` varchar(8) DEFAULT NULL COMMENT '배송요청일자: YYYYMMDD  / DLVY_PROC_CD - 31(지연실패) 40(물품준비지연) / 재고/물류상태 - 10(입고) 20(출고대기) 30(피킹) 40(패킹) 50(상차) 60(배송) 70(반품) 80(교환) 90(긴급배송) 91(긴급회수)',
  `DLVY_REQ_TIME` char(6) DEFAULT NULL COMMENT '배송요청시간: HHMISS',
  `DLVY_COMPLETE_YN` char(1) DEFAULT 'Y' COMMENT '배송완료여부: Y,N',
  `DLVY_COMPLETE_DT` datetime DEFAULT NULL COMMENT '배송시간',
  `DLVY_UNSUCC_CD` char(2) DEFAULT '00' COMMENT '배송실패:00(없음) 11(부재중) 12(수령거부) 21(교환) 31(반품) 41(결제취소)',
  `DLVY_DESC` varchar(511) DEFAULT NULL COMMENT '배송상세정보',
  `RECIPIENT_NM` varchar(30) DEFAULT NULL COMMENT '수령인',
  `RECIPIENT_NM_EN` varchar(30) DEFAULT NULL COMMENT '수령인(영문)',
  `RECIPIENT_TEL` varchar(30) DEFAULT NULL COMMENT '수령인 전화번호',
  `RECIPIENT_DESC` varchar(255) DEFAULT NULL COMMENT '상세객실정보',
  `RECIPIENT_TY` char(1) DEFAULT NULL COMMENT 'S(직접수령,사인) P(업로드사진확인) D(대리인확인)',
  `RECIPIENT_PIN` varchar(20) DEFAULT NULL COMMENT '수령인 PIN',
  `PREPARE_PROD_SEQNO` int(11) DEFAULT NULL COMMENT '임시주문-상품SEQ',
  `SELLING_PRICE` decimal(13,0) DEFAULT '0' COMMENT '상품 판매가',
  `SELLING_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '상품 판매가(위안화)',
  `RETAIL_PRICE` decimal(13,0) DEFAULT '0' COMMENT '시중 판매가',
  `RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '시중 판매가/소비자가(위안화)',
  `SUPPLY_PRICE` decimal(13,0) DEFAULT '0' COMMENT '공급 가격',
  `OPTION_ADDED_PRICE` decimal(10,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가시 금액(원화)',
  `OPTION_ADDED_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '옵션 추가시 금액(위안화)',
  `OPTION_ADDED_SUPPLY_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가 공급 가격: 공급가격 차액입력',
  `OPTION_ADDED_RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가 시중 판매가/소비자가: 소비자가 차액입력',
  `OPTION_ADDED_RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '옵션 추가 시중 판매가/소비자가(위안화): 소비자가 차액입력',
  `PAY_DC_COUPON_USE_YN` char(1) DEFAULT 'N' COMMENT '할인권(DC 쿠폰)사용여부',
  `PAY_DC_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '총 할인금액_USD',
  `PAY_DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '총 할인금액_KRW',
  `PAY_DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '총 할인금액_CNY',
  `PAY_CASH_USE_YN` char(1) DEFAULT 'N' COMMENT '결제시 캐시 사용 결제 여부',
  `PAY_CASH_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '결제시 사용한 캐시 금액_CNY',
  `PAY_CASH_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT 'ROUND(PAY_CASH_AMT_CNY * EHGT_RATE_CNY_TO_KRW)',
  `PAY_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액 - 캐시사용금액',
  `PAY_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액 - 캐시사용금액',
  `PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액 - 캐시사용금액',
  `TAX_REFUND_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '세금환급가능금액_KRW',
  `TAX_REFUND_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '세금환급가능금액_CNY',
  `TAX_REFUND_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '세금환급가능금액_USD',
  `CANCLE_PAY_TY` char(1) DEFAULT 'N' COMMENT 'N(정상) T(전체취소) P(부분취소)',
  `CANCLE_PAY_CASH_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액 05.11 Add',
  `CANCLE_PAY_CASH_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액 05.11 Add',
  `CANCLE_PAY_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `TAX_YN` char(1) DEFAULT 'Y' COMMENT '과세 여부',
  `TAX_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '과세 RATE',
  `JOB_ORDER_SEQNO` int(11) DEFAULT NULL COMMENT '작업할당SEQNO',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`HST_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=6378 DEFAULT CHARSET=utf8 COMMENT='주문_배송상품 배송상세 정보_HST';

