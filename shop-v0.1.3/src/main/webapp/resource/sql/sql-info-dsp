DROP TABLE IF EXISTS `TB_PROD_COUPON_STOCK`;
CREATE TABLE `TB_PROD_COUPON_STOCK` (
  `PROD_STOCK_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'TB_PLM_PRODUCT_INFO.PROD_STOCK_SEQNO',
  `PROD_UUID` int(11) NOT NULL COMMENT '= PROD_COUPON_UUID',
  `PROD_OPT_UUID` int(11) DEFAULT NULL COMMENT '= COUPON_OPTION_UUID',
  `INITIAL_REG_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '최초(등록)수량',
  `ADITIONAL_REG_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '추가(등록)수량',
  `SELLING_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '판매수량',
  `STOCK_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '재고수량 = (최초등록수량+추가등록수량) - 판매수량',
  `PLM_PROD_SEQNO` int(11) DEFAULT NULL COMMENT 'PLM_상품정보SEQ',
  `PLM_STOCK_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '실보유량=입고-출고. STOCK과 동기화되어야함',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PROD_STOCK_SEQNO`),
  UNIQUE KEY `UIX_TB_PROD_COUPON_STOCK_PROD_OPT` (`PROD_UUID`,`PROD_OPT_UUID`),
  KEY `FK_TB_PROD_COUPON_OPTION_INFO_TO_TB_PROD_COUPON_STOCK` (`PROD_OPT_UUID`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_PROD_COUPON_STOCK` (`PLM_PROD_SEQNO`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_PROD_COUPON_STOCK` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`),
  CONSTRAINT `FK_TB_PROD_COUPON_INFO_TO_TB_PROD_COUPON_STOCK` FOREIGN KEY (`PROD_UUID`) REFERENCES `TB_PROD_COUPON_INFO` (`PROD_COUPON_UUID`),
  CONSTRAINT `FK_TB_PROD_COUPON_OPTION_INFO_TO_TB_PROD_COUPON_STOCK` FOREIGN KEY (`PROD_OPT_UUID`) REFERENCES `TB_PROD_COUPON_OPTION_INFO` (`COUPON_OPTION_UUID`)
) ENGINE=InnoDB AUTO_INCREMENT=1299 DEFAULT CHARSET=utf8 COMMENT='상품_쿠폰상품_재고정보';

























