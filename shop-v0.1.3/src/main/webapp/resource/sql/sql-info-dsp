DROP TABLE IF EXISTS `TB_COMPANY_INFO`;
CREATE TABLE `TB_COMPANY_INFO` (
  `COMPANY_CODE` char(10) NOT NULL COMMENT '업체코드',
  `COMPANY_TY` char(1) DEFAULT NULL COMMENT 'C(쿠폰대행) L(지역) F(프랜차이즈) S(매장) P(제휴사) A(영.대) T(여행사)',
  `COMPANY_NAME` varchar(63) DEFAULT NULL COMMENT '업체명',
  `COMPANY_NAME_ZH` varchar(80) DEFAULT NULL COMMENT '업체명(중문)',
  `SUPLER_NAME_KO` varchar(80) DEFAULT NULL COMMENT '공급업체명_국문',
  `SUPLER_NAME_ZH` varchar(80) DEFAULT NULL COMMENT '공급업체명_중문',
  `COMPANY_NAME2` varchar(63) DEFAULT NULL COMMENT '업체명',
  `COMPANY_NAME3` varchar(63) DEFAULT NULL COMMENT '업체명',
  `CEO_NAME` varchar(63) DEFAULT NULL COMMENT '대표자명',
  `BUSINESS_TYPE` char(1) DEFAULT NULL COMMENT '사업자구분 : 1 법인사업자,  2 개인사업자, 3 개인',
  `BUSINESS_NO1` varchar(63) DEFAULT NULL COMMENT '사업자번호/주민번호',
  `BUSINESS_NO3` varchar(20) DEFAULT NULL COMMENT '법인번호',
  `OPEN_DATE` varchar(8) DEFAULT NULL COMMENT '개업일',
  `EMAIL` varchar(40) DEFAULT NULL COMMENT 'EMAIL',
  `TELNO` varchar(30) DEFAULT NULL COMMENT 'TELNO',
  `MOBLPHON_NO` varchar(20) DEFAULT NULL COMMENT '휴대폰번호',
  `FAXNO` varchar(30) DEFAULT NULL COMMENT 'FAXNO',
  `UPTAE` varchar(63) DEFAULT NULL COMMENT '업태',
  `JONGMOK` varchar(63) DEFAULT NULL COMMENT '종목',
  `BANK_NAME` varchar(63) DEFAULT NULL COMMENT '거래은행',
  `BANK_ACCOUNT` varchar(63) DEFAULT NULL COMMENT '계좌번호',
  `BANK_ACCNTNAME` varchar(30) DEFAULT NULL COMMENT '예금주',
  `ONLINE_SITENAME` varchar(127) DEFAULT NULL COMMENT '온라인사업장 사이트명',
  `ONLINE_SITEURL` varchar(255) DEFAULT NULL COMMENT '온라인사업장 사이트URL',
  `AGENCY_CODE` varchar(20) DEFAULT NULL COMMENT '영업대행사 업체코드',
  `BIZCATEGORY1_CD` char(6) DEFAULT NULL COMMENT '업종카테고리 대분류',
  `BIZCATEGORY2_CD` char(6) DEFAULT NULL COMMENT '업종카테고리 중분류',
  `BIZCATEGORY3_CD` char(6) DEFAULT NULL COMMENT '업종카테고리 소분류',
  `BIZ_ZIPCODE` varchar(7) DEFAULT NULL COMMENT '사업지 BIZ_ZIPCODE',
  `BIZ_ADDR1` varchar(255) DEFAULT NULL COMMENT '사업지 BIZ_ADDR1',
  `BIZ_ADDR2` varchar(255) DEFAULT NULL COMMENT '사업지 BIZ_ADDR2',
  `BIZ_ADDR3` varchar(255) DEFAULT NULL COMMENT '사업지 BIZ_ADDR3',
  `BIZ_ADDR4` varchar(255) DEFAULT NULL COMMENT '사업지 BIZ_ADDR4',
  `LOC_ZIPCODE` varchar(7) DEFAULT NULL COMMENT '소재지 LOC_ZIPCODE',
  `LOC_ADDR1` varchar(255) DEFAULT NULL COMMENT '소재지 LOC_ADDR1',
  `LOC_ADDR2` varchar(255) DEFAULT NULL COMMENT '소재지 LOC_ADDR2',
  `LOC_ADDR3` varchar(255) DEFAULT NULL COMMENT '소재지 LOC_ADDR3',
  `LOC_ADDR4` varchar(255) DEFAULT NULL COMMENT '소재지 LOC_ADDR4',
  `MANAGER_NAME` varchar(30) DEFAULT NULL COMMENT '매장담당자명',
  `MANAGER_DEPT` varchar(30) DEFAULT NULL COMMENT '매장담당자명 DEPT',
  `MANAGER_MOBILE` varchar(20) DEFAULT NULL COMMENT '매장담당자 핸드폰번호',
  `MANAGER_EMAIL` varchar(40) DEFAULT NULL COMMENT '매장담당자 EMAIL',
  `MEMO` varchar(1023) DEFAULT NULL COMMENT 'ETC',
  `PARTNER_TYPE` char(1) NOT NULL COMMENT '제휴사구분 1:프랜차이즈, 2:가맹/개인사업, 3:온라인사업, 9:그외',
  `COMMISSION_PAY` decimal(13,2) DEFAULT '0.00' COMMENT '건당비용',
  `COMMISSION_PERCENT` decimal(6,3) DEFAULT '0.000' COMMENT '매출수수료 %',
  `CONTRACT_CODE` varchar(20) DEFAULT NULL COMMENT '계약코드',
  `BIZCATEGORY_LOGO_YN` char(1) DEFAULT 'Y' COMMENT '업종대표이미지 사용여부',
  `MASTER_NAME` varchar(30) DEFAULT NULL COMMENT '[담당자-정]',
  `SLAVE_NAME` varchar(30) DEFAULT NULL COMMENT '[담당자-부]',
  `MASTER_EMAIL` varchar(40) DEFAULT NULL COMMENT '[이메일-정]',
  `SLAVE_EMAIL` varchar(40) DEFAULT NULL COMMENT '[이메일-부]',
  `STATUS` char(1) DEFAULT '0' COMMENT '[상태]1:입점대기, 2:입점가능, 3:정지',
  `USER_TYPE` char(1) DEFAULT 'U' COMMENT '[사용자타입]U:일반사용자, A:관리자',
  `PASWORD` varchar(127) DEFAULT NULL COMMENT '[비밀번호]',
  `FEE_CHK` char(1) DEFAULT 'Y' COMMENT '[지급수수료사용여부]',
  `FEERATE` decimal(13,2) NOT NULL DEFAULT '0.10' COMMENT '[지급수수료율]',
  `MMS_USE` char(1) DEFAULT '0' COMMENT '연동 상품 MMS 발송 유무 (1:발송, 0:미발송)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`COMPANY_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='업체정보';

