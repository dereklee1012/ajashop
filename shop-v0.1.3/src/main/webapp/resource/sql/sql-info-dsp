DROP TABLE IF EXISTS `TB_BANNER_INFO`;
CREATE TABLE `TB_BANNER_INFO` (
  `BANNER_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '배너일련번호',
  `BANNER_TY` char(1) DEFAULT NULL COMMENT 'M(메인), 배너분류코드 M(Mall) C(카테고리) B(브랜드)',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출 여부',
  `DISP_ORDER` tinyint(4) NOT NULL DEFAULT '1' COMMENT '노출 순서',
  `MAIN_BANNER_EXPSR_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '메인배너노출',
  `EXPSR_DUR_STR` datetime DEFAULT NULL COMMENT '노출기간(시작)',
  `EXPSR_DUR_END` datetime DEFAULT NULL COMMENT '노출기간(종료)',
