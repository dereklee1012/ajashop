DROP TABLE IF EXISTS `TB_CART_PROD_COUPON_OPTION_INFO`;
CREATE TABLE `TB_CART_PROD_COUPON_OPTION_INFO` (
  `CART_SEQNO` int(11) NOT NULL COMMENT 'CART_SEQNO',
  `PROD_OPT_UUID` int(11) DEFAULT NULL COMMENT 'COUPON_OPTION_UUID, COUPON_OPTION_UUID',
  `OPT_CODE` varchar(30) DEFAULT NULL COMMENT '상품옵션코드',
  `OPT_LVL1_CD` varchar(30) NOT NULL DEFAULT '1' COMMENT '레벨1 옵션코드',
  `OPT_LVL1_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(영문)',
  `OPT_LVL1_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(국문)',
  `OPT_LVL1_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(중문)',
  `OPT_LVL1_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(영어)',
  `OPT_LVL1_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(국문)',
  `OPT_LVL1_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(중문)',
  `OPT_LVL2_CD` varchar(30) NOT NULL DEFAULT '1' COMMENT '레벨2 옵션코드',
  `OPT_LVL2_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(영문)',
  `OPT_LVL2_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(국문)',
  `OPT_LVL2_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(중문)',
  `OPT_LVL2_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(영어)',
  `OPT_LVL2_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(국문)',
  `OPT_LVL2_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(중문)',
  `OPT_LVL3_CD` varchar(30) NOT NULL DEFAULT '1' COMMENT '레벨3 옵션코드',
  `OPT_LVL3_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(영문)',
  `OPT_LVL3_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(국문)',
  `OPT_LVL3_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(중문)',
  `OPT_LVL3_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(영어)',
  `OPT_LVL3_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(국문)',
  `OPT_LVL3_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(중문)',
  `ORG_PROD_BARCODE` varchar(20) DEFAULT NULL COMMENT '업체 정의 상품(바)코드',
  `PROD_BARCODE` varchar(20) DEFAULT NULL COMMENT '상품 바코드(자체)',
  `OPTION_ORDER` tinyint(4) NOT NULL DEFAULT '1' COMMENT '옵션상품 노출순서',
  `OPTION_ADDED_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가시 금액',
  `OPTION_ADDED_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '옵션 추가시 금액(위안화)',
  `OPTION_ADDED_SUPPLY_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가 공급 가격: 공급가격 차액입력',
  `OPTION_ADDED_RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가 시중 판매가/소비자가: 소비자가 차액입력',
  `OPTION_ADDED_RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '옵션 추가 시중 판매가/소비자가(위안화): 소비자가 차액입력',
  `GENUINE_MARK_PROD_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '정품인증마크 적용여부',
  PRIMARY KEY (`CART_SEQNO`),
  KEY `FK_TB_PROD_COUPON_OPTION_INFO_TO_TB_CART_PROD_COUPON_OPTION_idx` (`PROD_OPT_UUID`),
  CONSTRAINT `FK_TB_CART_PROD_COUPON_INFO_TO_TB_CART_PROD_COUPON_OPTION_INFO` FOREIGN KEY (`CART_SEQNO`) REFERENCES `TB_CART_PROD_COUPON_INFO` (`CART_SEQNO`),
  CONSTRAINT `FK_TB_PROD_COUPON_OPTION_INFO_TO_TB_CART_PROD_COUPON_OPTION_INFO` FOREIGN KEY (`PROD_OPT_UUID`) REFERENCES `TB_PROD_COUPON_OPTION_INFO` (`COUPON_OPTION_UUID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='사용자-쿠폰(Coupon)상품_카트_옵션정보';

