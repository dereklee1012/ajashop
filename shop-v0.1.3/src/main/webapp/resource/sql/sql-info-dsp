DROP TABLE IF EXISTS `TB_CODE`;
CREATE TABLE `TB_CODE` (
  `GRP_ID` varchar(10) NOT NULL COMMENT 'GRP_ID',
  `COMM_CD` varchar(40) NOT NULL COMMENT 'COMM_CD',
  `CODE_NM` varchar(127) DEFAULT NULL COMMENT 'CODE_NM',
  `CODE_NM_ZH` varchar(127) DEFAULT NULL COMMENT 'CODE_NM_ZH',
  `NOTE` varchar(255) DEFAULT NULL COMMENT 'NOTE',
  `CODE_VALUE` varchar(255) DEFAULT NULL COMMENT 'CODE_VALUE',
  `CODE_VALUE2` varchar(255) DEFAULT NULL COMMENT 'CODE_VALUE2',
  `CODE_VALUE3` varchar(255) DEFAULT NULL COMMENT 'CODE_VALUE3',
  `CODE_VALUE4` varchar(255) DEFAULT NULL COMMENT 'CODE_VALUE4',
  `DISP_ORDER` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'DISP_ORDER',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`GRP_ID`,`COMM_CD`),
  CONSTRAINT `FK_TB_CODE_GRP_TO_TB_CODE` FOREIGN KEY (`GRP_ID`) REFERENCES `TB_CODE_GRP` (`GRP_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='TB_CODE';

