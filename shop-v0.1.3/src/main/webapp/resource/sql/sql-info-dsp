  `PROD_INFO_CONT10` varchar(1023) DEFAULT NULL COMMENT '상품정보-상세10',
  `PROD_INFO_ITEM11` varchar(255) DEFAULT NULL COMMENT '상품정보11',
  `PROD_INFO_CONT11` varchar(1023) DEFAULT NULL COMMENT '상품정보-상세11',
  `PROD_INFO_ITEM12` varchar(255) DEFAULT NULL COMMENT '상품정보12',
  `PROD_INFO_CONT12` varchar(1023) DEFAULT NULL COMMENT '상품정보-상세12',
