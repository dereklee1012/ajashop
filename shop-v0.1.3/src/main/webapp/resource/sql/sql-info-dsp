DROP TABLE IF EXISTS `TB_CS_USER_QNA`;
CREATE TABLE `TB_CS_USER_QNA` (
  `QNA_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'QNA SEQNO',
  `UPPER_QNA_SEQNO` int(11) DEFAULT NULL COMMENT '상위QNA_SEQNO',
  `QNA_LEVEL` tinyint(4) DEFAULT '1' COMMENT 'LEVEL',
  `QNA_PROC_CD` char(1) NOT NULL DEFAULT '0' COMMENT '처리경과코드 0(질의) 1(내용확인) 2(답변작성중) 3(답변완료) 9(처리완료)',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `QNA_TY` char(2) DEFAULT '00' COMMENT '문의유형: 00(일반) 10(상품) 20(구매) 21(환불) 22(교환) 30(배송) 40(여행사)',
  `TRAVEL_AGENCY_CD` char(4) DEFAULT NULL COMMENT '여행사코드 TB_CODE(TRAVEL)',
  `PRODUCT_UUID` int(11) DEFAULT NULL COMMENT '상품UUID',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `PROD_DLVY_UUID` int(11) DEFAULT NULL COMMENT 'PROD_DLVY_UUID',
  `PROD_COUPON_UUID` int(11) DEFAULT NULL COMMENT 'PROD_COUPON_UUID',
  `INQUERY_ROUTE_TY` char(1) DEFAULT 'W' COMMENT 'Q(QNA게시판) W(Wechat) O(1:1문의) P(상품문의)',
  `INQUIRY_TITLE` varchar(127) DEFAULT NULL COMMENT 'QNA 타이틀',
  `INQUIRY_CONT` varchar(1024) DEFAULT NULL COMMENT 'QNA 내용',
  `INQUIRY_DT` datetime DEFAULT NULL COMMENT '질의일자',
  `ANSWER_USER_TY` char(1) DEFAULT NULL COMMENT 'U(사용자), M(관리자)',
  `ANSWER_CHARGER` int(11) DEFAULT NULL COMMENT '답변담당자',
  `ANSWER_TITLE` varchar(127) DEFAULT NULL COMMENT '답변 타이틀',
  `ANSWER_CONT` varchar(1024) DEFAULT NULL COMMENT '답변(노출)',
  `ANSWER_MEMO` varchar(1024) DEFAULT NULL COMMENT '(내부용)처리 시 이력 메모',
  `ANSWER_DT` datetime DEFAULT NULL COMMENT '답변일시',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출 여부',
  `EXPSR_DUR_STR` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '노출기간(시작)',
  `EXPSR_DUR_END` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '노출기간(종료)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`QNA_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_CS_USER_QNA` (`UNO`),
  KEY `FK_TB_PROD_DLVY_INFO_TO_TB_CS_USER_QNA` (`PROD_DLVY_UUID`),
  KEY `FK_TB_PROD_COUPON_INFO_TO_TB_CS_USER_QNA` (`PROD_COUPON_UUID`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_CS_USER_QNA` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_CS_USER_QNA` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_PROD_COUPON_INFO_TO_TB_CS_USER_QNA` FOREIGN KEY (`PROD_COUPON_UUID`) REFERENCES `TB_PROD_COUPON_INFO` (`PROD_COUPON_UUID`),
  CONSTRAINT `FK_TB_PROD_DLVY_INFO_TO_TB_CS_USER_QNA` FOREIGN KEY (`PROD_DLVY_UUID`) REFERENCES `TB_PROD_DLVY_INFO` (`PROD_DLVY_UUID`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_CS_USER_QNA` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=143 DEFAULT CHARSET=utf8 COMMENT='고겍센터 > 사용자문의(위챗문의포함)';

-----------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_DISCOUNT_COUPON_INFO`;
CREATE TABLE `TB_DISCOUNT_COUPON_INFO` (
  `DC_COUPON_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '할인권_SEQNO',
  `NAME_EN` varchar(30) DEFAULT NULL COMMENT '할인권 명칭(영어)',
  `NAME_KO` varchar(30) DEFAULT NULL COMMENT '할인권 명칭(한국어)',
  `NAME_ZH` varchar(30) DEFAULT NULL COMMENT '할인권 명칭(간체)',
  `DC_CPN_OFFER_PERIOD_STR_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '할인권 제공기간 시작일 Add 05.07',
  `DC_CPN_OFFER_PERIOD_END_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '할인권 제공기간 종료일 Add 05.07',
  `DC_COUPON_PROMO_TY` char(2) DEFAULT 'SS' COMMENT '할인권 프로모션 타입: PS(프로모션-가입) SS(서비스-제품할인)',
  `COUPON_LAYOUT_CD` varchar(20) DEFAULT NULL COMMENT '쿠폰발행시 레이아웃 코드드(사용하지않음)',
  `DC_COUPON_CODE_CIPHER` tinyint(4) NOT NULL DEFAULT '15' COMMENT '15(기본)',
  `DC_COUPON_CODE_FORMAT` varchar(127) DEFAULT '15 = 1(할인쿠폰유형) + 1(PUBLISH_CD) + 6(유효기간) + 7(PUBLISH_SEQ)' COMMENT '1(A,R) + 1(PUBLISH_CD) + 6(유효기간) + 7(DC_COUPON_SEQ)',
  `PROD_RGN_CD` char(1) DEFAULT NULL COMMENT '상품 적용 범위 A(전체) D(배송) C(쿠폰) L(로컬)',
  `PROD_RGN_DET_CD` char(1) DEFAULT NULL COMMENT '상품 적용 상세 범위 A(전체) C(카테고리) B(브랜드) P(상품코드)',
  `BRAND_CODE` char(10) DEFAULT NULL COMMENT '브랜드코드',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `PRODUCT_UUID` int(11) DEFAULT NULL COMMENT '상품UUID',
  `PROD_UUID` int(11) DEFAULT NULL COMMENT '(중요)PROD_COUPON_UUID or PROD_DLVY_UUID',
  `COUPON_KIND_CD` char(1) DEFAULT NULL COMMENT '할인권 구분(사용치않음) 0(매장할인권) 1(상품) 2(사용자/이벤트)',
  `USABLE_TERMS_LIMIT_TY` char(1) NOT NULL DEFAULT '2' COMMENT '1:유효기간 2:사용기간(구매일 후 며칠동안)',
  `AVAILABLE_PERIOD_STR_DT` datetime DEFAULT NULL COMMENT '쿠폰 유효기간(시작)',
  `AVAILABLE_PERIOD_END_DT` datetime DEFAULT NULL COMMENT '쿠폰 유효기간(종료)',
  `USABLE_TERMS_HOUR` int(11) NOT NULL DEFAULT '0' COMMENT '사용기한 - 시간(H)',
  `DSCNT_TYPE_CD` char(1) NOT NULL DEFAULT 'A' COMMENT '할인권 종류(A:금액, R:비율)',
  `DSCNT_BASIS_CD` char(1) NOT NULL DEFAULT 'N' COMMENT '할인 사용 기준 금액(N:없음, A:금액, C:갯수)',
  `DSCNT_VALUE` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '할인 값(금액, 비율)',
  `DSCNT_VALUE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '할인 값(금액)_위안화',
  `DSCNT_VLM_USE_LMTT_AT_CD` char(1) NOT NULL DEFAULT '0' COMMENT '할인권 사용제한 코드(0:없음, 1:결제금액, 2:할인금액)',
  `DSCNT_AVALIBLE_PAY_AMT_MIN` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '할인권 사용 - 최소결제 금액',
  `DSCNT_AVALIBLE_PAY_AMT_MAX` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '할인권 사용 - 최대결제 금액',
  `DSCNT_LIMIT_AMT` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '할인권 적용 최대 금액(5,000원까지)',
  `DSCNT_AVALIBLE_PAY_AMT_MIN_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '할인권 사용 - 최소결제 금액(위안화)',
  `DSCNT_AVALIBLE_PAY_AMT_MAX_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '할인권 사용 - 최대결제 금액(위안화)',
  `DSCNT_LIMIT_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '할인권 적용 최대 금액(위안화)',
  `PBLICTE_QY_LMTT_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '발행 수량 제한',
  `PBLICTE_QY` int(11) NOT NULL DEFAULT '0' COMMENT '발행 수량',
  `PBLICTE_CUR_QY` int(11) NOT NULL DEFAULT '0' COMMENT '현재까지 발행 수량',
  `DSCNT_USER_ISSUE_QTY_LIMIT_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '사용자별 발급 수량 제한 여부 Add 05.07 - PS(로직추가) SS(로직추가필요)',
  `DSCNT_USER_ISSUE_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '사용자별 발급 제한 수량 Add 05.07 - PS(로직추가) SS(로직추가필요)',
  `IMG_PATH` varchar(255) DEFAULT NULL COMMENT '쿠폰 이미지 경로',
  `IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '쿠폰 이미지 파일 이름',
  `IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '쿠폰 이미지 (원)파일 이름',
  `IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '쿠폰 이미지 사이즈_H',
  `IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '쿠폰 이미지 사이즈_V',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출 여부',
  `DISP_ORDER` tinyint(4) NOT NULL DEFAULT '1' COMMENT '노출순서',
  `RELEVANT_WORDS` varchar(1023) DEFAULT NULL COMMENT '관련주제 키워드, 해시태그',
  `NOTICE` varchar(1023) DEFAULT NULL COMMENT 'NOTICE',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`DC_COUPON_SEQNO`),
  KEY `FK_TB_BRAND_MST_TO_TB_DISCOUNT_COUPON_INFO` (`BRAND_CODE`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_DISCOUNT_COUPON_INFO` (`CTGRY_CODE`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_DISCOUNT_COUPON_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_BRAND_MST_TO_TB_DISCOUNT_COUPON_INFO` FOREIGN KEY (`BRAND_CODE`) REFERENCES `TB_BRAND_MST` (`BRAND_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_DISCOUNT_COUPON_INFO` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_DISCOUNT_COUPON_INFO` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COMMENT='할인권 등록 정보';

