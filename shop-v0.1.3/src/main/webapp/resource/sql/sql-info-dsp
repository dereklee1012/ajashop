  KEY `FK_TB_USER_INFO_TO_TB_LOG_LOCATION` (`UNO`),
  KEY `IX_TB_LOG_LOCATION` (`ZONE_CODE`,`LAT`,`LNG`),
  CONSTRAINT `FK_TB_ZONE_MST_TO_TB_LOG_LOCATION` FOREIGN KEY (`ZONE_CODE`) REFERENCES `TB_ZONE_MST` (`ZONE_CODE`)
) ENGINE=InnoDB AUTO_INCREMENT=3288 DEFAULT CHARSET=utf8 COMMENT='API로깅-지도API 로깅2';

