DROP TABLE IF EXISTS `TB_ORDER_PAY_DC`;
CREATE TABLE `TB_ORDER_PAY_DC` (
  `DISCOUNT_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'DISCOUNT_SEQNO',
  `ORG_DISCOUNT_SEQNO` int(11) DEFAULT NULL COMMENT '(원)할인권적용SEQNO',
  `USER_DC_CPN_SEQNO` int(11) NOT NULL COMMENT 'DC_CPN_SEQNO -> 사용자 다운로드 할인권SEQNO',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORDER_SEQNO',
  `DC_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액_USD',
  `DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '할인금액_KRW',
  `DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액_위안화',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`DISCOUNT_SEQNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_ORDER_PAY_DC` (`ORDER_SEQNO`),
  KEY `FK_TB_USER_DISCOUNT_COUPON_TO_TB_ORDER_PAY_DC` (`USER_DC_CPN_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_ORDER_PAY_DC` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_USER_DISCOUNT_COUPON_TO_TB_ORDER_PAY_DC` FOREIGN KEY (`USER_DC_CPN_SEQNO`) REFERENCES `TB_USER_DISCOUNT_COUPON` (`USER_DC_CPN_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=12 DEFAULT CHARSET=utf8 COMMENT='주문_할인권 사용정보';

