DROP TABLE IF EXISTS `TB_ORDER_PROD_DLVY_INFO`;
CREATE TABLE `TB_ORDER_PROD_DLVY_INFO` (
  `ORDER_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'ORDER_SEQNO',
  `PAYMENT_SEQNO` int(11) DEFAULT NULL COMMENT 'PAYMENT_SEQNO',
  `UNO` int(11) NOT NULL COMMENT 'UNO(???NO)',
  `PRODUCT_UUID` int(11) NOT NULL COMMENT '??UUID',
  `OPTION_ITEM_CHOICE_YN` char(1) DEFAULT 'Y' COMMENT '?? ?? ????',
  `PAY_TYPE` char(2) DEFAULT '00' COMMENT '????',
  `PAY_YN` char(1) DEFAULT 'Y' COMMENT '????',
  `PAY_PROC_CD` char(2) DEFAULT '00' COMMENT '????????',
  `ORDER_DT` datetime DEFAULT NULL COMMENT '????',
  `SHELF_LIFE_DT` datetime DEFAULT NULL COMMENT '??????',
  `ITEM_QTY` int(11) DEFAULT '0' COMMENT '?? ??',
  `PROD_SELLING_PRICE` decimal(13,3) DEFAULT '0.000' COMMENT '??(???*??+??)',
  `RMB_EXCHANGE_RATE` decimal(13,3) DEFAULT '0.000' COMMENT '??? ??',
  `STORE_SALE_PRICE` decimal(13,3) DEFAULT '0.000' COMMENT '?? ???',
  `SELLING_PRICE` decimal(13,3) DEFAULT '0.000' COMMENT '?? ???',
  `SELLING_PRICE_CNY` decimal(13,0) DEFAULT '0',
  `OPTION_ADDED_PRICE` decimal(13,3) DEFAULT '0.000' COMMENT '?? ?? ??(??)',
  `RETAIL_PRICE` decimal(13,3) DEFAULT '0.000' COMMENT '?? ???',
  `RETAIL_PRICE_CNY` decimal(13,3) NOT NULL DEFAULT '0.000' COMMENT '?????/????_???',
  `SUPPLY_PRICE` decimal(13,3) DEFAULT '0.000' COMMENT '?? ??',
  `DISCOUNT_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '???(?????)',
  `TAX_YN` char(1) DEFAULT 'Y' COMMENT '?? ??',
  `TAX_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '?? RATE',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` char(1) NOT NULL DEFAULT 'N' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`ORDER_SEQNO`),
  KEY `FK_TB_PAYMENT_PROD_DLVY_INFO_TO_TB_ORDER_PROD_DLVY_INFO` (`PAYMENT_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_ORDER_PROD_DLVY_INFO` (`UNO`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_ORDER_PROD_DLVY_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_ORDER_PROD_DLVY_INFO` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_ORDER_PROD_DLVY_INFO` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='??(Delivery)?? - ????';

