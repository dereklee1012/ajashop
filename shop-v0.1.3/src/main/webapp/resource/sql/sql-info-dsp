  `REPRSNT_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '대표이미지파일명',
  `REPRSNT_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '대표이미지 (원)파일명',
  `REPRSNT_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '대표이미지사이즈_H',
  `REPRSNT_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '대표이미지사이즈_V',
  `PROD_INFO_ITEM1` varchar(255) DEFAULT NULL COMMENT '상품정보1',
  `PROD_INFO_CONT1` varchar(1023) DEFAULT NULL COMMENT '상품정보-상세1',
