DROP TABLE IF EXISTS `TB_PAYMENT_TR_CNL_LOG`;
CREATE TABLE `TB_PAYMENT_TR_CNL_LOG` (
  `TR_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'TR_SEQNO',
  `ORDER_UUID` varchar(30) DEFAULT NULL COMMENT '(D,C)+YYMMDDHHMISS(UTC+0) + (PAYMENT_SEQNO / 100,001 나머지)',
  `TRADE_NO` varchar(80) DEFAULT NULL COMMENT '알리페이거래번호',
  `TRADE_STATUS` varchar(127) DEFAULT NULL COMMENT '거래상태',
  `NOTIFY_TYPE` varchar(30) DEFAULT NULL COMMENT 'trade_status_sync',
  `NOTIFIY_TIME` varchar(30) DEFAULT NULL COMMENT '공지시작',
  `NOTIFIY_ID` varchar(63) DEFAULT NULL COMMENT '공지아이디',
  `NOTIFY_REG_TIME` varchar(30) DEFAULT NULL COMMENT '알리페이공지등록시간',
  `SIGN` varchar(255) DEFAULT NULL COMMENT '서명',
  `TOTAL_FEE` varchar(20) DEFAULT NULL COMMENT '결제금액',
  `FEE_CURRENCY` varchar(10) DEFAULT NULL COMMENT '결제통화',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`TR_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='주문 -취소결제 - 트랜잭션 응답 로그';

