  `DC_COUPON_APPLIED_YN` char(1) DEFAULT 'Y' COMMENT '할인권적용상품여부',
  `PROD_DISP_ORDER` int(11) NOT NULL DEFAULT '1' COMMENT '상품 노출 순서',
  `MNMNU_EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '메인메뉴 노출',
  `MNMNU_EXPSR_ORDR` int(11) NOT NULL COMMENT '메인메뉴 노출순서',
  `SHELF_LIFE_DT` datetime DEFAULT NULL COMMENT '식품유효기간',
  `PROD_NAME_EN` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(영어)',
  `PROD_NAME_KO` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(한국어)',
  `PROD_NAME_ZH` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(간체)',
  `PROD_OPTION_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '상품옵션 유무',
  `PROD_OPTION_LEVEL` tinyint(4) NOT NULL DEFAULT '0' COMMENT '상품옵션 LEVEL',
  `PROD_OPTION_CNT` int(11) NOT NULL DEFAULT '0' COMMENT '총옵션상품종류',
  `SALE_OVERLAP_YN` char(1) DEFAULT 'Y' COMMENT '[중복구매가능여부]',
