DROP TABLE IF EXISTS `TB_CS_HIST`;
CREATE TABLE `TB_CS_HIST` (
  `CS_HIST_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '히스토리 일련번호',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT '주문번호',
  `ORDER_TRADE_NO` varchar(23) DEFAULT NULL COMMENT '개별 주문 번호 ORDER_UUID+(SEQ/101 나머지)',
  `CS_TY` char(2) DEFAULT NULL COMMENT '상담유형 ( 구매/결제 00, 배송/수령 01, 취소/교환/환불 02, 서비스 기타 03)',
  `CS_STAT` char(2) DEFAULT NULL COMMENT '처리상태 ( 진행중 00/완료 01)',
  `ADM_ID` char(10) DEFAULT NULL COMMENT '작성자어드민 ID',
  `QUESTION` varchar(1000) DEFAULT NULL COMMENT '문의',
  `ANSWER` varchar(1000) DEFAULT NULL COMMENT '처리내용',
  `MOBLPHON_NO` varchar(30) DEFAULT NULL COMMENT '휴대폰번호',
  `USER_ID` varchar(255) DEFAULT NULL COMMENT '고객ID',
  `USER_EMAIL` varchar(255) NOT NULL COMMENT '고객 E-MAIL',
  `CS_HIST_TY` char(2) DEFAULT NULL COMMENT 'CS_HIST 유형',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  `CS_HOTEL_STAT` varchar(2) DEFAULT NULL,
  PRIMARY KEY (`CS_HIST_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8 COMMENT='CS관리';

