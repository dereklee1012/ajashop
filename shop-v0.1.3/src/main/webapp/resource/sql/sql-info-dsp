  `IP문자` varchar(30) DEFAULT NULL COMMENT '(형태) 127.0.0.1',
  `MENU_URL` varchar(512) DEFAULT NULL COMMENT 'URL주소',
  `MENU_URL_PARAM` varchar(127) DEFAULT NULL COMMENT 'URL파라미터',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  PRIMARY KEY (`LOG_IP_SEQNO`),
  KEY `FK_TB_DEVICE_INFO_TO_TB_LOG_IP` (`DEVICE_ID`),
  KEY `FK_TB_USER_INFO_TO_TB_LOG_IP` (`UNO`),
  CONSTRAINT `FK_TB_DEVICE_INFO_TO_TB_LOG_IP` FOREIGN KEY (`DEVICE_ID`) REFERENCES `TB_DEVICE_INFO` (`DEVICE_ID`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_LOG_IP` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='로깅_디바이스 IP 로깅';

