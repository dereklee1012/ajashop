# =============================================================
DROP TABLE IF EXISTS `TB_USER_INFO`;
CREATE TABLE `TB_USER_INFO` (
  `UNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'UNO(사용자NO)',
  `USER_ID` varchar(128) NOT NULL COMMENT '사용자ID',
  `USER_ID_MOD_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '사용자 ID 변경을 가입 후 1회만 허용되도록 적용',
  `WECHAT_OPENID` varchar(128) DEFAULT NULL COMMENT '위챗 Open ID',
  `PASSWORD` varchar(256) DEFAULT NULL COMMENT '비밀번호',
  `MBER_SBSCRB_TY` char(1) DEFAULT 'I' COMMENT '가입유형: W(Wechat) I(Its member) T(여행사회원)',
  `MBER_SBSCRB_CHANNEL` char(1) DEFAULT 'M' COMMENT '회원가입채널: M(모바일웹) I(iOS) A(Android) W(웹) Q(QR - 미사용)',
  `SBSCRB_STATUS_CD` char(1) DEFAULT 'S' COMMENT '가입상태: S(Sign-up) B(Block) U(unsubscribe)',
  `DEVICE_ID` varchar(255) DEFAULT NULL COMMENT 'DEVICE_ID',
  `NAME` varchar(30) DEFAULT NULL COMMENT 'NAME',
  `PASSPORT_NO` varchar(30) DEFAULT NULL COMMENT '여권번호',
  `PASSPORT_NAME_EN` varchar(30) DEFAULT NULL COMMENT '여권-영문 이름',
  `PASSPORT_NAME_KO` varchar(30) DEFAULT NULL COMMENT '여권-국문 이름',
  `PASSPORT_NAME_ZH` varchar(30) DEFAULT NULL COMMENT '여권-중국어 성명',
  `BIRTHDAY` char(8) DEFAULT NULL COMMENT 'BIRTHDAY',
  `GENDER` char(1) DEFAULT 'U' COMMENT 'GENDER',
  `EMAIL_CRTFC_CNFIRM_YN` char(1) DEFAULT 'Y' COMMENT '전자우편인증확인가부',
  `EMAIL` varchar(127) DEFAULT NULL COMMENT 'EMAIL',
  `MOBLPHON_CRTFC_CNFIRM_YN` char(1) DEFAULT 'Y' COMMENT '핸드폰인증확인가부',
  `MOBLPHON_INTR_DIAL_CD` varchar(5) DEFAULT '00' COMMENT 'international dialling codes',
  `MOBLPHON_NO` varchar(30) DEFAULT NULL COMMENT '핸드폰 번호',
  `PROFILE_PATH` varchar(255) DEFAULT NULL COMMENT '프로파일 이미지 파일경로',
  `PROFILE_FILENAME` varchar(128) DEFAULT NULL COMMENT '프로파일 이미지 파일명',
  `PROFILE_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '프로파일 이미지 (원)파일명칭',
  `PROFILE_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '프로파일 이미지 사이즈_H',
  `PROFILE_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '프로파일 이미지 사이즈_V',
  `SUBS_DT` datetime DEFAULT NULL COMMENT '가입 일시',
  `UNSUBS_DT` datetime DEFAULT NULL COMMENT '탈퇴 일시',
  `BLOCK_DT` datetime DEFAULT NULL COMMENT 'Block 일시',
  `AGR_LOCATION_POLICY_YN` char(1) DEFAULT 'Y' COMMENT '위치정보 활용 승인 여부',
  `AGR_PRIVACY_POLICY_YN` char(1) DEFAULT 'Y' COMMENT '개인정보 활용 승인 여부',
  `AGR_RECV_PNS_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'PNS  수신 여부',
  `AGR_RECV_EVENT_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '이벤트정보 수신여부',
  `TOUR_AGENCY_CODE` char(10) DEFAULT NULL COMMENT '여행사 업체코드',
  `TOUR_AGENCY_REG_DT` datetime DEFAULT NULL COMMENT '여행사 업체 등록/수정일시',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`UNO`),
  UNIQUE KEY `UIX_TB_USER_INFO_USER_ID` (`USER_ID`),
  UNIQUE KEY `UIX_TB_USER_INFO_EMAIL` (`EMAIL`),
  UNIQUE KEY `UIX_TB_USER_INFO_MOBILE_PHONE` (`MOBLPHON_INTR_DIAL_CD`,`MOBLPHON_NO`),
  KEY `FK_TB_TOUR_AGENCY_INFO_TO_TB_USER_INFO` (`TOUR_AGENCY_CODE`),
  CONSTRAINT `FK_TB_TOUR_AGENCY_INFO_TO_TB_USER_INFO` FOREIGN KEY (`TOUR_AGENCY_CODE`) REFERENCES `TB_TOUR_AGENCY_INFO` (`TOUR_AGENCY_CODE`)
) ENGINE=InnoDB AUTO_INCREMENT=11143 DEFAULT CHARSET=utf8 COMMENT='사용자 정보';

--------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_USER_INFO_HST`;
CREATE TABLE `TB_USER_INFO_HST` (
  `HST_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'HST_SEQNO',
  `HST_TY` char(1) CHARACTER SET latin1 NOT NULL DEFAULT 'I' COMMENT 'I,O,U',
  `HST_CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'HST_CREATED_AT',
  `HST_CREATED_USER` varchar(30) NOT NULL DEFAULT '0' COMMENT 'HST_CREATED_USER',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `USER_ID` varchar(128) NOT NULL COMMENT '사용자ID',
  `USER_ID_MOD_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '사용자 ID 변경을 가입 후 1회만 허용되도록 적용',
  `WECHAT_OPENID` varchar(128) DEFAULT NULL COMMENT '위챗 Open ID',
  `PASSWORD` varchar(256) DEFAULT NULL COMMENT '비밀번호',
  `MBER_SBSCRB_TY` char(1) DEFAULT 'I' COMMENT '가입유형: W(Wechat) I(Its member) T(여행사회원)',
  `MBER_SBSCRB_CHANNEL` char(1) DEFAULT 'M' COMMENT '회원가입채널: M(모바일웹) I(iOS) A(Android) W(웹) Q(QR - 미사용)',
  `SBSCRB_STATUS_CD` char(1) DEFAULT 'S' COMMENT '가입상태: S(Sign-up) B(Block) U(unsubscribe)',
  `DEVICE_ID` varchar(255) DEFAULT NULL COMMENT 'DEVICE_ID',
  `NAME` varchar(30) DEFAULT NULL COMMENT 'NAME',
  `PASSPORT_NO` varchar(30) DEFAULT NULL COMMENT '여권번호',
  `PASSPORT_NAME_EN` varchar(30) DEFAULT NULL COMMENT '여권-영문 이름',
  `PASSPORT_NAME_KO` varchar(30) DEFAULT NULL COMMENT '여권-국문 이름',
  `PASSPORT_NAME_ZH` varchar(30) DEFAULT NULL COMMENT '여권-중국어 성명',
  `BIRTHDAY` char(8) DEFAULT NULL COMMENT 'BIRTHDAY',
  `GENDER` char(1) DEFAULT 'U' COMMENT 'GENDER',
  `EMAIL_CRTFC_CNFIRM_YN` char(1) DEFAULT 'Y' COMMENT '전자우편인증확인가부',
  `EMAIL` varchar(127) DEFAULT NULL COMMENT 'EMAIL',
  `MOBLPHON_CRTFC_CNFIRM_YN` char(1) DEFAULT 'Y' COMMENT '핸드폰인증확인가부',
  `MOBLPHON_INTR_DIAL_CD` varchar(5) DEFAULT '00' COMMENT 'international dialling codes',
  `MOBLPHON_NO` varchar(30) DEFAULT NULL COMMENT '핸드폰 번호',
  `PROFILE_PATH` varchar(255) DEFAULT NULL COMMENT '프로파일 이미지 파일경로',
  `PROFILE_FILENAME` varchar(128) DEFAULT NULL COMMENT '프로파일 이미지 파일명',
  `PROFILE_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '프로파일 이미지 (원)파일명칭',
  `PROFILE_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '프로파일 이미지 사이즈_H',
  `PROFILE_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '프로파일 이미지 사이즈_V',
  `SUBS_DT` datetime DEFAULT NULL COMMENT '가입 일시',
  `UNSUBS_DT` datetime DEFAULT NULL COMMENT '탈퇴 일시',
  `BLOCK_DT` datetime DEFAULT NULL COMMENT 'Block 일시',
  `AGR_LOCATION_POLICY_YN` char(1) DEFAULT 'Y' COMMENT '위치정보 활용 승인 여부',
  `AGR_PRIVACY_POLICY_YN` char(1) DEFAULT 'Y' COMMENT '개인정보 활용 승인 여부',
  `AGR_RECV_PNS_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'PNS  수신 여부',
  `AGR_RECV_EVENT_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '이벤트정보 수신여부',
  `TOUR_AGENCY_CODE` char(10) DEFAULT NULL COMMENT '여행사 업체코드',
  `TOUR_AGENCY_REG_DT` datetime DEFAULT NULL COMMENT '여행사 업체 등록/수정일시',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`HST_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=2425 DEFAULT CHARSET=utf8 COMMENT='사용자 정보_HST';

-----------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_USER_NAVI_BOOKMARK`;
CREATE TABLE `TB_USER_NAVI_BOOKMARK` (
  `USER_NAVI_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'USER_NAVI_SEQNO',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `NAVY_TY` char(1) DEFAULT '1' COMMENT '1(택시) 2(도보) 3(지하철)',
  `STR_POINT_NM_EN` varchar(255) DEFAULT NULL COMMENT '출발지명_EN',
  `DTN_POINT_NM_EN` varchar(255) DEFAULT NULL COMMENT '도착지명_EN',
  `STR_POINT_NM_KO` varchar(255) DEFAULT NULL COMMENT '출발지명_KO',
  `DTN_POINT_NM_KO` varchar(255) DEFAULT NULL COMMENT '도착지명_KO',
  `STR_POINT_NM_ZH` varchar(255) DEFAULT NULL COMMENT '출발지명_중문',
  `DTN_POINT_NM_ZH` varchar(255) DEFAULT NULL COMMENT '도착지명_중문',
  `STR_POINT_LAT` varchar(20) DEFAULT NULL COMMENT '출발지 위도',
  `STR_POINT_LNG` varchar(20) DEFAULT NULL COMMENT '출발지 경도',
  `DTN_POINT_LAT` varchar(20) DEFAULT NULL COMMENT '도착지 위도 ',
  `DTN_POINT_LNG` varchar(20) DEFAULT NULL COMMENT '도착지 경도',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`USER_NAVI_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_USER_NAVI_BOOKMARK` (`UNO`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_USER_NAVI_BOOKMARK` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=34 DEFAULT CHARSET=utf8 COMMENT='딩동 사용자 PNS ID 관리';

-----------------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_USER_SIGN_INOUT_LOG`;
CREATE TABLE `TB_USER_SIGN_INOUT_LOG` (
  `SIGN_INOUT_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '로그인/아웃 SEQ',
  `DEVICE_ID` varchar(255) DEFAULT NULL COMMENT 'DEVICE_ID',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `SIGN_INOUT_TY` char(1) DEFAULT NULL COMMENT '로그인/아웃타입(I,O)',
  `SIGN_INOUT_DT` datetime DEFAULT CURRENT_TIMESTAMP COMMENT '로그인/아웃시간',
  `CONECT_COURS` char(1) DEFAULT NULL COMMENT '접속경로 A(And) I(iOS),M(M.W), W(Web)',
  `PLATFORM` char(1) NOT NULL COMMENT 'PLATFORM I(iOS) A(Android)',
  `OS_VER` varchar(20) DEFAULT NULL COMMENT 'OS 버전',
  `DEVICE_MODEL` varchar(63) DEFAULT NULL COMMENT '디바이스 모델',
  `APP_VER` varchar(20) NOT NULL COMMENT '어플리케이션 버전',
  `PNS_REGIST_TOKEN` varchar(511) DEFAULT NULL COMMENT 'PNS토큰, RegistrationID',
  `TOUR_AGENCY_CODE` char(10) DEFAULT NULL COMMENT '여행사 업체코드',
  `MNC` varchar(10) DEFAULT NULL COMMENT 'MOBILE NETWORK CODE',
  `MCC` varchar(10) DEFAULT NULL COMMENT 'MOBILE COMPANY CODE',
  `LAT` varchar(20) DEFAULT NULL COMMENT '위도',
  `LNG` varchar(20) DEFAULT NULL COMMENT '경도',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `SIGN_IN_DT` datetime DEFAULT NULL COMMENT 'SIGN-IN Date',
  `SIGN_OUT_DT` datetime DEFAULT NULL COMMENT 'SIGN-OUT Date',
  `RECENT_CONECT_DT` datetime DEFAULT NULL COMMENT '최근접속일시',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`SIGN_INOUT_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_USER_SIGN_INOUT_LOG` (`UNO`),
  KEY `FK_TB_DEVICE_INFO_TO_TB_USER_SIGN_INOUT_LOG` (`DEVICE_ID`),
  KEY `FK_TB_TOUR_AGENCY_INFO_TO_TB_USER_SIGN_INOUT_LOG` (`TOUR_AGENCY_CODE`),
  CONSTRAINT `FK_TB_DEVICE_INFO_TO_TB_USER_SIGN_INOUT_LOG` FOREIGN KEY (`DEVICE_ID`) REFERENCES `TB_DEVICE_INFO` (`DEVICE_ID`),
  CONSTRAINT `FK_TB_TOUR_AGENCY_INFO_TO_TB_USER_SIGN_INOUT_LOG` FOREIGN KEY (`TOUR_AGENCY_CODE`) REFERENCES `TB_TOUR_AGENCY_INFO` (`TOUR_AGENCY_CODE`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_USER_SIGN_INOUT_LOG` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=123 DEFAULT CHARSET=utf8 COMMENT='회원_로그인/아웃 정보';

-----------------------------------------------------------

DROP TABLE IF EXISTS `TB_USER_SNS_SET_INFO`;
CREATE TABLE `TB_USER_SNS_SET_INFO` (
  `SEQ` int(11) NOT NULL COMMENT '일련번호',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `SNS_DIV` char(1) DEFAULT NULL COMMENT 'T:TWITTER,F:FACEBOOK',
  `SNS_ID` varchar(63) DEFAULT NULL COMMENT 'SNS_ID',
  `SNS_KEY1` varchar(255) DEFAULT NULL COMMENT 'SNS_KEY1',
  `SNS_KEY2` varchar(512) DEFAULT NULL COMMENT 'SNS연동URL',
  `SNS_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Y:활성,N:제한',
  `RGS_DT` datetime DEFAULT NULL COMMENT '등록일',
  `EXPR_DATE` datetime DEFAULT NULL COMMENT '완료일(3개월후)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`SEQ`),
  KEY `FK_TB_USER_INFO_TO_TB_USER_SNS_SET_INFO` (`UNO`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_USER_SNS_SET_INFO` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='SNS 설정';

--------------------------------------------------------------

DROP TABLE IF EXISTS `TB_USER_STORE_COUPON`;
CREATE TABLE `TB_USER_STORE_COUPON` (
  `STR_CPN_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'STR_CPN_SEQNO',
  `UNO` int(11) DEFAULT NULL COMMENT 'UNO(사용자NO)',
  `COUPON_SEQNO` int(11) DEFAULT NULL COMMENT 'COUPON_SEQNO',
  `PUBLISH_SEQNO` int(11) DEFAULT NULL COMMENT 'PUBLISH SEQNO',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드',
  `ISSUE_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '발급일자(다운로드)',
  `COUPON_NO` varchar(20) NOT NULL DEFAULT '0000-0000-00000' COMMENT '15 = 4(MMDD) - 4(COUPON_SEQ) - 5(ISSUE_SEQNO)',
  `USE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '사용여부',
  `CRTFC_NO` varchar(20) DEFAULT NULL COMMENT '쿠폰사용인증번호',
  `USE_DT` datetime DEFAULT NULL COMMENT '사용일자',
  `DEL_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '삭제여부',
  `DEL_DT` datetime DEFAULT NULL COMMENT '삭제일자',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`STR_CPN_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_USER_STORE_COUPON` (`UNO`),
  KEY `FK_TB_STORE_COUPON_INFO_TO_TB_USER_STORE_COUPON` (`COUPON_SEQNO`),
  KEY `FK_TB_STORE_COUPON_PUBLISH_TO_TB_USER_STORE_COUPON` (`PUBLISH_SEQNO`),
  CONSTRAINT `FK_TB_STORE_COUPON_INFO_TO_TB_USER_STORE_COUPON` FOREIGN KEY (`COUPON_SEQNO`) REFERENCES `TB_STORE_COUPON_INFO` (`COUPON_SEQNO`),
  CONSTRAINT `FK_TB_STORE_COUPON_PUBLISH_TO_TB_USER_STORE_COUPON` FOREIGN KEY (`PUBLISH_SEQNO`) REFERENCES `TB_STORE_COUPON_PUBLISH` (`PUBLISH_SEQNO`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_USER_STORE_COUPON` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='사용자_매장(무료)쿠폰 정보';

---------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_USER_STORE_KEEP`;
CREATE TABLE `TB_USER_STORE_KEEP` (
  `STORE_CODE` char(10) NOT NULL COMMENT '매장코드',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`STORE_CODE`,`UNO`),
  KEY `FK_TB_USER_INFO_TO_TB_USER_STORE_KEEP` (`UNO`),
  CONSTRAINT `FK_TB_STORE_INFO_TO_TB_USER_STORE_KEEP` FOREIGN KEY (`STORE_CODE`) REFERENCES `TB_STORE_INFO` (`STORE_CODE`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_USER_STORE_KEEP` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='매장_찜정보';

-----------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_ACOM_ADMIN_INFO`;
CREATE TABLE `TB_ACOM_ADMIN_INFO` (
  `ADMIN_NO` int(11) NOT NULL DEFAULT '10000002' COMMENT '관리자 NO',
  `ADMIN_ID` varchar(128) NOT NULL COMMENT '어드민 ID',
  `PASSWORD` varchar(256) DEFAULT NULL COMMENT '암호',
  `ROLE_CD` char(10) NOT NULL COMMENT 'SUPR00(슈퍼관리자),... ROLE_GUBUN + GRADE_CD + ROLE_GRP',
  `NAME` varchar(30) DEFAULT NULL COMMENT 'NAME',
  `EMAIL` varchar(127) DEFAULT NULL COMMENT 'EMAIL',
  `CELLPHONE_LOCALE_CD` char(2) DEFAULT '00' COMMENT '핸드폰 LOCALE_CD',
  `CELLPHONE_NO` varchar(20) DEFAULT NULL COMMENT '핸드폰번호',
  `GRADE_CD` char(2) DEFAULT '00' COMMENT '00(일반), 99(슈퍼유저)',
  `SUBS_STATUS_CD` char(2) DEFAULT '00' COMMENT '가입상태 코드',
  `SUBS_DT` datetime DEFAULT NULL COMMENT '가입 일시',
  `UNSUBS_DT` datetime DEFAULT NULL COMMENT '탈퇴 일시',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`ADMIN_NO`),
  KEY `FK_TB_ROLE_TO_TB_ACOM_ADMIN_INFO` (`ROLE_CD`),
  CONSTRAINT `FK_TB_ROLE_TO_TB_ACOM_ADMIN_INFO` FOREIGN KEY (`ROLE_CD`) REFERENCES `TB_ROLE` (`ROLE_CD`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='관리자 정보';

---------------------------------------------------------

DROP TABLE IF EXISTS `TB_ADMIN_INFO`;
CREATE TABLE `TB_ADMIN_INFO` (
  `ADMIN_NO` int(11) NOT NULL AUTO_INCREMENT COMMENT '관리자 NO',
  `ADMIN_ID` varchar(128) NOT NULL COMMENT '어드민 ID',
  `PASSWORD` varchar(256) DEFAULT NULL COMMENT '암호',
  `ROLE_CD` char(6) DEFAULT NULL COMMENT '역할CD(권한코드)',
  `CONFM_STTUS` varchar(4) DEFAULT '10' COMMENT '승인상태(00: 가입 / 01: 승인요청 / 10: 승인완료 / 20: 승인반려)',
  `NAME` varchar(30) DEFAULT NULL COMMENT 'NAME',
  `EMAIL` varchar(127) DEFAULT NULL COMMENT 'EMAIL',
  `CELLPHONE_LOCALE_CD` char(4) DEFAULT NULL COMMENT '핸드폰 LOCALE_CD',
  `CELLPHONE_NO` varchar(20) DEFAULT NULL COMMENT '핸드폰번호',
  `GRADE_CD` char(2) DEFAULT '00' COMMENT '00(일반), 99(슈퍼유저)',
  `SUBS_STATUS_CD` char(2) DEFAULT '00' COMMENT '00(가입/재직) 10(퇴직/탈퇴) 20(휴직) 90(정직)',
  `SUBS_DT` datetime DEFAULT NULL COMMENT '가입 일시',
  `UNSUBS_DT` datetime DEFAULT NULL COMMENT '탈퇴 일시',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`ADMIN_NO`),
  KEY `FK_TB_ROLE_TO_TB_ADMIN_INFO` (`ROLE_CD`),
  CONSTRAINT `FK_TB_ROLE_TO_TB_ADMIN_INFO` FOREIGN KEY (`ROLE_CD`) REFERENCES `TB_ROLE` (`ROLE_CD`)
) ENGINE=InnoDB AUTO_INCREMENT=10000005 DEFAULT CHARSET=utf8 COMMENT='관리자 정보';

--------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_APP_VERSION`;
CREATE TABLE `TB_APP_VERSION` (
  `PLATFORM` char(1) NOT NULL COMMENT 'PLATFORM I(iOS) A(Android)',
  `MARKET_TY` char(1) NOT NULL COMMENT '마켓유형 A(Apple) P(Playstore)',
  `APP_TY` char(2) NOT NULL DEFAULT '00' COMMENT '어플리케이션 유형 00(상용) 02(시연용) 03(스테이징) 04(개발용) 10(배송상용) 14(배송기사개발)',
  `OS_VER` varchar(20) DEFAULT NULL COMMENT 'MIN_OS_VER',
  `APP_VER` varchar(20) NOT NULL COMMENT 'APP_VER(00.00.0000)',
  `UPDATE_TY` char(1) DEFAULT 'S' COMMENT '업데이트유형 F(Force) S(Select),N',
  `UPDATE_URL` varchar(512) DEFAULT NULL COMMENT 'UPDATE_URL',
  `MESSAGE` varchar(1023) DEFAULT NULL COMMENT 'MESSAGE',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PLATFORM`,`MARKET_TY`,`APP_TY`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='어플리케이션 버전정보';

------------------------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_AREA_MST`;
CREATE TABLE `TB_AREA_MST` (
  `AREA_CODE` char(10) NOT NULL DEFAULT '00' COMMENT '지역 코드',
  `AREA_NAME_EN` varchar(30) DEFAULT NULL COMMENT '지역명칭(영어)',
  `AREA_NAME_KO` varchar(30) DEFAULT NULL COMMENT '지역명칭(한국어)',
  `AREA_NAME_ZH` varchar(30) NOT NULL COMMENT '지역명칭(간체)',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출 여부',
  `DISP_ORDER` tinyint(4) NOT NULL DEFAULT '1' COMMENT '노출순서',
  `MNMNU_EXPSR_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '메인메뉴 노출여부',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`AREA_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='지역구분정보_MST';

-----------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_AUTH_TOKENS`;
CREATE TABLE `TB_AUTH_TOKENS` (
  `AUTHE_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'AUTH_SEQNO',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `DEVICE_ID` varchar(255) NOT NULL COMMENT 'DEVICE_ID',
  `TOKEN` varchar(255) DEFAULT NULL COMMENT 'TOKEN',
  `EXPIRED_AT` timestamp NULL DEFAULT NULL COMMENT 'EXPIRED_AT',
  `DEVICE_TYPE` char(2) DEFAULT '00' COMMENT 'DEVICE_TYPE',
  `DEVICE_MODEL` varchar(63) DEFAULT NULL COMMENT 'DEVICE_MODEL',
  `OS_VER` varchar(20) DEFAULT NULL COMMENT 'OS_VER',
  `APP_VER` varchar(20) DEFAULT NULL COMMENT 'APP_VER',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`AUTHE_SEQNO`),
  UNIQUE KEY `UIX_TB_AUTH_TOKENS` (`UNO`,`DEVICE_ID`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_AUTH_TOKENS` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=3110 DEFAULT CHARSET=utf8 COMMENT='TB_AUTH_TOKENS';

------------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_AUTH_TOKENS_DELIVERER`;
CREATE TABLE `TB_AUTH_TOKENS_DELIVERER` (
  `DELIVERER_SEQNO` int(11) NOT NULL COMMENT '배송직원SEQ',
  `DEVICE_ID` varchar(255) NOT NULL COMMENT 'DEVICE_ID',
  `TOKEN` varchar(255) DEFAULT NULL COMMENT 'TOKEN',
  `EXPIRED_AT` timestamp NULL DEFAULT NULL COMMENT 'EXPIRED_AT',
  `DEVICE_TYPE` char(2) DEFAULT '00' COMMENT 'DEVICE_TYPE',
  `DEVICE_MODEL` varchar(63) DEFAULT NULL COMMENT 'DEVICE_MODEL',
  `OS_VER` varchar(20) DEFAULT NULL COMMENT 'OS_VER',
  `APP_VER` varchar(20) DEFAULT NULL COMMENT 'APP_VER',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`DELIVERER_SEQNO`),
  UNIQUE KEY `UIX_TB_AUTH_TOKENS_DELIVERER` (`DEVICE_ID`),
  CONSTRAINT `FK_TB_COURIER_DELIVERER_INFO_TO_TB_AUTH_TOKENS_DELIVERER` FOREIGN KEY (`DELIVERER_SEQNO`) REFERENCES `TB_COURIER_DELIVERER_INFO` (`DELIVERER_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='TB_AUTH_TOKENS';

-------------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_B2B_ORDER_INFO`;
CREATE TABLE `TB_B2B_ORDER_INFO` (
  `B2B_ORDER_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'B2B 주문SEQ',
  `B2B_UNO` int(11) DEFAULT NULL COMMENT 'B2B UNO(사용자NO)',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'AT' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `ORG_ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORG_ORDER_SEQNO',
  `TZ_TIMESTAMP` tinyint(4) DEFAULT NULL COMMENT '(Timezone)Timestamp',
  `BUYERID` varchar(30) NOT NULL COMMENT '외부마켓구매자아이디',
  `ORDER_STATUS_CD` char(2) NOT NULL DEFAULT 'OR' COMMENT 'OP(주문요청준비) OR(주문요청완료) OC(주문완료) OF(주문실패) CS(주문취소 CS요청) CR(주문취소요청) CC(취소완료) CF(취소실패) PR(부분취소요청) PC(부분취소완료) PD(구매확정)',
  `CANCLE_STATUS_CD` char(2) DEFAULT 'NA' COMMENT '주문취소진행상태: NA(진행내용없음) CR(주문취소요청) CC(취소완료) CF(취소실패)',
  `TRADE_NO` varchar(30) DEFAULT NULL COMMENT '거래번호',
  `TRADE_DT` datetime DEFAULT NULL COMMENT '거래일시',
  `TRADE_PD_DT` datetime DEFAULT NULL COMMENT '거래확정일시',
  `TAX_REFUND_REQ_TY` char(1) NOT NULL DEFAULT 'N' COMMENT 'N(미신청) Y(주문시신청) A(추후신청)',
  `CANCLE_PAY_TY` char(1) DEFAULT 'N' COMMENT 'N(정상) T(전체취소) P(부분취소)',
  `CANCLE_TRADE_NO` varchar(30) DEFAULT NULL COMMENT '취소거래번호',
  `CANCLE_TRADE_REQ_DT` datetime DEFAULT NULL COMMENT '주문취소요청일시',
  `CANCLE_TRADE_FIN_DT` datetime DEFAULT NULL COMMENT '주문취소완료일시',
  `ORDER_QTY` int(11) DEFAULT '0' COMMENT '주문 물품 수량',
  `ORDER_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '주문금액 = (판매단가*수량+옵션) + 배송금액',
  `ORDER_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '주문금액 = (판매단가*수량+옵션) + 배송금액',
  `ORDER_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '주문금액 = (판매단가*수량+옵션) + 배송금액',
  `DC_COUPON_USE_YN` char(1) DEFAULT 'N' COMMENT '할인권(DC 쿠폰)사용여부',
  `PAY_DC_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '총 할인금액_USD',
  `PAY_DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '총 할인금액_KRW',
  `PAY_DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '총 할인금액_CNY',
  `CASH_USE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '결제시 캐시 사용 결제 여부',
  `PAY_CASH_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '결제시 사용한 캐시 금액_CNY',
  `PAY_CASH_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT 'ROUND(PAY_CASH_AMT_CNY * EHGT_RATE_CNY_TO_KRW)',
  `PAY_COURIER_DLVY_FEE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '배송비 결제여부',
  `PAY_COURIER_DLVY_FEE_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '배송비_USD',
  `PAY_COURIER_DLVY_FEE_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '배송비_KRW',
  `PAY_COURIER_DLVY_FEE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '배송비_CNY',
  `PAY_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액 - 캐시사용금액',
  `PAY_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액 - 캐시사용금액',
  `PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '결제금액 + 캐시사용금액 = (주문금액 + 배송비) - 할인금액',
  `TAX_REFUND_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '세금환급가능금액_KRW',
  `TAX_REFUND_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '세금환급가능금액_USD',
  `TAX_REFUND_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '세금환급가능금액_CNY',
  `TAX_YN` char(1) DEFAULT 'Y' COMMENT '과세 여부',
  `TAX_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '과세 RATE',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '적용가능상품이 있는지 여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '주문이 면세인지',
  `CANCLE_PAY_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_CASH_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액 05.11 Add',
  `CANCLE_PAY_CASH_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액 05.11 Add',
  `CANCLE_RESON_CD` char(2) DEFAULT '00' COMMENT '[취소사유]01(변심) 12(품절취소) 13(배송지연취소) 14(상품불량) 05(기타) 16(구매의사 없어짐) 07:다른상품 재주문 08(판매자배송지연) 09(타사이트 상품주문)',
  `CANCLE_RESON_DESC` varchar(1023) DEFAULT NULL COMMENT '주문취소 상세사유',
  `EHGT_SEQNO` int(11) NOT NULL COMMENT '환율SEQNO',
  `EHGT_SEQNO_USD` int(11) DEFAULT NULL COMMENT 'USD환율SEQNO',
  `EHGT_RATE_KRW_TO_CNY` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '원화,위안화환율',
  `EHGT_RATE_CNY_TO_KRW` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,원화환율',
  `EHGT_RATE_CNY_TO_USD` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,USD환율',
  `PAY_CURRENCY` char(3) NOT NULL DEFAULT 'CNY' COMMENT 'CNY(위안화) USD(달러) KRW(한화)',
  `ACC_CURRENCY` char(3) NOT NULL DEFAULT 'USD' COMMENT 'CNY(위안화) USD(달러) KRW(한화)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`B2B_ORDER_SEQNO`),
  KEY `FK_TB_B2B_USER_INFO_TO_TB_B2B_ORDER_INFO` (`B2B_UNO`),
  CONSTRAINT `FK_TB_B2B_USER_INFO_TO_TB_B2B_ORDER_INFO` FOREIGN KEY (`B2B_UNO`) REFERENCES `TB_B2B_USER_INFO` (`B2B_UNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='B2B 주문정보';

---------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_B2B_ORDER_MAPPING`;
CREATE TABLE `TB_B2B_ORDER_MAPPING` (
  `B2B_ORDER_SEQNO` int(11) NOT NULL COMMENT 'B2B 주문SEQ',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'AT' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORDER_SEQNO',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`B2B_ORDER_SEQNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_B2B_ORDER_MAPPING` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_B2B_ORDER_INFO_TO_TB_B2B_ORDER_MAPPING` FOREIGN KEY (`B2B_ORDER_SEQNO`) REFERENCES `TB_B2B_ORDER_INFO` (`B2B_ORDER_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_B2B_ORDER_MAPPING` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='B2B 주문 (내부주문)매핑정보';

----------------------------------------------------------

DROP TABLE IF EXISTS `TB_B2B_USER_INFO`;
CREATE TABLE `TB_B2B_USER_INFO` (
  `B2B_UNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'B2B UNO(사용자NO)',
  `UNO` int(11) DEFAULT NULL COMMENT 'UNO(사용자NO)',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'AT' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `BUYERID` varchar(128) NOT NULL COMMENT '사용자ID',
  `MBER_SBSCRB_TY` char(1) DEFAULT 'W' COMMENT '가입방법: W(Wechat) I(Its member)',
  `SBSCRB_STATUS_CD` char(1) DEFAULT 'S' COMMENT '가입상태: S(Sign-up) B(Block) U(unsubscribe)',
  `NAME` varchar(30) DEFAULT NULL COMMENT 'NAME',
  `PASSPORT_NAME_EN` varchar(30) DEFAULT NULL COMMENT '여권-영문 이름',
  `PASSPORT_NAME_KO` varchar(30) DEFAULT NULL COMMENT '여권-국문 이름',
  `PASSPORT_NAME_ZH` varchar(30) DEFAULT NULL COMMENT '여권-중국어 성명',
  `BIRTHDAY` char(8) DEFAULT NULL COMMENT 'YYYYMMDD',
  `GENDER` char(1) DEFAULT 'U' COMMENT 'M, F, U(Unknown)',
  `EMAIL` varchar(127) DEFAULT NULL COMMENT 'EMAIL',
  `MOBLPHON_INTR_DIAL_CD` varchar(5) DEFAULT '00' COMMENT 'international dialling codes',
  `MOBLPHON_NO` varchar(30) DEFAULT NULL COMMENT '핸드폰 번호',
  `SUBS_DT` datetime DEFAULT NULL COMMENT '가입 일시',
  `UNSUBS_DT` datetime DEFAULT NULL COMMENT '탈퇴 일시',
  `BLOCK_DT` datetime DEFAULT NULL COMMENT 'Block 일시',
  `AGR_RECV_PNS_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'PNS  수신 여부',
  `PNS_REGIST_TOKEN` varchar(511) DEFAULT NULL COMMENT 'PNS토큰, RegistrationID',
  `DEVICE_ID` varchar(255) DEFAULT NULL COMMENT 'DEVICE_ID',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`B2B_UNO`),
  UNIQUE KEY `UIX_TB_B2B_USER_INFO` (`MARKET_CODE`,`BUYERID`,`MOBLPHON_INTR_DIAL_CD`,`MOBLPHON_NO`,`EMAIL`),
  KEY `FK_TB_DEVICE_INFO_TO_TB_B2B_USER_INFO` (`DEVICE_ID`),
  KEY `FK_TB_USER_INFO_TO_TB_B2B_USER_INFO` (`UNO`),
  CONSTRAINT `FK_TB_DEVICE_INFO_TO_TB_B2B_USER_INFO` FOREIGN KEY (`DEVICE_ID`) REFERENCES `TB_DEVICE_INFO` (`DEVICE_ID`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_B2B_USER_INFO` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='B2B_사용자 정보';

----------------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_B2BMK_ORDER_INFO`;
CREATE TABLE `TB_B2BMK_ORDER_INFO` (
  `B2BMK_ORDER_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'B2B마켓 주문서SEQNO',
  `ORG_B2BMK_ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORG_B2B마켓 주문서SEQNO',
  `ADMIN_NO` int(11) DEFAULT NULL COMMENT '처리담당자 - 관리자 NO',
  `B2BMK_PROC_CD` char(2) DEFAULT '00' COMMENT '00(업로드) 10(주문요청) 20(주문완료) 30(주문실패)',
  `B2BMK_PROC_DT` datetime NOT NULL COMMENT '마켓주문정보 주문처리일시',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'AT' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `TZ_TIMESTAMP` tinyint(4) DEFAULT '8' COMMENT '(Timezone)Timestamp',
  `BUYERID` varchar(30) NOT NULL COMMENT '외부마켓구매자아이디',
  `TRADE_NO` varchar(30) DEFAULT NULL COMMENT '거래번호',
  `TRADE_DT` datetime DEFAULT NULL COMMENT '거래일시',
  `ORDER_QTY` int(11) DEFAULT '0' COMMENT '주문 물품 수량',
  `ORDER_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '주문금액(판매가*수량+옵션)_USD',
  `ORDER_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '주문금액(판매가*수량+옵션)_원화',
  `ORDER_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '주문금액(판매가*수량+옵션)_위안화',
  `DC_COUPON_USE_YN` char(1) DEFAULT 'N' COMMENT '할인권(DC 쿠폰)사용여부-할인금액이 0원이면 ''N''',
  `PAY_DC_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '총 할인금액_USD',
  `PAY_DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '총 할인금액_KRW',
  `PAY_DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '총 할인금액_CNY',
  `PAY_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액',
  `PAY_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액',
  `PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '결제금액 = (주문금액 + 배송비) - 할인금액',
  `CANCLE_PAY_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_TY` char(1) DEFAULT 'N' COMMENT 'N(정상) T(전체취소) P(부분취소)',
  `MOBLPHON_INTR_DIAL_CD` varchar(5) DEFAULT '00' COMMENT 'international dialling codes',
  `MOBLPHON_NO` varchar(30) DEFAULT NULL COMMENT '핸드폰 번호',
  `EMAIL` varchar(127) DEFAULT NULL COMMENT 'EMAIL',
  `PASSPORT_NO` varchar(30) DEFAULT NULL COMMENT '여권번호',
  `PASSPORT_COUNTRY_CD` char(3) DEFAULT 'CHN' COMMENT 'ISO 3166-1 기준. CHN(중국), KOR, JPN. TB_CODE(CNTRYCD)',
  `PASSPORT_NAME_EN` varchar(30) DEFAULT NULL COMMENT '여권-영문 이름',
  `PASSPORT_NAME_KO` varchar(30) DEFAULT NULL COMMENT '여권-국문 이름',
  `PASSPORT_NAME_ZH` varchar(30) DEFAULT NULL COMMENT '여권-중국어 성명',
  `NAME` varchar(30) DEFAULT NULL COMMENT 'NAME',
  `UNO` int(11) DEFAULT NULL COMMENT 'UNO(사용자NO)',
  `TEMP_ORDER_NO` int(11) DEFAULT NULL COMMENT '임시주문번호',
  `DLVY_REQ_DAY` varchar(8) DEFAULT NULL COMMENT '배송요청일자: YYYYMMDD',
  `DLVY_REQ_TIME` char(6) DEFAULT NULL COMMENT '배송요청시간: HHMISS',
  `DLVY_DESC` varchar(511) DEFAULT NULL COMMENT '배송상세정보',
  `DLVY_RECV_SEQNO` int(11) DEFAULT NULL COMMENT '배송지리스트SEQNO',
  `RECV_ADDR_NM_KO` varchar(127) DEFAULT NULL COMMENT '배송지명_국문',
  `RECV_ADDR_NM_ZH` varchar(127) DEFAULT NULL COMMENT '배송지명_중문',
  `RECV_ADDR_NM_EN` varchar(127) DEFAULT NULL COMMENT '배송지명_영문',
  `RECV_ZIPCODE` varchar(7) DEFAULT NULL COMMENT '배송지 우편번호_국문',
  `RECV_ADDR_KO` varchar(511) DEFAULT NULL COMMENT '배송지 주소_국문',
  `RECV_ADDR_ZH` varchar(511) DEFAULT NULL COMMENT '배송지 주소_중문',
  `RECV_ADDR_EN` varchar(511) DEFAULT NULL COMMENT '배송지 주소_영문',
  `RECIPIENT_DESC` varchar(255) DEFAULT NULL COMMENT '방번호...',
  `RECIPIENT_NM` varchar(30) DEFAULT NULL COMMENT '수령인',
  `RECIPIENT_NM_EN` varchar(30) DEFAULT NULL COMMENT '수령인(영문)',
  `RECIPIENT_TEL` varchar(30) DEFAULT NULL COMMENT '수령인 전화번호',
  `TAX_REFUND_REQ_TY` char(1) NOT NULL DEFAULT 'N' COMMENT 'N(미신청) Y(주문시신청) A(추후신청)',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORDER_SEQNO',
  `ORDER_UUID` varchar(30) DEFAULT NULL COMMENT '(D,C)+YYMMDDHHMISS(UTC+0) + (SEQNO / 100,001 나머지)',
  `ORDER_REQ_DT` datetime DEFAULT NULL COMMENT '? ',
  `ORDER_FIN_DT` datetime DEFAULT NULL COMMENT '?',
  `ORDER_STATUS_CD` char(2) NOT NULL DEFAULT 'OR' COMMENT '사용여부다시 검토필요 ?',
  `WECHAT_ID` varchar(128) DEFAULT NULL COMMENT '위챗 Open ID',
  `CATALOG_PAY_URL_SEND_YN` char(1) DEFAULT 'N' COMMENT '카탈로그 주문결제 URL - 전달여부',
  `CATALOG_PAY_URL` varchar(512) DEFAULT NULL COMMENT '카탈로그 주문결제 URL',
  `ORDER_COMPLETE_SMS_CNT` tinyint(4) DEFAULT NULL COMMENT '주문결제 완료결과 SMS전송 회수',
  `ORDER_COMPLETE_EMAIL_CNT` tinyint(4) DEFAULT NULL COMMENT '주문결제 완료결과 Email 전송 회수',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`B2BMK_ORDER_SEQNO`),
  UNIQUE KEY `UIX_TB_B2BMK_ORDER_INFO` (`MARKET_CODE`,`TRADE_NO`)
) ENGINE=InnoDB AUTO_INCREMENT=123 DEFAULT CHARSET=utf8 COMMENT='B2B마켓 주문서정보';

