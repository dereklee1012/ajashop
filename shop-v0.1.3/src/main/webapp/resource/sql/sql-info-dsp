DROP TABLE IF EXISTS `TB_B2BMK_ORDER_PROD`;
CREATE TABLE `TB_B2BMK_ORDER_PROD` (
  `B2BMK_ORDER_PROD_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'B2B마켓 주문-상품SEQ',
  `B2BMK_ORDER_SEQNO` int(11) NOT NULL COMMENT 'B2B마켓 주문서SEQNO',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'LC' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `B2BMK_GDTP_CODE` char(1) DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `B2BMK_PROD_CODE` char(10) DEFAULT NULL COMMENT '마켓_상품코드',
  `B2BMK_PROD_NAME_KO` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(한국어)',
  `B2BMK_PROD_NAME_ZH` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(중문)',
  `B2BMK_PROD_NAME_EN` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(영어)',
  `B2BMK_PROD_OPT_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '상품 옵션 여부',
  `B2BMK_OPT_CODE` varchar(30) DEFAULT NULL COMMENT '상품옵션코드',
  `B2BMK_OPT_CODE_NM_ZH` varchar(30) DEFAULT NULL COMMENT '옵션코드 명칭(중문)',
  `B2BMK_OPT_CODE_NM_KO` varchar(30) DEFAULT NULL COMMENT '옵션코드 명칭(국문)',
  `B2BMK_OPT_CODE_NM_EN` varchar(30) DEFAULT NULL COMMENT '옵션코드 명칭(영문)',
  `B2BMK_OPT_NM_ZH` varchar(30) DEFAULT NULL COMMENT '옵션명(중문)',
  `B2BMK_OPT_NM_KO` varchar(30) DEFAULT NULL COMMENT '옵션명(국문)',
  `B2BMK_OPT_NM_EN` varchar(30) DEFAULT NULL COMMENT '옵션명(영문)',
  `ITEM_QTY` int(11) DEFAULT '0' COMMENT '물품 수량',
  `PROD_ORDER_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '가격(판매가*수량+옵션)',
  `PROD_ORDER_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '가격(판매가*수량+옵션)_CNY',
  `PROD_ORDER_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '가격(판매가*수량+옵션)_USD',
  `RETAIL_PRICE_KRW` decimal(13,0) DEFAULT '0' COMMENT '시중 판매가,소비자가_KRW',
  `RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '시중 판매가,소비자가_CNY',
  `RETAIL_PRICE_USD` decimal(13,0) DEFAULT '0' COMMENT '시중 판매가,소비자가_USD',
  `SELLING_PRICE_KRW` decimal(13,0) DEFAULT '0' COMMENT '상품 판매가_KRW',
  `SELLING_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '상품 판매가_CNY',
  `SELLING_PRICE_USD` decimal(13,2) DEFAULT '0.00' COMMENT '상품 판매가_USD',
  `PROD_DC_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '할인율',
  `PROD_DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '할인금액(원화)',
  `PROD_DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액(위안화)',
  `PROD_DC_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액(USD)',
  `PROD_PAY_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '지불금액(원화)',
  `PROD_PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '지불금액(위안화)',
  `PROD_PAY_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '지불금액(USD)',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`B2BMK_ORDER_PROD_SEQNO`),
  KEY `FK_TB_B2BMK_ORDER_INFO_TO_TB_B2BMK_ORDER_PROD` (`B2BMK_ORDER_SEQNO`),
  CONSTRAINT `FK_TB_B2BMK_ORDER_INFO_TO_TB_B2BMK_ORDER_PROD` FOREIGN KEY (`B2BMK_ORDER_SEQNO`) REFERENCES `TB_B2BMK_ORDER_INFO` (`B2BMK_ORDER_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=197 DEFAULT CHARSET=utf8 COMMENT='B2B마켓 주문서 상품정보';

