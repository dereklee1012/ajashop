DROP TABLE IF EXISTS `TB_PROD_COUPON_MEMO`;
CREATE TABLE `TB_PROD_COUPON_MEMO` (
  `MEMO_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'SEQNO',
  `PROD_COUPON_UUID` int(11) DEFAULT NULL COMMENT 'PROD_COUPON_UUID',
  `MEMO` varchar(1023) DEFAULT NULL COMMENT '메모',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`MEMO_SEQNO`),
  KEY `FK_TB_PROD_COUPON_INFO_TO_TB_PROD_COUPON_MEMO` (`PROD_COUPON_UUID`),
  CONSTRAINT `FK_TB_PROD_COUPON_INFO_TO_TB_PROD_COUPON_MEMO` FOREIGN KEY (`PROD_COUPON_UUID`) REFERENCES `TB_PROD_COUPON_INFO` (`PROD_COUPON_UUID`)
) ENGINE=InnoDB AUTO_INCREMENT=89 DEFAULT CHARSET=utf8 COMMENT='쿠폰상품_특이사항(메모)';

