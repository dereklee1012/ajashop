DROP TABLE IF EXISTS `TB_EVENT_INFO`;
CREATE TABLE `TB_EVENT_INFO` (
  `EVENT_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '이벤트 일련번호',
  `EVENT_CL_CODE` char(1) DEFAULT 'M' COMMENT '배너분류코드 M(Mall) C(카테고리) B(브랜드)',
  `EVENT_MAIN_EXPSR_YN` char(4) DEFAULT NULL COMMENT '메인화면노출여부',
  `EVENT_CTGRY_MAIN_EXPSR_YN` char(4) DEFAULT NULL COMMENT '메인화면노출여부2',
  `EVENT_CTGRY_MAIN_EXPSR_YN2` char(4) DEFAULT NULL COMMENT '메인화면노출여부3',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출 여부',
  `EXPSR_ORDR` tinyint(4) DEFAULT NULL COMMENT '노출 순서',
  `MAIN_BANNER_EXPSR_YN` char(1) DEFAULT 'N' COMMENT '메인배너노출',
  `EXPSR_DUR_STR` datetime DEFAULT NULL COMMENT '노출기간(시작)',
  `EXPSR_DUR_END` datetime DEFAULT NULL COMMENT '노출기간(종료)',
  `EVENT_DUR_STR` datetime DEFAULT NULL COMMENT '이벤트기간(시작)',
  `EVENT_DUR_END` datetime DEFAULT NULL COMMENT '이벤트기간(종료)',
  `EVENT_NM_EN` varchar(1023) DEFAULT NULL COMMENT '이벤트명칭(영어)',
  `EVENT_NM_KO` varchar(1023) DEFAULT NULL COMMENT '이벤트명칭(한국어)',
  `EVENT_NM_ZH` varchar(1023) DEFAULT NULL COMMENT '이벤트명칭(간체)',
  `BANNER_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '배너이미지 PATH',
  `BANNER_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '배너이미지 파일명',
  `BANNER_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '배너이미지 (원)파일 이름',
  `BANNER_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '배너이미지 사이즈_H',
  `BANNER_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '배너이미지 사이즈_V',
  `THUMNAIL_PATH` varchar(255) DEFAULT NULL COMMENT '썸네일 경로',
  `THUMNAIL_FILENAME` varchar(128) DEFAULT NULL COMMENT '썸네일 파일명',
  `THUMNAIL_ORG_FILENAME` varchar(128) DEFAULT NULL COMMENT '썸네일 (원)파일 이름',
  `THUMNAIL_SIZE_H` decimal(4,0) DEFAULT NULL COMMENT '썸네일 사이즈_H',
  `THUMNAIL_SIZE_V` decimal(4,0) DEFAULT NULL COMMENT '썸네일 사이즈_V',
  `EVENT_URL` varchar(512) DEFAULT NULL COMMENT ' 이벤트상세 URL',
  `EVENT_DETAIL_SE_CODE` char(1) DEFAULT NULL COMMENT '이벤트상세구분코드 I(이미지)U(URL)',
  `EVENT_DETAIL_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '이벤트상세 이미지 파일경로',
  `EVENT_DETAIL_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '이벤트상세 이미지 파일명칭',
  `EVENT_DETAIL_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '이벤트상세 이미지 (원)파일명칭',
  `EVENT_DETAIL_IMG_SIZE_H` decimal(4,0) DEFAULT NULL COMMENT '이벤트상세 이미지 사이즈_H',
  `EVENT_DETAIL_IMG_SIZE_V` decimal(4,0) DEFAULT NULL COMMENT '이벤트상세 이미지 사이즈_V',
  `EVENT_DETAIL_URL` varchar(512) DEFAULT NULL COMMENT '이벤트상세URL',
  `EVENT_MVMN_DTRMN` char(1) DEFAULT NULL COMMENT '이벤트이동설정 S:매장,P:상품,M:Mall,C:쿠폰상품,O:외부링크',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드.',
  `PROD_DLVY_UUID` int(11) DEFAULT NULL COMMENT 'PROD_CODE',
  `PROD_COUPON_UUID` int(11) DEFAULT NULL,
  `RELEVANT_WORDS` varchar(1023) DEFAULT NULL COMMENT '관련주제 키워드, 해시태그 #',
  `EXTRL_LINK_URL` varchar(512) DEFAULT NULL COMMENT '외부링크URL',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`EVENT_SEQNO`),
  KEY `FK_TB_STORE_INFO_TO_TB_EVENT_INFO` (`STORE_CODE`),
  CONSTRAINT `FK_TB_STORE_INFO_TO_TB_EVENT_INFO` FOREIGN KEY (`STORE_CODE`) REFERENCES `TB_STORE_INFO` (`STORE_CODE`)
) ENGINE=InnoDB AUTO_INCREMENT=46 DEFAULT CHARSET=utf8 COMMENT='이벤트정보';

