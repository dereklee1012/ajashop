  `PIN_NUMBER` varchar(48) DEFAULT NULL COMMENT '[모바일발송코드]',
  `USE_PROC_CD` char(2) NOT NULL DEFAULT '10' COMMENT '10(발급전) 00(발급) 01(발급실패) 20(사용) 21(사용실패) 30(사용취소후 재사용) 31(사용취소 후 폐기) 32(주문취소요청)',
  `_USE_PROC_CD` char(2) NOT NULL DEFAULT '00' COMMENT '00(발급)11(배송중,쿠폰발송) 12(재발송) 13(배송완료/수령) 21(쿠폰사용) 31(주문취소-사용,발행취소) 41(발송실패)',
