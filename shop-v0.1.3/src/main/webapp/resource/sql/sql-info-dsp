DROP TABLE IF EXISTS `TB_CS_FAQ`;
CREATE TABLE `TB_CS_FAQ` (
  `FAQ_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'FAQ SEQNO',
  `FAQ_CTGRY_CD` char(4) NOT NULL DEFAULT '0000' COMMENT 'FAQ 분류코드 TB_CODE(FAQ) PAYM, DLVY, EXCG, NAVI, WIFI, ELSE',
  `INQUIRY_EN` varchar(511) DEFAULT NULL COMMENT '질의_영문',
  `INQUIRY_KO` varchar(511) DEFAULT NULL COMMENT '질의_국문',
  `INQUIRY_ZH` varchar(511) DEFAULT NULL COMMENT '질의_중문',
  `ANSWER_EN` varchar(1024) DEFAULT NULL COMMENT '답변_영문',
  `ANSWER_KO` varchar(1024) DEFAULT NULL COMMENT '답변_국문',
  `ANSWER_ZH` varchar(1024) DEFAULT NULL COMMENT '답변_중문',
  `MEMO` varchar(1024) DEFAULT NULL COMMENT '이력 메모',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출 여부',
  `DISP_ORDER` int(11) NOT NULL DEFAULT '1' COMMENT '노출순서',
  `DETAIL_URL` varchar(512) DEFAULT NULL COMMENT '상세정보 URL',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`FAQ_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=78 DEFAULT CHARSET=utf8 COMMENT='고겍센터 > 사용자문의(위챗문의포함)';

