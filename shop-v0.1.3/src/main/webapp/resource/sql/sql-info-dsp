DROP TABLE IF EXISTS `TB_CATEGORY_MST`;
CREATE TABLE `TB_CATEGORY_MST` (
  `CTGRY_CODE` char(10) NOT NULL COMMENT 'S000000001,D000000001,C000000001',
  `LEVEL` tinyint(4) DEFAULT '1' COMMENT '레벨',
  `UPPER_CTGRY_CODE` char(10) DEFAULT NULL COMMENT '상위 카테고리코드',
  `CTGRY_TY` char(1) DEFAULT 'D' COMMENT 'S(매장) D(배송상품) C(쿠폰상품) B(삭제) E(쿠폰상품-브랜드) F(배송상품-브랜드) I(입고상품)',
  `CTGRY_NAME_EN` varchar(30) DEFAULT NULL COMMENT '카테고리명(영어)',
  `CTGRY_NAME_KO` varchar(30) DEFAULT NULL COMMENT '카테고리명(한국어)',
  `CTGRY_NAME_ZH` varchar(30) DEFAULT NULL COMMENT '카테고리명(간체)',
  `ICON_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '아이콘이미지경로',
  `ICON_IMG_NM` varchar(128) DEFAULT NULL COMMENT '아이콘이미지명',
  `ICON_ORG_IMG_NM` varchar(128) DEFAULT NULL COMMENT '아이콘원본이미지명',
  `ICON_IMG_SIZE_H` decimal(4,0) NOT NULL DEFAULT '100' COMMENT '아이콘이미지 사이즈_H',
  `ICON_IMG_SIZE_V` decimal(4,0) NOT NULL DEFAULT '100' COMMENT '아이콘이미지 사이즈_V',
  `BANNER_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '아이콘이미지경로',
  `BANNER_IMG_NM` varchar(128) DEFAULT NULL COMMENT '아이콘이미지명',
  `BANNER_ORG_IMG_NM` varchar(128) DEFAULT NULL COMMENT '아이콘원본이미지명',
  `BANNER_IMG_SIZE_H` decimal(4,0) NOT NULL DEFAULT '100' COMMENT '아이콘이미지 사이즈_H',
  `BANNER_IMG_SIZE_V` decimal(4,0) NOT NULL DEFAULT '100' COMMENT '아이콘이미지 사이즈_V',
  `EXPSR_YN` char(1) DEFAULT 'Y' COMMENT '노출 여부',
  `DISP_ORDER` tinyint(4) DEFAULT '1' COMMENT 'DISP_ORDER',
  `MNMNU_EXPSR_YN` char(1) DEFAULT 'N' COMMENT '메인메뉴 노출여부',
  `IMAGE_PATH` varchar(255) DEFAULT NULL COMMENT '[이미지경로]',
  `AN_ACTIVE` varchar(127) DEFAULT NULL COMMENT '[안드로이드활성]',
  `AN_UNACTIVE` varchar(127) DEFAULT NULL COMMENT '[안드로이드비활성]',
  `IP_ACTIVE` varchar(127) DEFAULT NULL COMMENT '[아이폰활성]',
  `IP_UNACTIVE` varchar(127) DEFAULT NULL COMMENT '[아이폰비활성]',
  `WB_ACTIVE` varchar(127) DEFAULT NULL COMMENT '[웹활성]',
  `WB_UNACTIVE` varchar(127) DEFAULT NULL COMMENT '[웹비활성]',
  `USE_YN` char(1) DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`CTGRY_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='카테고리 정보_MST';

