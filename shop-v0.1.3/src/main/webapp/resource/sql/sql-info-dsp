DROP TABLE IF EXISTS `TB_ORDER_PAYMENT_HST`;
CREATE TABLE `TB_ORDER_PAYMENT_HST` (
  `HST_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'HST_SEQNO',
  `HST_CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'HST_CREATED_AT',
  `HST_TY` char(1) NOT NULL DEFAULT 'I' COMMENT '히스토리 타입(I, U, D)',
  `HST_CREATED_USER` varchar(30) NOT NULL DEFAULT '0' COMMENT 'HST_CREATED_USER',
  `PAYMENT_SEQNO` int(11) NOT NULL COMMENT 'PAYMENT_SEQNO',
  `ORG_PAYMENT_SEQNO` int(11) DEFAULT NULL COMMENT '(?)??SEQNO',
  `ORDER_SEQNO` int(11) DEFAULT NULL COMMENT 'ORDER_SEQNO',
  `ORDER_UUID` varchar(20) DEFAULT NULL COMMENT '(D,C)+YYMMDDHHMISS(UTC+0) + (PAYMENT_SEQNO / 100,001 ???)',
  `UNO` int(11) DEFAULT NULL COMMENT 'UNO(???NO)',
  `PG_TY` char(1) NOT NULL DEFAULT 'A' COMMENT 'A(????) W(????) U(?????)',
  `PAY_TYPE` char(1) NOT NULL DEFAULT 'C' COMMENT 'C(??)',
  `PAY_YN` char(1) DEFAULT 'Y' COMMENT '????',
  `PAY_STATUS_CD` char(2) DEFAULT '00' COMMENT '00(????) 01(????) 20(????) 50(??????) 51(????) 60(????) 90(????) 92(??????)',
  `PAY_CNL_CD` char(2) DEFAULT '00' COMMENT '00(????) 01(????) 20(????) 50(??????) 51(????) 60(????) 90(????) 92(??????)',
  `EHGT_SEQNO` int(11) NOT NULL COMMENT '??SEQNO',
  `PAY_CURRENCY` char(3) NOT NULL DEFAULT 'CNY' COMMENT 'CNY(???) USD(??) KRW(??)',
  `ACC_CURRENCY` char(3) NOT NULL DEFAULT 'USD' COMMENT 'CNY(???) USD(??) KRW(??)',
  `PROD_SELLING_PRICE` decimal(13,0) DEFAULT '0' COMMENT '??(???*??+??)',
  `PAY_CASH_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT 'TB_ORDER_INFO.PAY_CASH_AMT_KRW 05.11 Add',
  `PAY_CASH_AMT_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT 'TB_ORDER_INFO.PAY_CASH_AMT_CNY  05.11 Add',
  `PAY_AMT_KRW` decimal(13,2) DEFAULT '0.00' COMMENT '????_KRW',
  `PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '????_???',
  `PAY_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '????_USD',
  `CNY_EXCHANGE_RATE` decimal(13,6) DEFAULT '0.000000' COMMENT '??? ??',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Tax refund ??????',
  `TAX_YN` char(1) DEFAULT 'Y' COMMENT '?? ??',
  `TAX_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '?? RATE',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '??????',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`HST_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=5606 DEFAULT CHARSET=utf8 COMMENT='??_????';

