DROP TABLE IF EXISTS `TB_PLM_STOCK_INFO`;
CREATE TABLE `TB_PLM_STOCK_INFO` (
  `STOCK_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '재고관리SEQ',
  `PLM_PROD_SEQNO` int(11) DEFAULT NULL COMMENT 'PLM_상품정보SEQ',
  `PLM_STOCK_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '실보유량=입고-출고',
  `PLM_STOCK_RECEIPT_QTY` int(11) NOT NULL DEFAULT '0' COMMENT 'SUM(STORING)',
  `PLM_STOCK_ISSUE_QTY` int(11) NOT NULL DEFAULT '0' COMMENT 'SUM(UNSTORING)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`STOCK_SEQNO`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_STOCK_INFO` (`PLM_PROD_SEQNO`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_STOCK_INFO` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=140 DEFAULT CHARSET=utf8 COMMENT='재고-재고정보';

-----------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_PLM_STOCK_ISSUE_PICKING`;
CREATE TABLE `TB_PLM_STOCK_ISSUE_PICKING` (
  `STOCK_ISSUE_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '재고상품-출고SEQNO',
  `STOCK_SEQNO` int(11) DEFAULT NULL COMMENT '재고관리SEQ',
  `ISSUE_PICKING_MENO` varchar(255) DEFAULT NULL COMMENT '(출고)출고메모',
  `ISSUE_PICKING_RSN_CD` char(2) DEFAULT '00' COMMENT 'TB_CODE(PICKING) N OR(주문) EX(교환) ET(기타)',
  `ISSUE_PICKING_RSN_DESC` varchar(255) DEFAULT NULL COMMENT '(출고)출고사유-상세',
  `ISSUE_PICKING_DT` datetime DEFAULT NULL COMMENT '(출고)출고일시',
  `ISSUE_PICKING_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '출고수량',
  `LOGI_DLVY_TR_SEQNO` int(11) DEFAULT NULL COMMENT '배송(운송장)SEQ',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`STOCK_ISSUE_SEQNO`),
  KEY `FK_TB_PLM_STOCK_INFO_TO_TB_PLM_STOCK_ISSUE_PICKING` (`STOCK_SEQNO`),
  KEY `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_STOCK_ISSUE_PICKING` (`LOGI_DLVY_TR_SEQNO`),
  CONSTRAINT `FK_TB_PLM_LOGI_DLVY_TR_INFO_TO_TB_PLM_STOCK_ISSUE_PICKING` FOREIGN KEY (`LOGI_DLVY_TR_SEQNO`) REFERENCES `TB_PLM_LOGI_DLVY_TR_INFO` (`LOGI_DLVY_TR_SEQNO`),
  CONSTRAINT `FK_TB_PLM_STOCK_INFO_TO_TB_PLM_STOCK_ISSUE_PICKING` FOREIGN KEY (`STOCK_SEQNO`) REFERENCES `TB_PLM_STOCK_INFO` (`STOCK_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=55 DEFAULT CHARSET=utf8 COMMENT='재고-출고정보';

