) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8 COMMENT='재고-입고-메모정보';

--------------------------------------------------------

DROP TABLE IF EXISTS `TB_PLM_STOCK_SUPLER_ISSUE`;
CREATE TABLE `TB_PLM_STOCK_SUPLER_ISSUE` (
  `SUPLER_ISSUE_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '공급사별-출고SEQNO',
  `STOCK_ISSUE_SEQNO` int(11) DEFAULT NULL COMMENT '재고상품-출고SEQNO',
  `STOCK_RECEIPT_SEQNO` int(11) DEFAULT NULL COMMENT '재고상품-입고SEQNO',
  `ISSUE_DT` datetime DEFAULT NULL COMMENT '(출고)출고일시',
  `ISSUE_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '출고수량',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`SUPLER_ISSUE_SEQNO`),
  KEY `FK_TB_PLM_STOCK_RECEIPT_TO_TB_PLM_STOCK_SUPLER_ISSUE` (`STOCK_RECEIPT_SEQNO`),
  KEY `FK_TB_PLM_STOCK_ISSUE_PICKING_TO_TB_PLM_STOCK_SUPLER_ISSUE` (`STOCK_ISSUE_SEQNO`),
  CONSTRAINT `FK_TB_PLM_STOCK_ISSUE_PICKING_TO_TB_PLM_STOCK_SUPLER_ISSUE` FOREIGN KEY (`STOCK_ISSUE_SEQNO`) REFERENCES `TB_PLM_STOCK_ISSUE_PICKING` (`STOCK_ISSUE_SEQNO`),
  CONSTRAINT `FK_TB_PLM_STOCK_RECEIPT_TO_TB_PLM_STOCK_SUPLER_ISSUE` FOREIGN KEY (`STOCK_RECEIPT_SEQNO`) REFERENCES `TB_PLM_STOCK_RECEIPT` (`STOCK_RECEIPT_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=18 DEFAULT CHARSET=utf8 COMMENT='재고-공급사별-출고정보';

-------------------------------------------------

DROP TABLE IF EXISTS `TB_PLM_STOCK_SUPLER_RECEIPT`;
CREATE TABLE `TB_PLM_STOCK_SUPLER_RECEIPT` (
  `SUPLER_ISSUE_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '공급사별-출고SEQNO',
  `STOCK_RETURN_SEQNO` int(11) DEFAULT NULL COMMENT '재고상품-회수SEQNO',
  `STOCK_RECEIPT_SEQNO` int(11) DEFAULT NULL COMMENT '재고상품-입고SEQNO',
  `RECEIPT_DT` datetime DEFAULT NULL COMMENT '입고일시',
  `RECEIPT_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '입고수량',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`SUPLER_ISSUE_SEQNO`),
  KEY `FK_TB_PLM_STOCK_RETURN_TO_TB_PLM_STOCK_SUPLER_RECEIPT` (`STOCK_RETURN_SEQNO`),
  KEY `FK_TB_PLM_STOCK_RECEIPT_TO_TB_PLM_STOCK_SUPLER_RECEIPT` (`STOCK_RECEIPT_SEQNO`),
  CONSTRAINT `FK_TB_PLM_STOCK_RECEIPT_TO_TB_PLM_STOCK_SUPLER_RECEIPT` FOREIGN KEY (`STOCK_RECEIPT_SEQNO`) REFERENCES `TB_PLM_STOCK_RECEIPT` (`STOCK_RECEIPT_SEQNO`),
  CONSTRAINT `FK_TB_PLM_STOCK_RETURN_TO_TB_PLM_STOCK_SUPLER_RECEIPT` FOREIGN KEY (`STOCK_RETURN_SEQNO`) REFERENCES `TB_PLM_STOCK_RETURN` (`STOCK_RETURN_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=229 DEFAULT CHARSET=utf8 COMMENT='재고-공급사별-입고정보';

