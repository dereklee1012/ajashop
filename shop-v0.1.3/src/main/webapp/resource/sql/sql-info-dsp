DROP TABLE IF EXISTS `TB_PLM_PROD_OPT_MAPPING`;
CREATE TABLE `TB_PLM_PROD_OPT_MAPPING` (
  `MAP_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'MAP_SEQNO',
  `PLM_PROD_SEQNO` int(11) DEFAULT NULL COMMENT 'PLM_상품정보SEQ',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `PRODUCT_UUID` int(11) DEFAULT NULL COMMENT '상품UUID',
  `DLVY_OPTION_UUID` int(11) DEFAULT NULL COMMENT 'DLVY_OPTION_UUID',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`MAP_SEQNO`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_PROD_OPT_MAPPING` (`PLM_PROD_SEQNO`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_PLM_PROD_OPT_MAPPING` (`PRODUCT_UUID`),
  KEY `FK_TB_PROD_DLVY_OPTION_INFO_TO_TB_PLM_PROD_OPT_MAPPING` (`DLVY_OPTION_UUID`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_PROD_OPT_MAPPING` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_PLM_PROD_OPT_MAPPING` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_PROD_COUPON_OPTION_INFO_TO_TB_PLM_PROD_OPT_MAPPING` FOREIGN KEY (`DLVY_OPTION_UUID`) REFERENCES `TB_PROD_COUPON_OPTION_INFO` (`COUPON_OPTION_UUID`),
  CONSTRAINT `FK_TB_PROD_DLVY_OPTION_INFO_TO_TB_PLM_PROD_OPT_MAPPING` FOREIGN KEY (`DLVY_OPTION_UUID`) REFERENCES `TB_PROD_DLVY_OPTION_INFO` (`DLVY_OPTION_UUID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='상품(PROTDUCT) 정보2';

---------------------------------------------------------------------

DROP TABLE IF EXISTS `TB_PLM_PRODUCT_HST`;
CREATE TABLE `TB_PLM_PRODUCT_HST` (
  `HST_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'HST_SEQNO',
  `HST_CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'HST_CREATED_AT',
  `HST_TY` char(1) NOT NULL DEFAULT 'I' COMMENT 'HST 생성사유(I, U, D)',
  `HST_CREATED_USER` varchar(30) DEFAULT NULL COMMENT 'mysql user 가 들어올수 있어, VARCHAR',
  `PLM_PROD_SEQNO` int(11) NOT NULL COMMENT '상품코드(10000001,...)',
  `MFR_LOT_SEQNO` int(11) DEFAULT NULL COMMENT '(제조사)제품SEQNO',
  `PLM_PROD_CODE` varchar(30) NOT NULL COMMENT 'D + 분류코드(4) + PLM_PROD_SEQNO(8)',
  `PROD_SALE_STATUS_CD` char(1) NOT NULL DEFAULT 'S' COMMENT '상품판매상태코드: S(대기) E(노출) O(품절) N(비활성)\n	',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `STORE_SALE_PRICE` decimal(13,0) DEFAULT '0' COMMENT '부가세 있을경우, 포함하여 입력',
  `SELLING_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '부가세 있을경우, 포함하여 입력',
  `RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '부가세 있을경우, 포함하여 입력',
  `SUPPLY_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '부가세 있을경우, 제외하고 입력',
  `SUPPLY_PRICE_VAT` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '공급가액 * 0.1 = 부가세',
  `TAX_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '부가세 적용상품 여부',
  `TAX_RATE` decimal(6,3) NOT NULL DEFAULT '0.000' COMMENT '부가세율',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Tax refund 적용가능여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '면세상품여부',
  `GENUINE_MARK_PROD_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '정품인증마크 적용여부',
  `WAREHOUSING_TY` char(3) NOT NULL DEFAULT 'PST' COMMENT '발주방식 PRE(선구매) PST(후발주)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`HST_SEQNO`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_PRODUCT_HST` (`PLM_PROD_SEQNO`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_PRODUCT_HST` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='재고/물류_상품정보2';

-----------------------------------------------------------------

DROP TABLE IF EXISTS `TB_PLM_PRODUCT_INFO`;
CREATE TABLE `TB_PLM_PRODUCT_INFO` (
  `PLM_PROD_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '상품코드(10000001,...)',
  `MFR_LOT_BRCD` varchar(30) DEFAULT NULL COMMENT '제조사 상품바코',
  `MFR_LOT_SEQNO` int(11) DEFAULT NULL COMMENT '(제조사)제품SEQNO',
  `PLM_PROD_CODE` varchar(30) NOT NULL COMMENT 'D + 분류코드(4) + PLM_PROD_SEQNO(8) - D-00-000001',
  `PROD_SALE_STATUS_CD` char(1) NOT NULL DEFAULT 'S' COMMENT '상품판매상태코드: S(대기) E(노출) O(품절) N(비활성)\n	',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `PROD_NAME_KO` varchar(127) DEFAULT NULL COMMENT '상품(제조사) 명칭_ 국문',
  `PROD_NAME_ZH` varchar(127) DEFAULT NULL COMMENT '상품(제조사) 명칭_ 중문',
  `PROD_NAME_EN` varchar(127) DEFAULT NULL COMMENT '상품(제조사) 명칭_ 영문',
  `STORE_SALE_PRICE` decimal(13,0) DEFAULT '0' COMMENT '업체 판매가: 부가세 있을경우, 포함하여 입력',
  `SELLING_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '상품 판매가: 부가세 있을경우, 포함하여 입력',
  `RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '시중 판매가/소비자가: 부가세 있을경우, 포함하여 입력',
  `SUPPLY_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '공급 가격: 부가세 있을경우, 제외하고 입력',
  `SUPPLY_PRICE_VAT` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '공급 가격_부가세: 공급가액 * 0.1 = 부가세',
  `TAX_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '부가세 적용상품 여부',
  `TAX_RATE` decimal(6,3) NOT NULL DEFAULT '0.100' COMMENT '부가세율',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Tax refund 적용가능여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '면세상품여부',
  `GENUINE_MARK_PROD_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '정품인증마크 적용여부',
  `WAREHOUSING_TY` char(3) NOT NULL DEFAULT 'PST' COMMENT '발주방식 PRE(선구매) PST(후발주)',
  `BRAND_CODE` char(10) DEFAULT NULL COMMENT '브랜드코드',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `WRHOUS_CMPRT_SEQNO` int(11) DEFAULT NULL COMMENT '창고구획SEQNO',
  `OPTION_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '상품옵션 유무',
  `OPT_CODE` varchar(30) DEFAULT '000000' COMMENT '2(LVL1_CD) + 2(LVL2_CD) + (LVL3_CD)',
  `OPT_LEVEL` tinyint(4) NOT NULL DEFAULT '0' COMMENT '해당옵션 최대레벨',
  `OPT_LVL1_CD` varchar(30) DEFAULT '00' COMMENT '영,숫자',
  `OPT_LVL1_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(국문)',
  `OPT_LVL1_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(영문)',
  `OPT_LVL1_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(중문)',
  `OPT_LVL1_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(국문)',
  `OPT_LVL1_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(영어)',
  `OPT_LVL1_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(중문)',
  `OPT_LVL2_CD` varchar(30) DEFAULT '00' COMMENT '영,숫자',
  `OPT_LVL2_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(국문)',
  `OPT_LVL2_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(영문)',
  `OPT_LVL2_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(중문)',
  `OPT_LVL2_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(국문)',
  `OPT_LVL2_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(영어)',
  `OPT_LVL2_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(중문)',
  `OPT_LVL3_CD` varchar(30) DEFAULT '00' COMMENT '영,숫자',
  `OPT_LVL3_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(국문)',
  `OPT_LVL3_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(영문)',
  `OPT_LVL3_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(중문)',
  `OPT_LVL3_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(국문)',
  `OPT_LVL3_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(영어)',
  `OPT_LVL3_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(중문)',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  `PLM_PROD_NAME` varchar(127) DEFAULT NULL,
  PRIMARY KEY (`PLM_PROD_SEQNO`),
  KEY `FK_TB_PLM_MFR_LOT_MST_TO_TB_PLM_PRODUCT_INFO` (`MFR_LOT_SEQNO`),
  KEY `FK_TB_PLM_WRHOUS_COMPART_INFO_TO_TB_PLM_PRODUCT_INFO` (`WRHOUS_CMPRT_SEQNO`),
  KEY `FK_TB_BRAND_MST_TO_TB_PLM_PRODUCT_INFO` (`BRAND_CODE`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_PLM_PRODUCT_INFO` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_BRAND_MST_TO_TB_PLM_PRODUCT_INFO` FOREIGN KEY (`BRAND_CODE`) REFERENCES `TB_BRAND_MST` (`BRAND_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_PLM_PRODUCT_INFO` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_PLM_MFR_LOT_MST_TO_TB_PLM_PRODUCT_INFO` FOREIGN KEY (`MFR_LOT_SEQNO`) REFERENCES `TB_PLM_MFR_LOT_MST` (`MFR_LOT_SEQNO`),
  CONSTRAINT `FK_TB_PLM_WRHOUS_COMPART_INFO_TO_TB_PLM_PRODUCT_INFO` FOREIGN KEY (`WRHOUS_CMPRT_SEQNO`) REFERENCES `TB_PLM_WRHOUS_COMPART_INFO` (`WRHOUS_CMPRT_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=683 DEFAULT CHARSET=utf8 COMMENT='재고/물류_상품정보';

