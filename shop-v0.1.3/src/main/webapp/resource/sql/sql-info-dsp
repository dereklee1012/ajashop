DROP TABLE IF EXISTS `TB_ORDER_PREPARE_PROD`;
CREATE TABLE `TB_ORDER_PREPARE_PROD` (
  `PREPARE_PROD_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '임시주문-상품SEQ',
  `TEMP_ORDER_NO` int(11) DEFAULT NULL COMMENT '임시주문번호',
  `COMPANY_CODE` char(10) DEFAULT NULL COMMENT '업체코드',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드',
  `PROD_UUID` int(11) DEFAULT NULL COMMENT 'PROD_COUPON_UUID or PROD_DLVY_UUID',
  `PROD_CODE` char(10) DEFAULT NULL COMMENT 'D: Delivery C: Coupon L:Local coupon',
  `SUPLER_GOODS_CODE` varchar(32) DEFAULT NULL COMMENT '공급사상품코드',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'LC' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `PRODUCT_UUID` int(11) NOT NULL COMMENT '상품UUID',
  `PLM_PROD_SEQNO` int(11) DEFAULT NULL COMMENT 'PLM_상품정보SEQ',
  `PROD_BRCD` varchar(20) DEFAULT NULL COMMENT '상품바코드',
  `AREA_CODE` char(10) DEFAULT NULL COMMENT '지역 코드',
  `BRAND_CODE` char(10) DEFAULT NULL COMMENT '브랜드코드',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `DC_COUPON_APPLIED_YN` char(1) DEFAULT 'Y' COMMENT '[할인권적용상품여부]',
  `CANCEL_AVAIL_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '[취소가능여부]',
  `ITEM_QTY` int(11) DEFAULT '0' COMMENT '물품 수량',
  `PROD_NAME_EN` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(영어)',
  `PROD_NAME_KO` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(한국어)',
  `PROD_NAME_ZH` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(간체)',
  `PROD_SELLING_PRICE_KRW` decimal(13,0) DEFAULT '0' COMMENT '가격(판매가*수량+옵션)',
  `PROD_SELLING_PRICE_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '가격(판매가*수량+옵션)_CNY',
  `PROD_SELLING_PRICE_USD` decimal(13,2) DEFAULT '0.00' COMMENT '가격(판매가*수량+옵션)_USD',
  `PROD_DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '할인금액(원화)',
  `PROD_DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액(위안화)',
  `PROD_DC_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액(USD)',
  `STORE_SALE_PRICE_KRW` decimal(13,0) DEFAULT '0' COMMENT '업체 판매가_KRW',
  `STORE_SALE_PRICE_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '업체 판매가_CNY',
  `STORE_SALE_PRICE_USD` decimal(13,2) DEFAULT '0.00' COMMENT '업체 판매가_USD',
  `SELLING_PRICE_KRW` decimal(13,0) DEFAULT '0' COMMENT '상품 판매가_KRW',
  `SELLING_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '상품 판매가_CNY',
  `SELLING_PRICE_USD` decimal(13,2) DEFAULT '0.00' COMMENT '상품 판매가_USD',
  `RETAIL_PRICE_KRW` decimal(13,0) DEFAULT '0' COMMENT '시중 판매가,소비자가_KRW',
  `RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '시중 판매가,소비자가_CNY',
  `RETAIL_PRICE_USD` decimal(13,2) DEFAULT '0.00' COMMENT '시중 판매가,소비자가_USD',
  `SUPPLY_PRICE` decimal(13,0) DEFAULT '0' COMMENT '공급 가격',
  `TAX_REFUND_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '세금환급가능금액_KRW',
  `TAX_REFUND_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '세금환급가능금액_CNY',
  `TAX_REFUND_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '세금환급가능금액_USD',
  `TAX_YN` char(1) DEFAULT 'Y' COMMENT '과세 여부',
  `TAX_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '과세 RATE',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Tax refund 적용가능여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '면세상품여부',
  `REPRSNT_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '대표이미지경로',
  `REPRSNT_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '대표이미지파일명',
  `REPRSNT_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '대표이미지 (원)파일명',
  `REPRSNT_IMG_SIZE_H` int(11) DEFAULT NULL COMMENT '대표이미지사이즈_H',
  `REPRSNT_IMG_SIZE_V` int(11) DEFAULT NULL COMMENT '대표이미지사이즈_V',
  `SHELF_LIFE_DT` datetime DEFAULT NULL COMMENT '식품유효기간',
  `OPTION_ITEM_CHOICE_YN` char(1) DEFAULT 'Y' COMMENT '물품 옵션 선택여부',
  `OPTION_ITEM_LEVEL` tinyint(4) NOT NULL DEFAULT '0' COMMENT '최대 옵션 레벨',
  `OPTION_ADDED_PRICE` decimal(13,0) DEFAULT '0' COMMENT '옵션 추가 금액(단가)',
  `OPTION_ADDED_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '옵션 추가시 금액(위안화)',
  `OPTION_ADDED_SUPPLY_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가 공급 가격: 공급가격 차액입력',
  `OPTION_ADDED_RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가 시중 판매가/소비자가: 소비자가 차액입력',
  `OPTION_ADDED_RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '옵션 추가 시중 판매가/소비자가(위안화): 소비자가 차액입력',
  `PROD_OPT_UUID` int(11) DEFAULT NULL COMMENT 'COUPON_OPTION_UUID or DLVY_OPTION_UUID',
  `OPT_CODE` varchar(30) DEFAULT NULL COMMENT '상품옵션코드',
  `OPT_LVL1_CD` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션코드',
  `OPT_LVL1_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(영문)',
  `OPT_LVL1_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(국문)',
  `OPT_LVL1_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨1옵션코드 명칭(중문)',
  `OPT_LVL1_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(영어)',
  `OPT_LVL1_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(국문)',
  `OPT_LVL1_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨1 옵션명칭(중문)',
  `OPT_LVL2_CD` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션코드',
  `OPT_LVL2_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(영문)',
  `OPT_LVL2_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(국문)',
  `OPT_LVL2_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨2옵션코드 명칭(중문)',
  `OPT_LVL2_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(영어)',
  `OPT_LVL2_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(국문)',
  `OPT_LVL2_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨2 옵션명칭(중문)',
  `OPT_LVL3_CD` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션코드',
  `OPT_LVL3_CD_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(영문)',
  `OPT_LVL3_CD_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(국문)',
  `OPT_LVL3_CD_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨3옵션코드 명칭(중문)',
  `OPT_LVL3_NM_EN` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(영어)',
  `OPT_LVL3_NM_KO` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(국문)',
  `OPT_LVL3_NM_ZH` varchar(30) DEFAULT NULL COMMENT '레벨3 옵션명칭(중문)',
  `GENUINE_MARK_PROD_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '정품인증마크 적용여부',
  `WAREHOUSING_TY` char(3) DEFAULT NULL COMMENT '발주방식 PRE(선구매) PST(후발주)',
  `DISP_ORDER` tinyint(4) NOT NULL DEFAULT '1' COMMENT '노출순서',
  `EHGT_SEQNO` int(11) DEFAULT NULL COMMENT '환율SEQNO',
  `EHGT_SEQNO_USD` int(11) DEFAULT NULL COMMENT 'USD환율SEQNO',
  `EHGT_RATE_KRW_TO_CNY` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '원화,위안화환율',
  `EHGT_RATE_CNY_TO_KRW` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,원화환율',
  `EHGT_RATE_CNY_TO_USD` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,USD환율',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PREPARE_PROD_SEQNO`),
  KEY `FK_TB_ORDER_PREPARE_TO_TB_ORDER_PREPARE_PROD` (`TEMP_ORDER_NO`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_ORDER_PREPARE_PROD` (`CTGRY_CODE`),
  KEY `FK_TB_BRAND_MST_TO_TB_ORDER_PREPARE_PROD` (`BRAND_CODE`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_ORDER_PREPARE_PROD` (`PRODUCT_UUID`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_ORDER_PREPARE_PROD` (`PLM_PROD_SEQNO`),
  KEY `FK_TB_COMPANY_INFO_TO_TB_ORDER_PREPARE_PROD` (`COMPANY_CODE`),
  KEY `FK_TB_STORE_INFO_TO_TB_ORDER_PREPARE_PROD` (`STORE_CODE`),
  KEY `FK_TB_AREA_MST_TO_TB_ORDER_PREPARE_PROD` (`AREA_CODE`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PREPARE_PROD` (`EHGT_SEQNO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PREPARE_PROD_USD` (`EHGT_SEQNO_USD`),
  CONSTRAINT `FK_TB_AREA_MST_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`AREA_CODE`) REFERENCES `TB_AREA_MST` (`AREA_CODE`),
  CONSTRAINT `FK_TB_BRAND_MST_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`BRAND_CODE`) REFERENCES `TB_BRAND_MST` (`BRAND_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_COMPANY_INFO_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`COMPANY_CODE`) REFERENCES `TB_COMPANY_INFO` (`COMPANY_CODE`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`EHGT_SEQNO`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PREPARE_PROD_USD` FOREIGN KEY (`EHGT_SEQNO_USD`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_PREPARE_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`TEMP_ORDER_NO`) REFERENCES `TB_ORDER_PREPARE` (`TEMP_ORDER_NO`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_STORE_INFO_TO_TB_ORDER_PREPARE_PROD` FOREIGN KEY (`STORE_CODE`) REFERENCES `TB_STORE_INFO` (`STORE_CODE`)
) ENGINE=InnoDB AUTO_INCREMENT=7431 DEFAULT CHARSET=utf8 COMMENT='주문_임시주문-상품정보';

