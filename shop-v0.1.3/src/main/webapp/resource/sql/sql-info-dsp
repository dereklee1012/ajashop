DROP TABLE IF EXISTS `TB_PROD_COUPON_INFO`;
CREATE TABLE `TB_PROD_COUPON_INFO` (
  `PROD_COUPON_UUID` int(11) NOT NULL AUTO_INCREMENT COMMENT 'PROD_COUPON_UUID',
  `PROD_CODE` char(10) DEFAULT NULL COMMENT 'D: Delivery C: Coupon L:Local coupon',
  `PRODUCT_UUID` int(11) DEFAULT NULL COMMENT '상품UUID',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드',
  `COMPANY_CODE` char(10) DEFAULT NULL COMMENT '쿠폰업체',
  `UPPER_PROD_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '(상위)상품 여부 (패키지 상품 여부)',
  `UPPER_PROD_COUPON_UUID` int(11) DEFAULT NULL COMMENT '(상위)상품 UUID',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'LC' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `SUPLER_GOODS_CODE` varchar(32) DEFAULT NULL COMMENT '공급사상품코드',
  `PROD_SALE_STATUS_CD` char(1) NOT NULL DEFAULT 'S' COMMENT '상품판매상태코드: S(대기) E(노출) O(품절) N(비활성)\n	',
  `WAREHOUSING_TY` char(3) DEFAULT 'PST' COMMENT '입고방식 PRE(선구매) PST(후발주)',
