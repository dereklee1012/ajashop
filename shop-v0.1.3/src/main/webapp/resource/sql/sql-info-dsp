DROP TABLE IF EXISTS `TB_PROD_COUPON_REVIEW_INFO`;
CREATE TABLE `TB_PROD_COUPON_REVIEW_INFO` (
  `REVIEW_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '리뷰SEQNO',
  `PROD_COUPON_UUID` int(11) NOT NULL COMMENT 'PROD_COUPON_UUID',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `REVIEW_CONTENTS` varchar(1024) DEFAULT NULL COMMENT 'REVIEW_CONTENTS',
  `PHOTO_CNT` tinyint(4) NOT NULL DEFAULT '0' COMMENT '등록사진개수',
  `PHOTO_SEQNO1` int(11) DEFAULT '0' COMMENT 'PHOTO_SEQNO1',
  `PHOTO_SEQNO2` int(11) DEFAULT '0' COMMENT 'PHOTO_SEQNO2',
  `PHOTO_SEQNO3` int(11) DEFAULT '0' COMMENT 'PHOTO_SEQNO3',
  `PHOTO_SEQNO4` int(11) DEFAULT '0' COMMENT 'PHOTO_SEQNO4',
  `PHOTO_SEQNO5` int(11) DEFAULT '0' COMMENT 'PHOTO_SEQNO5',
  `THUMNAIL_PATH` varchar(255) DEFAULT NULL COMMENT '이미지 썸네일 경로',
  `THUMNAIL_FILENAME` varchar(128) DEFAULT NULL COMMENT '이미지 썸네일명',
  `THUMNAIL_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '이미지 썸네일 (원)파일명',
  `THUMNAIL_SIZE_H` decimal(4,0) DEFAULT NULL COMMENT '썸네일이미지사이즈_H',
  `THUMNAIL_SIZE_V` decimal(4,0) DEFAULT NULL COMMENT '썸네일이미지사이즈_V',
  `EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '노출',
  `STAR_POINT` decimal(3,1) DEFAULT NULL COMMENT '별점',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`REVIEW_SEQNO`),
  KEY `FK_TB_PROD_COUPON_INFO_TO_TB_PROD_COUPON_REVIEW_INFO` (`PROD_COUPON_UUID`),
  CONSTRAINT `FK_TB_PROD_COUPON_INFO_TO_TB_PROD_COUPON_REVIEW_INFO` FOREIGN KEY (`PROD_COUPON_UUID`) REFERENCES `TB_PROD_COUPON_INFO` (`PROD_COUPON_UUID`)
) ENGINE=InnoDB AUTO_INCREMENT=55 DEFAULT CHARSET=utf8 COMMENT='상품_쿠폰_리뷰정보';

