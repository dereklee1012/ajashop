DROP TABLE IF EXISTS `TB_PROD_COUPON_INFO_HST`;
CREATE TABLE `TB_PROD_COUPON_INFO_HST` (
  `HST_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'HST_SEQNO',
  `HST_CREATED_USER` varchar(20) DEFAULT NULL COMMENT 'HST_CREATED_USER',
  `HST_TY` char(1) NOT NULL DEFAULT 'D' COMMENT 'History write At - I(nsert), U(pdate), D(Delete)',
  `HST_CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'HST_CREATED_AT',
  `PROD_COUPON_UUID` int(11) NOT NULL COMMENT 'PROD_COUPON_UUID',
  `PROD_CODE` char(10) DEFAULT NULL COMMENT 'D: Delivery C: Coupon L:Local coupon',
  `PRODUCT_UUID` int(11) DEFAULT NULL COMMENT '상품UUID',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드',
  `COMPANY_CODE` char(10) DEFAULT NULL COMMENT '쿠폰업체',
  `UPPER_PROD_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '(상위)상품 여부 (패키지 상품 여부)',
  `UPPER_PROD_COUPON_UUID` int(11) DEFAULT NULL COMMENT '(상위)상품 UUID',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'LC' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'C' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드) T(티켓)',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `SUPLER_GOODS_CODE` varchar(32) DEFAULT NULL COMMENT '공급사상품코드',
  `PROD_SALE_STATUS_CD` char(1) NOT NULL DEFAULT 'S' COMMENT 'S(대기) E(노출) O(품절) N(비활성)',
  `WAREHOUSING_TY` char(3) DEFAULT NULL COMMENT '입고방식 PRE(선구매) PST(후발주)',
  `AREA_CODE` char(10) DEFAULT NULL COMMENT '지역 코드',
  `BRAND_CODE` char(10) DEFAULT NULL COMMENT '브랜드코드',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `PROD_SALE_START_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '판매 시작일시',
  `PROD_SALE_END_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '판매 종료일시',
  `RECOMEND_PROD_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '추천상품여부',
  `DC_COUPON_APPLIED_YN` char(1) DEFAULT 'Y' COMMENT '[할인권적용상품여부]',
  `PROD_DISP_ORDER` int(11) NOT NULL DEFAULT '1' COMMENT '상품 노출 순서',
  `MNMNU_EXPSR_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '메인메뉴 노출',
  `MNMNU_EXPSR_ORDR` int(11) NOT NULL COMMENT '메인메뉴 노출순서',
  `SHELF_LIFE_DT` datetime DEFAULT NULL COMMENT '식품유효기간',
  `PROD_NAME_EN` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(영어)',
  `PROD_NAME_KO` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(한국어)',
  `PROD_NAME_ZH` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(간체)',
  `PROD_OPTION_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '상품옵션 유무',
  `PROD_OPTION_LEVEL` tinyint(4) NOT NULL DEFAULT '0' COMMENT '상품옵션 LEVEL',
  `PROD_OPTION_CNT` int(11) NOT NULL DEFAULT '0' COMMENT '총옵션상품종류',
  `SALE_OVERLAP_YN` char(1) DEFAULT 'Y' COMMENT '[중복구매가능여부]',
  `REPRSNT_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '대표이미지경로',
  `REPRSNT_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '대표이미지파일명',
  `REPRSNT_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '대표이미지 (원)파일명',
  `REPRSNT_IMG_SIZE_H` int(11) DEFAULT NULL COMMENT '대표이미지사이즈_H',
  `REPRSNT_IMG_SIZE_V` int(11) DEFAULT NULL COMMENT '대표이미지사이즈_V',
  `PROD_INFO_ITEM1` varchar(255) DEFAULT NULL COMMENT '상품정보1',
  `PROD_INFO_CONT1` varchar(1023) DEFAULT NULL COMMENT '상품정보-상세1',
