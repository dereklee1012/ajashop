  `NW_ITEM_PR_PHRASE_KO` varchar(1023) DEFAULT NULL COMMENT '주요아이템 홍보문구(한국어)',
  `NW_ITEM_PR_PHRASE_ZH` varchar(1023) DEFAULT NULL COMMENT '주요아이템 홍보문구(간체)',
  `NW_ITEM_PR_PHRASE_EN` varchar(1023) DEFAULT NULL COMMENT '주요아이템 홍보문구(영어)',
  `NW_ITEM_TY` char(1) NOT NULL DEFAULT 'P' COMMENT '주요아이템 타입: P(상품템플릿) E(이벤트템플릿)',
