  `PROD_INFO_ITEM3` varchar(255) DEFAULT NULL COMMENT '상품정보3',
  `PROD_INFO_CONT3` varchar(1023) DEFAULT NULL COMMENT '상품정보-상세3',
  `PROD_INFO_ITEM4` varchar(255) DEFAULT NULL COMMENT '상품정보4',
  `PROD_INFO_CONT4` varchar(1023) DEFAULT NULL COMMENT '상품정보-상세4',
