  `STORE_CODE` char(10) NOT NULL COMMENT '매장코드',
  `COMPANY_CODE` char(10) NOT NULL COMMENT '업체코드',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`STORE_CODE`,`COMPANY_CODE`),
  KEY `FK_TB_COMPANY_INFO_TO_TB_COMPANY_STORE_MAP` (`COMPANY_CODE`),
  CONSTRAINT `FK_TB_COMPANY_INFO_TO_TB_COMPANY_STORE_MAP` FOREIGN KEY (`COMPANY_CODE`) REFERENCES `TB_COMPANY_INFO` (`COMPANY_CODE`),
  CONSTRAINT `FK_TB_STORE_INFO_TO_TB_COMPANY_STORE_MAP` FOREIGN KEY (`STORE_CODE`) REFERENCES `TB_STORE_INFO` (`STORE_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='업체, 매장 매핑 정보';

