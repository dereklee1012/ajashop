DROP TABLE IF EXISTS `TB_CONTRACT_INFO`;
CREATE TABLE `TB_CONTRACT_INFO` (
  `CONTRACT_CODE` varchar(20) NOT NULL COMMENT '계약코드',
  `COMPANY_CODE` char(10) NOT NULL COMMENT '업체코드',
  `CONTRACT_STATE_CD` char(1) DEFAULT NULL COMMENT '계약상태 - 0 계약대기, 1 계약승인요청, 2 계약완료, 3 계약종료',
  `APPROVAL_STATE_CD` char(1) DEFAULT NULL COMMENT '승인상태 - 0 : 미승인, 1 : 승인완료, 2 : 승인거부',
  `CANCEL_REASON` varchar(511) DEFAULT NULL COMMENT '승인거부사유',
  `ETC` varchar(511) DEFAULT NULL COMMENT '특이사항',
  `CONTRACT_NAME` varchar(127) DEFAULT NULL COMMENT '계약명',
  `CNTRCT_STR_DTS` varchar(8) DEFAULT NULL COMMENT '계약시작일',
  `CNTRCT_END_DTS` varchar(8) DEFAULT NULL COMMENT '계약종료일',
  `MANAGER_NAME` varchar(30) DEFAULT NULL COMMENT '계약담당자명',
  `FILE_NAME` varchar(128) DEFAULT NULL COMMENT '계약서 파일명',
  `MNG_ADMIN_NO` int(11) DEFAULT NULL COMMENT '등록자 ID',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`CONTRACT_CODE`),
  KEY `FK_TB_COMPANY_INFO_TO_TB_CONTRACT_INFO` (`COMPANY_CODE`),
  CONSTRAINT `FK_TB_COMPANY_INFO_TO_TB_CONTRACT_INFO` FOREIGN KEY (`COMPANY_CODE`) REFERENCES `TB_COMPANY_INFO` (`COMPANY_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='계약정보';

