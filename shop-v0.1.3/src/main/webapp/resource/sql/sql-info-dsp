  `EHGT_RATE_CNY_TO_USD` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,USD환율',
  `PAY_CURRENCY` char(3) NOT NULL DEFAULT 'CNY' COMMENT 'CNY(위안화) USD(달러) KRW(한화)',
  `ACC_CURRENCY` char(3) NOT NULL DEFAULT 'USD' COMMENT 'CNY(위안화) USD(달러) KRW(한화)',
  `RETURN_STATUS_CD` char(2) NOT NULL DEFAULT 'NA' COMMENT 'NA(진행내용없음) RS(반품요청접수. C/S) RR(반품요청) RD(반품중) RC(반품완료) RF(반품실패)',
  `RETURN_REQ_DT` datetime DEFAULT NULL COMMENT '반품요청일시',
  `RETURN_FIN_DT` datetime DEFAULT NULL COMMENT '반품완료일시',
  `RETURN_RESON_CD` char(2) DEFAULT '00' COMMENT '반품사유코드',
  `RETURN_RESON_DESC` varchar(1023) DEFAULT NULL COMMENT '반품상세사유',
  `EXCHANGE_STATUS_CD` char(2) NOT NULL DEFAULT 'NA' COMMENT 'NA(진행내용없음) ES(교환요청접수. C/S) ER(교환요청) ED(교환중) EC(교환완료) EF(교환실패)',
  `EXCHANGE_REQ_DT` datetime DEFAULT NULL COMMENT '교환요청일시',
  `EXCHANGE_FIN_DT` datetime DEFAULT NULL COMMENT '교환완료일시',
  `EXCHANGE_RESON_CD` char(2) DEFAULT '00' COMMENT '교환사유코드',
  `EXCHANGE_RESON_DESC` varchar(1023) DEFAULT NULL COMMENT '교환상세사유',
  `TOUR_AGENCY_CODE` char(10) DEFAULT NULL COMMENT '여행사 업체코드',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`ORDER_SEQNO`),
  UNIQUE KEY `UIX_TB_ORDER_INFO_ORDER_UUID` (`ORDER_UUID`),
  KEY `FK_TB_USER_INFO_TO_TB_ORDER_INFO` (`UNO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_INFO` (`EHGT_SEQNO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_INFO2` (`EHGT_SEQNO_USD`),
  KEY `FK_TB_TOUR_AGENCY_INFO_TO_TB_ORDER_INFO` (`TOUR_AGENCY_CODE`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_INFO` FOREIGN KEY (`EHGT_SEQNO`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_INFO2` FOREIGN KEY (`EHGT_SEQNO_USD`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_TOUR_AGENCY_INFO_TO_TB_ORDER_INFO` FOREIGN KEY (`TOUR_AGENCY_CODE`) REFERENCES `TB_TOUR_AGENCY_INFO` (`TOUR_AGENCY_CODE`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_ORDER_INFO` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=96146 DEFAULT CHARSET=utf8 COMMENT='주문정보';

