DROP TABLE IF EXISTS `TB_ORDER_PROD`;
CREATE TABLE `TB_ORDER_PROD` (
  `PROD_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'PROD_SEQNO',
  `ORDER_SEQNO` int(11) NOT NULL COMMENT 'ORDER_SEQNO',
  `COMPANY_CODE` char(10) DEFAULT NULL COMMENT '업체코드',
  `STORE_CODE` char(10) DEFAULT NULL COMMENT '매장코드',
  `UNO` int(11) NOT NULL COMMENT 'UNO(사용자NO)',
  `MARKET_CODE` char(2) NOT NULL DEFAULT 'LC' COMMENT 'LC(딩동내) AT(알리트립) CT(씨트립)',
  `PLM_PROD_SEQNO` int(11) DEFAULT NULL COMMENT 'PLM_상품정보SEQ',
  `PROD_UUID` int(11) DEFAULT NULL COMMENT 'PROD_COUPON_UUID or PROD_DLVY_UUID',
  `PROD_CODE` char(10) NOT NULL COMMENT 'D: Delivery C: Coupon L:Local coupon',
  `PRODUCT_UUID` int(11) DEFAULT NULL COMMENT '상품UUID',
  `GDTP_CODE` char(1) NOT NULL DEFAULT 'D' COMMENT '상품유형코드 D(배송) C(쿠폰) W(WiFI) G(딩동카드)',
  `GOODS_TYPE` char(2) DEFAULT '01' COMMENT '[상품유형]01:배송상품, 02:E쿠폰, 03:포인트선물,10:로컬쿠폰 90(B2B2C쿠폰상품), 91(B2B2C배송상품) 20:티켓/예매권',
  `SUPLER_GOODS_CODE` varchar(32) DEFAULT NULL COMMENT '공급사상품코드',
  `PROD_ORDER_STATUS_CD` char(2) NOT NULL DEFAULT 'OR' COMMENT 'TB_ORDER_INFO.ORDER_STATUS_CD 참고',
  `PROD_ORDER_REQ_DT` datetime DEFAULT NULL COMMENT '주문일시',
  `PROD_ORDER_FIN_DT` datetime DEFAULT NULL COMMENT '주문완료일시',
  `PROD_CANCLE_STATUS_CD` char(2) DEFAULT 'NA' COMMENT 'NA(진행내용없음) CR(주문취소요청) CC(취소완료) CF(취소실패)',
  `PROD_CANCLE_REQ_DT` datetime DEFAULT NULL COMMENT '주문취소요청일시',
  `PROD_CANCLE_FIN_DT` datetime DEFAULT NULL COMMENT '주문취소완료일시',
  `PROD_NAME_EN` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(영어)',
  `PROD_NAME_KO` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(한국어)',
  `PROD_NAME_ZH` varchar(127) DEFAULT NULL COMMENT 'PRODUCT_명(간체)',
  `OPTION_ITEM_CHOICE_YN` char(1) DEFAULT 'Y' COMMENT '물품 옵션 선택여부',
  `OPTION_ITEM_LEVEL` tinyint(4) NOT NULL DEFAULT '0' COMMENT '최대 옵션 레벨',
  `PROD_SELLING_PRICE` decimal(13,0) DEFAULT '0' COMMENT '가격(판매가*수량+옵션)',
  `PROD_SELLING_PRICE_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '가격(판매가*수량+옵션)_CNY',
  `SELLING_PRICE` decimal(13,0) DEFAULT '0' COMMENT '상품 판매가',
  `SELLING_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '상품 판매가(위안화)',
  `ITEM_QTY` int(11) DEFAULT '0' COMMENT '물품 수량',
  `PROD_DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '할인금액(원화)',
  `PROD_DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액(위안화)',
  `OPTION_ADDED_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '옵션 추가 금액(단가) ',
  `OPTION_ADDED_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '옵션 추가시 금액(위안화) ',
  `STORE_SALE_PRICE` decimal(13,0) DEFAULT '0' COMMENT '업체 판매가',
  `STORE_SALE_PRICE_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '업체 판매가_CNY',
  `RETAIL_PRICE` decimal(13,0) DEFAULT '0' COMMENT '시중 판매가',
  `RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '시중 판매가/소비자가(위안화)',
  `SUPPLY_PRICE` decimal(13,0) DEFAULT '0' COMMENT '공급 가격',
  `CANCLE_PAY_TY` char(1) NOT NULL DEFAULT 'N' COMMENT 'N(정상) T(전체취소) P(부분취소)',
  `CANCLE_PAY_AMT_USD` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_KRW` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `CANCLE_PAY_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '전체취소시는 결제금액과 동일. 부분취소시는 취소금액',
  `BRAND_CODE` char(10) DEFAULT NULL COMMENT '브랜드코드',
  `AREA_CODE` char(10) DEFAULT NULL COMMENT '지역 코드',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `DC_COUPON_APPLIED_YN` char(1) DEFAULT 'Y' COMMENT '[할인권적용상품여부]',
  `CANCEL_AVAIL_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '[취소가능여부]',
  `PROD_CANCLE_RESON_CD` char(2) DEFAULT '00' COMMENT 'TB_ORDER_INFO.CANCLE_RESON_CD 참고',
  `PROD_CANCLE_RESON_DESC` varchar(1023) DEFAULT NULL COMMENT '주문취소 상세사유',
  `PROD_RETURN_STATUS_CD` char(2) NOT NULL DEFAULT 'NA' COMMENT 'NA(진행내용없음) RS(반품요청접수. C/S) RR(반품요청) RD(반품중) RC(반품완료) RF(반품실패)',
  `PROD_RETURN_REQ_DT` datetime DEFAULT NULL COMMENT '반품요청일시',
  `PROD_RETURN_FIN_DT` datetime DEFAULT NULL COMMENT '반품완료일시',
  `PROD_RETURN_RESON_CD` char(2) DEFAULT '00' COMMENT '반품사유코드',
  `PROD_RETURN_RESON_DESC` varchar(1023) DEFAULT NULL COMMENT '반품상세사유',
  `PROD_EXCHANGE_STATUS_CD` char(2) NOT NULL DEFAULT 'NA' COMMENT 'NA(진행내용없음) ES(교환요청접수. C/S) ER(교환요청) ED(교환중) EC(교환완료) EF(교환실패)',
  `PROD_EXCHANGE_REQ_DT` datetime DEFAULT NULL COMMENT '교환요청일시',
  `PROD_EXCHANGE_FIN_DT` datetime DEFAULT NULL COMMENT '교환완료일시',
  `PROD_EXCHANGE_RESON_CD` char(2) DEFAULT '00' COMMENT '교환사유코드',
  `PROD_EXCHANGE_RESON_DESC` varchar(1023) DEFAULT NULL COMMENT '교환상세사유',
  `PROD_BRCD` varchar(20) DEFAULT NULL COMMENT '상품바코드',
  `WAREHOUSING_TY` char(3) DEFAULT NULL COMMENT '발주방식 PRE(선구매) PST(후발주)',
  `SHELF_LIFE_DT` datetime DEFAULT NULL COMMENT '식품유효기간',
  `CNY_EXCHANGE_RATE` decimal(13,6) DEFAULT '0.000000' COMMENT '위안화 환율',
  `DISCOUNT_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '할인율(시중가대비)',
  `TAX_YN` char(1) DEFAULT 'Y' COMMENT '과세 여부',
  `TAX_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '과세 RATE',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Tax refund 적용가능여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '면세상품여부',
  `GENUINE_MARK_PROD_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '정품인증마크 적용여부',
  `REPRSNT_IMG_PATH` varchar(255) DEFAULT NULL COMMENT '대표이미지 경로',
  `REPRSNT_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '대표이미지 파일명',
  `REPRSNT_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '대표이미지 (원)파일명',
  `REPRSNT_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '대표이미지파일 사이즈_H',
  `REPRSNT_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '대표이미지파일 사이즈_V',
  `PREPARE_PROD_SEQNO` int(11) DEFAULT NULL COMMENT '임시주문-상품SEQ(주문처리 시 임시로 데이터 입력 후 삭제)',
  `EHGT_SEQNO` int(11) DEFAULT NULL COMMENT '환율SEQNO',
  `EHGT_SEQNO_USD` int(11) DEFAULT NULL COMMENT 'USD환율SEQNO',
  `EHGT_RATE_KRW_TO_CNY` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '원화,위안화환율',
  `EHGT_RATE_CNY_TO_KRW` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,원화환율',
  `EHGT_RATE_CNY_TO_USD` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,USD환율',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PROD_SEQNO`),
  KEY `FK_TB_USER_INFO_TO_TB_ORDER_PROD` (`UNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_ORDER_PROD` (`ORDER_SEQNO`),
  KEY `FK_TB_BRAND_MST_TO_TB_ORDER_PROD` (`BRAND_CODE`),
  KEY `FK_TB_AREA_MST_TO_TB_ORDER_PROD` (`AREA_CODE`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_ORDER_PROD` (`CTGRY_CODE`),
  KEY `FK_TB_COMPANY_INFO_TO_TB_ORDER_PROD` (`COMPANY_CODE`),
  KEY `FK_TB_STORE_INFO_TO_TB_ORDER_PROD` (`STORE_CODE`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_ORDER_PROD` (`PRODUCT_UUID`),
  KEY `FK_TB_ORDER_PREPARE_PROD_TO_TB_ORDER_PROD` (`PREPARE_PROD_SEQNO`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_ORDER_PROD` (`PLM_PROD_SEQNO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TB_ORDER_PROD` (`EHGT_SEQNO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PROD_USD` (`EHGT_SEQNO_USD`),
  CONSTRAINT `FK_TB_AREA_MST_TO_TB_ORDER_PROD` FOREIGN KEY (`AREA_CODE`) REFERENCES `TB_AREA_MST` (`AREA_CODE`),
  CONSTRAINT `FK_TB_BRAND_MST_TO_TB_ORDER_PROD` FOREIGN KEY (`BRAND_CODE`) REFERENCES `TB_BRAND_MST` (`BRAND_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_ORDER_PROD` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_COMPANY_INFO_TO_TB_ORDER_PROD` FOREIGN KEY (`COMPANY_CODE`) REFERENCES `TB_COMPANY_INFO` (`COMPANY_CODE`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TB_ORDER_PROD` FOREIGN KEY (`EHGT_SEQNO`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_ORDER_PROD_USD` FOREIGN KEY (`EHGT_SEQNO_USD`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_ORDER_PROD` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_PREPARE_PROD_TO_TB_ORDER_PROD` FOREIGN KEY (`PREPARE_PROD_SEQNO`) REFERENCES `TB_ORDER_PREPARE_PROD` (`PREPARE_PROD_SEQNO`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_ORDER_PROD` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_ORDER_PROD` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_STORE_INFO_TO_TB_ORDER_PROD` FOREIGN KEY (`STORE_CODE`) REFERENCES `TB_STORE_INFO` (`STORE_CODE`),
  CONSTRAINT `FK_TB_USER_INFO_TO_TB_ORDER_PROD` FOREIGN KEY (`UNO`) REFERENCES `TB_USER_INFO` (`UNO`)
) ENGINE=InnoDB AUTO_INCREMENT=3311 DEFAULT CHARSET=utf8 COMMENT='주문_상품정보';

