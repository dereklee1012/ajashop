DROP TABLE IF EXISTS `TB_PROD_COUPON_OVERLAP_INFO`;
CREATE TABLE `TB_PROD_COUPON_OVERLAP_INFO` (
  `PROD_COUPON_UUID` int(11) NOT NULL COMMENT 'PROD_COUPON_UUID',
  `START_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'START_DATE',
  `END_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'END_DATE',
  `SALE_OVERLAP_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '중복 구매 가능 여부',
  `SALE_OVERLAP_TERM` char(1) NOT NULL DEFAULT 'D' COMMENT '일, 주, 년',
  `SALE_OVERLAP_CNT` int(11) NOT NULL DEFAULT '10' COMMENT '중복 구매 가능 수량',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PROD_COUPON_UUID`),
  CONSTRAINT `FK_TB_PROD_COUPON_INFO_TO_TB_PROD_COUPON_OVERLAP_INFO` FOREIGN KEY (`PROD_COUPON_UUID`) REFERENCES `TB_PROD_COUPON_INFO` (`PROD_COUPON_UUID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='상품_쿠폰상품-중복구매정보';

---------------------------------------------------------

DROP TABLE IF EXISTS `TB_PROD_COUPON_PRICE_INFO`;
CREATE TABLE `TB_PROD_COUPON_PRICE_INFO` (
  `PROD_COUPON_UUID` int(11) NOT NULL COMMENT 'PROD_COUPON_UUID',
  `START_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'START_DATE',
  `END_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'END_DATE',
  `STORE_SALE_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '업체 판매가',
  `SELLING_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '상품 판매가',
  `SELLING_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '상품 판매가(위안화)',
  `RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '시중 판매가/소비자가',
  `RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '시중 판매가/소비자가(위안화)',
  `SUPPLY_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '공급 가격',
  `DISCOUNT_RATE` decimal(6,3) NOT NULL DEFAULT '0.000' COMMENT '할인율(시중가대비)',
  `TAX_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '과세 여부',
  `TAX_RATE` decimal(6,3) NOT NULL DEFAULT '0.000' COMMENT '과세 RATE',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Tax refund 적용가능여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '면세상품여부',
  `EHGT_SEQNO` int(11) DEFAULT NULL COMMENT '환율SEQNO',
  `EHGT_SEQNO_USD` int(11) DEFAULT NULL COMMENT 'USD환율SEQNO',
  `EHGT_RATE_KRW_TO_CNY` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '원화,위안화환율',
  `EHGT_RATE_CNY_TO_KRW` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,원화환율',
  `EHGT_RATE_CNY_TO_USD` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,USD환율',
  `FIXED_EXCHANGE_RATE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '상품 고정환율 가격 적용여부',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PROD_COUPON_UUID`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_PROD_COUPON_PRICE_INFO` (`EHGT_SEQNO`),
  KEY `FK_TB_EXCHANGE_RATE_MST_TO_TB_PROD_COUPON_PRICE_INFO2` (`EHGT_SEQNO_USD`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_PROD_COUPON_PRICE_INFO` FOREIGN KEY (`EHGT_SEQNO`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_EXCHANGE_RATE_MST_TO_TB_PROD_COUPON_PRICE_INFO2` FOREIGN KEY (`EHGT_SEQNO_USD`) REFERENCES `TB_EXCHANGE_RATE_MST` (`EHGT_SEQNO`),
  CONSTRAINT `FK_TB_PROD_COUPON_INFO_TO_TB_PROD_COUPON_PRICE_INFO` FOREIGN KEY (`PROD_COUPON_UUID`) REFERENCES `TB_PROD_COUPON_INFO` (`PROD_COUPON_UUID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='상품_쿠폰상품-가격정보';

---------------------------------------------------------

DROP TABLE IF EXISTS `TB_PROD_COUPON_PRICE_INFO_HST`;
CREATE TABLE `TB_PROD_COUPON_PRICE_INFO_HST` (
  `HST_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'HST_SEQNO',
  `HST_CREATED_USER` varchar(20) DEFAULT NULL COMMENT 'HST_CREATED_USER',
  `HST_TY` char(1) NOT NULL DEFAULT 'D' COMMENT 'History write At - I(nsert), U(pdate), D(Delete)',
  `HST_CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'HST_CREATED_AT',
  `PROD_COUPON_UUID` int(11) NOT NULL COMMENT 'PROD_COUPON_UUID',
  `START_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'START_DATE',
  `END_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'END_DATE',
  `STORE_SALE_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '업체 판매가',
  `SELLING_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '상품 판매가',
  `SELLING_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '상품 판매가(위안화)',
  `RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '시중 판매가/소비자가',
  `RETAIL_PRICE_CNY` decimal(13,2) NOT NULL DEFAULT '0.00' COMMENT '시중 판매가/소비자가(위안화)',
  `SUPPLY_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '공급 가격',
  `DISCOUNT_RATE` decimal(6,3) NOT NULL DEFAULT '0.000' COMMENT '할인율(시중가대비)',
  `TAX_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '과세 여부',
  `TAX_RATE` decimal(6,3) NOT NULL DEFAULT '0.000' COMMENT '과세 RATE',
  `TAX_REFUND_AVL_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Tax refund 적용가능여부',
  `TAX_FREE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '면세상품여부',
  `EHGT_SEQNO` int(11) DEFAULT NULL COMMENT '환율SEQNO',
  `EHGT_SEQNO_USD` int(11) DEFAULT NULL COMMENT 'USD환율SEQNO',
  `EHGT_RATE_KRW_TO_CNY` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '원화,위안화환율',
  `EHGT_RATE_CNY_TO_KRW` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,원화환율',
  `EHGT_RATE_CNY_TO_USD` decimal(13,9) NOT NULL DEFAULT '0.000000000' COMMENT '위안화,USD환율',
  `FIXED_EXCHANGE_RATE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '상품 고정환율 가격 적용여부',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`HST_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=161071 DEFAULT CHARSET=utf8 COMMENT='상품_쿠폰상품-가격정보_HST';

