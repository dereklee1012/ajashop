DROP TABLE IF EXISTS `TB_ORDER_PREPARE_DC`;
CREATE TABLE `TB_ORDER_PREPARE_DC` (
  `TEMP_ORDER_DC_NO` int(11) NOT NULL AUTO_INCREMENT COMMENT '임시주문-DC적용SEQ',
  `TEMP_ORDER_NO` int(11) DEFAULT NULL COMMENT '임시주문번호',
  `USER_DC_CPN_SEQNO` int(11) DEFAULT NULL COMMENT 'DC_CPN_SEQNO -> 사용자 다운로드 할인권SEQNO',
  `DC_AMT_USD` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액_USD',
  `DC_AMT_KRW` decimal(13,0) DEFAULT '0' COMMENT '할인금액_KRW',
  `DC_AMT_CNY` decimal(13,2) DEFAULT '0.00' COMMENT '할인금액_위안화',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`TEMP_ORDER_DC_NO`),
  KEY `FK_TB_ORDER_PREPARE_TO_TB_ORDER_PREPARE_DC` (`TEMP_ORDER_NO`),
  KEY `FK_TB_USER_DISCOUNT_COUPON_TO_TB_ORDER_PREPARE_DC` (`USER_DC_CPN_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_PREPARE_TO_TB_ORDER_PREPARE_DC` FOREIGN KEY (`TEMP_ORDER_NO`) REFERENCES `TB_ORDER_PREPARE` (`TEMP_ORDER_NO`),
  CONSTRAINT `FK_TB_USER_DISCOUNT_COUPON_TO_TB_ORDER_PREPARE_DC` FOREIGN KEY (`USER_DC_CPN_SEQNO`) REFERENCES `TB_USER_DISCOUNT_COUPON` (`USER_DC_CPN_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=45 DEFAULT CHARSET=utf8 COMMENT='주문_임시주문-DC';

