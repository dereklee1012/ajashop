  `EXPSR_ORDR` tinyint(4) NOT NULL DEFAULT '1' COMMENT '노출 순서',
  `NW_ITEM_LINK_URL_TY` char(1) NOT NULL DEFAULT 'I' COMMENT '이동설정 O:외부링크 I:내부링크',
  `NW_ITEM_LINK_URL` varchar(512) DEFAULT NULL COMMENT '주요아이템 이미지 내부링크 URL',
