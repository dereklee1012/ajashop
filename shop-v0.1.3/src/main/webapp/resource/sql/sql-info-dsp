  `PROD_INFO_CONT10` varchar(1024) DEFAULT NULL COMMENT '상품정보내용9',
  `PROD_INFO_CONT11` varchar(1024) DEFAULT NULL COMMENT '상품정보내용9',
  `PROD_INFO_CONT12` varchar(1024) DEFAULT NULL COMMENT '상품정보내용9',
  `PROD_INFO_CONT13` varchar(1024) DEFAULT NULL COMMENT '상품정보내용9',
  `PROD_INFO_CONT14` varchar(256) DEFAULT NULL COMMENT '상품정보내용14',
  `PROD_INFO_CONT15` varchar(256) DEFAULT NULL COMMENT '상품정보내용15',
  `RELEVANT_WORDS` varchar(1023) DEFAULT NULL COMMENT '관련주제 키워드, 해시태그',
  `NOTICE` varchar(1023) DEFAULT NULL COMMENT 'NOTICE',
  `REVIEW_CNT` int(11) NOT NULL DEFAULT '0' COMMENT '리뷰개수',
  `AVG_STAR_POINT` decimal(3,1) NOT NULL DEFAULT '0.0' COMMENT 'AVG(STAR_POINT) = (이전(AVG*개수)+PT) / 리뷰개수+1',
  `CNT_EXCG_PROD_COUPON` int(11) NOT NULL DEFAULT '0' COMMENT '교환가능한 쿠폰상품 개수',
  `GENUINE_MARK_PROD_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '정품인증마크 적용여부',
  `CART_SUPPORT_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '장바구니 지원여부',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`PROD_COUPON_UUID`),
  UNIQUE KEY `UIX_TB_PROD_COUPON_INFO_PROD_CODE` (`PROD_CODE`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_PROD_COUPON_INFO` (`PRODUCT_UUID`),
  KEY `FK_TB_STORE_INFO_TO_TB_PROD_COUPON_INFO` (`STORE_CODE`),
  KEY `FK_TB_BRAND_MST_TO_TB_PROD_COUPON_INFO` (`BRAND_CODE`),
  KEY `FK_TB_AREA_MST_TO_TB_PROD_COUPON_INFO` (`AREA_CODE`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_PROD_COUPON_INFO` (`CTGRY_CODE`),
  KEY `FK_TB_COMPANY_INFO_TO_TB_PROD_COUPON_INFO` (`COMPANY_CODE`),
  CONSTRAINT `FK_TB_AREA_MST_TO_TB_PROD_COUPON_INFO` FOREIGN KEY (`AREA_CODE`) REFERENCES `TB_AREA_MST` (`AREA_CODE`),
  CONSTRAINT `FK_TB_BRAND_MST_TO_TB_PROD_COUPON_INFO` FOREIGN KEY (`BRAND_CODE`) REFERENCES `TB_BRAND_MST` (`BRAND_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_PROD_COUPON_INFO` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_COMPANY_INFO_TO_TB_PROD_COUPON_INFO` FOREIGN KEY (`COMPANY_CODE`) REFERENCES `TB_COMPANY_INFO` (`COMPANY_CODE`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_PROD_COUPON_INFO` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`),
  CONSTRAINT `FK_TB_STORE_INFO_TO_TB_PROD_COUPON_INFO` FOREIGN KEY (`STORE_CODE`) REFERENCES `TB_STORE_INFO` (`STORE_CODE`)
) ENGINE=InnoDB AUTO_INCREMENT=1871 DEFAULT CHARSET=utf8 COMMENT='상품_쿠폰상품 정보';

