  `LAT` varchar(20) DEFAULT NULL COMMENT '위도',
  `LNG` varchar(20) DEFAULT NULL COMMENT '경도',
  `INET_IP` int(11) DEFAULT NULL COMMENT 'INET_ATON(expr)사용',
  `IP_ADDR` varchar(30) DEFAULT NULL COMMENT '(형태) 127.0.0.1 / INET_IP만 사용',
