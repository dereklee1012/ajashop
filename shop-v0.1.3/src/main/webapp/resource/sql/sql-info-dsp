DROP TABLE IF EXISTS `TB_PLM_STOCK_RECEIPT`;
CREATE TABLE `TB_PLM_STOCK_RECEIPT` (
  `STOCK_RECEIPT_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '입고시 Insert',
  `STOCK_SEQNO` int(11) DEFAULT NULL COMMENT '재고관리SEQ',
  `RECEIPT_DATE` varchar(8) NOT NULL COMMENT '입고일자',
  `SUPLER_COMPANY_NAME` varchar(63) DEFAULT NULL COMMENT '업체명',
  `SUPLER_GOODS_CODE` varchar(30) DEFAULT NULL COMMENT '없을경우 880+0000+일련번호(5)+체크비트(1)',
  `RETAIL_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '부가세 있을경우, 포함하여 입력',
  `SUPPLY_PRICE` decimal(13,0) NOT NULL DEFAULT '0' COMMENT '부가세 있을경우, 제외하고 입력',
  `RECEIPT_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '입고 수량',
  `DELIVERY_DATE` varchar(8) DEFAULT NULL COMMENT '납품일자',
  `DELIVERY_QTY` int(11) DEFAULT NULL COMMENT '납품수량,입고수량 비교위해',
  `PLM_STOCK_QTY` int(11) NOT NULL DEFAULT '0' COMMENT '실보유량=입고-출고',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) NOT NULL DEFAULT '0' COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`STOCK_RECEIPT_SEQNO`),
  KEY `FK_TB_PLM_STOCK_INFO_TO_TB_PLM_STOCK_RECEIPT` (`STOCK_SEQNO`),
  CONSTRAINT `FK_TB_PLM_STOCK_INFO_TO_TB_PLM_STOCK_RECEIPT` FOREIGN KEY (`STOCK_SEQNO`) REFERENCES `TB_PLM_STOCK_INFO` (`STOCK_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=229 DEFAULT CHARSET=utf8 COMMENT='재고-입고정보';

-------------------------------------------------------

