  `STOCK_RECEIPT_CFM_QTY` tinyint(4) DEFAULT NULL COMMENT '입고확인수량',
  `STOCK_RECEIPT_YN` char(1) DEFAULT 'N' COMMENT '입고확인여부',
  `STOCK_RECEIPT_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '입고확인시간',
  PRIMARY KEY (`JOB_ORDER_SEQNO`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_JOB_ORDER_PROD_RECIPT` (`PLM_PROD_SEQNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_PLM_JOB_ORDER_PROD_RECIPT` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_PLM_JOB_ORDER_PROD_RECIPT` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_PLM_JOB_ORDER_LIST_TO_TB_PLM_JOB_ORDER_PROD_RECIPT` FOREIGN KEY (`JOB_ORDER_SEQNO`) REFERENCES `TB_PLM_JOB_ORDER_LIST` (`JOB_ORDER_SEQNO`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_JOB_ORDER_PROD_RECIPT` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`)
) ENGINE=InnoDB AUTO_INCREMENT=127 DEFAULT CHARSET=utf8 COMMENT='작업지시리스트-입고상품내역';

