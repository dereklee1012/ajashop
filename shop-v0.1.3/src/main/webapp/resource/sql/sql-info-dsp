DROP TABLE IF EXISTS `TB_EVENT_CTGRY_INFO`;
CREATE TABLE `TB_EVENT_CTGRY_INFO` (
  `EVENT_STORE_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '이벤트 매장 SEQNO',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `EVENT_SEQNO` int(11) DEFAULT NULL COMMENT '이벤트 일련번호',
  PRIMARY KEY (`EVENT_STORE_SEQNO`),
  KEY `IX_TB_EVENT_CTGRY_INFO` (`CTGRY_CODE`),
  KEY `FK_TB_EVENT_INFO_TO_TB_EVENT_CTGRY_INFO` (`EVENT_SEQNO`),
  CONSTRAINT `FK_TB_EVENT_INFO_TO_TB_EVENT_CTGRY_INFO` FOREIGN KEY (`EVENT_SEQNO`) REFERENCES `TB_EVENT_INFO` (`EVENT_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='이벤트_관련매장 정보2';

