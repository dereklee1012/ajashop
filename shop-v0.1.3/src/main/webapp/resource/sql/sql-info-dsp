DROP TABLE IF EXISTS `TB_PLM_JOB_ORDER_PROD_ISSUE`;
CREATE TABLE `TB_PLM_JOB_ORDER_PROD_ISSUE` (
  `JOB_ORDER_SEQNO` int(11) NOT NULL COMMENT '작업할당SEQNO',
  `PLM_PROD_SEQNO` int(11) NOT NULL COMMENT 'PLM_상품정보SEQ',
  `ORDER_SEQNO` int(11) NOT NULL COMMENT 'ORDER_SEQNO',
  `STOCK_ISSUE_CFM_QTY` tinyint(4) DEFAULT NULL COMMENT '출고확인수량',
  `STOCK_ISSUE_YN` char(1) DEFAULT 'N' COMMENT '출고확인여부',
  `STOCK_ISSUE_DT` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '출고확인시간',
  `STOCK_ISSUE_QTY` tinyint(4) DEFAULT NULL COMMENT '출고수량',
  PRIMARY KEY (`JOB_ORDER_SEQNO`,`PLM_PROD_SEQNO`,`ORDER_SEQNO`),
  KEY `FK_TB_ORDER_INFO_TO_TB_PLM_JOB_ORDER_PROD_ISSUE` (`ORDER_SEQNO`),
  KEY `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_JOB_ORDER_PROD_ISSUE` (`PLM_PROD_SEQNO`),
  CONSTRAINT `FK_TB_ORDER_INFO_TO_TB_PLM_JOB_ORDER_PROD_ISSUE` FOREIGN KEY (`ORDER_SEQNO`) REFERENCES `TB_ORDER_INFO` (`ORDER_SEQNO`),
  CONSTRAINT `FK_TB_PLM_JOB_ORDER_LIST_TO_TB_PLM_JOB_ORDER_PROD_ISSUE` FOREIGN KEY (`JOB_ORDER_SEQNO`) REFERENCES `TB_PLM_JOB_ORDER_LIST` (`JOB_ORDER_SEQNO`),
  CONSTRAINT `FK_TB_PLM_PRODUCT_INFO_TO_TB_PLM_JOB_ORDER_PROD_ISSUE` FOREIGN KEY (`PLM_PROD_SEQNO`) REFERENCES `TB_PLM_PRODUCT_INFO` (`PLM_PROD_SEQNO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='작업지시리스트-출고상품내역';

