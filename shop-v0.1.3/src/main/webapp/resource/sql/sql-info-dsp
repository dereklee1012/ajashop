  `BANNER_IMG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '배너파일명',
  `BANNER_IMG_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT '배너(원)파일명',
  `BANNER_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '배너이미지사이즈_H',
  `BANNER_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '배너이미지사이즈_V',
  `THUMNAIL_PATH` varchar(255) DEFAULT NULL COMMENT '썸네일 경로',
  `THUMNAIL_FILENAME` varchar(128) DEFAULT NULL COMMENT '썸네일 파일명',
  `THUMNAIL_ORG_FILENAME` varchar(128) DEFAULT NULL COMMENT '썸네일 (원)파일명',
  `THUMNAIL_SIZE_H` decimal(4,0) DEFAULT NULL COMMENT '썸네일 H',
  `THUMNAIL_SIZE_V` decimal(4,0) DEFAULT NULL COMMENT '썸네일 V',
  `EVENT_MVMN_DTRMN` char(1) DEFAULT NULL COMMENT 'P(상품) B(브랜드)C(카테고리) E(이벤트) O(OutLink)',
  `PRODUCT_UUID` int(11) DEFAULT NULL COMMENT '상품UUID',
  `BRAND_CODE` char(10) DEFAULT NULL COMMENT '브랜드코드',
  `CTGRY_CODE` char(10) DEFAULT NULL COMMENT '카테고리 코드',
  `EVENT_SEQNO` int(11) DEFAULT NULL COMMENT '이벤트 일련번호',
  `INTERNAL_MENU_LINK_URL` varchar(512) DEFAULT NULL COMMENT '내부메뉴링크URL',
  `EXTRL_LINK_URL` varchar(512) DEFAULT NULL COMMENT '외부링크URL',
  `USE_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'USE_YN',
  `IS_DELETED` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'IS_DELETED',
  `CREATED_NO` int(11) DEFAULT NULL COMMENT 'CREATED_NO',
  `CREATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'CREATED_AT',
  `UPDATED_NO` int(11) DEFAULT NULL COMMENT 'UPDATED_NO',
  `UPDATED_AT` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'UPDATED_AT',
  PRIMARY KEY (`BANNER_SEQNO`),
  KEY `FK_TB_PRODUCT_INFO_TO_TB_BANNER_INFO` (`PRODUCT_UUID`),
  KEY `FK_TB_BRAND_MST_TO_TB_BANNER_INFO` (`BRAND_CODE`),
  KEY `FK_TB_CATEGORY_MST_TO_TB_BANNER_INFO` (`CTGRY_CODE`),
  KEY `FK_TB_EVENT_INFO_TO_TB_BANNER_INFO` (`EVENT_SEQNO`),
  CONSTRAINT `FK_TB_BRAND_MST_TO_TB_BANNER_INFO` FOREIGN KEY (`BRAND_CODE`) REFERENCES `TB_BRAND_MST` (`BRAND_CODE`),
  CONSTRAINT `FK_TB_CATEGORY_MST_TO_TB_BANNER_INFO` FOREIGN KEY (`CTGRY_CODE`) REFERENCES `TB_CATEGORY_MST` (`CTGRY_CODE`),
  CONSTRAINT `FK_TB_EVENT_INFO_TO_TB_BANNER_INFO` FOREIGN KEY (`EVENT_SEQNO`) REFERENCES `TB_EVENT_INFO` (`EVENT_SEQNO`),
  CONSTRAINT `FK_TB_PRODUCT_INFO_TO_TB_BANNER_INFO` FOREIGN KEY (`PRODUCT_UUID`) REFERENCES `TB_PRODUCT_INFO` (`PRODUCT_UUID`)
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8 COMMENT='배너정보';

