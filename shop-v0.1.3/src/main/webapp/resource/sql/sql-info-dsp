DROP TABLE IF EXISTS `TB_NOTICE_DET`;
CREATE TABLE `TB_NOTICE_DET` (
  `NOTICE_DET_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT '공지상세 SEQNO',
  `NOTICE_CODE` int(11) DEFAULT NULL COMMENT '공지번호',
  `NOTITYPE_CD` char(2) NOT NULL DEFAULT '00' COMMENT '공지타입(카테고리)00:전체,01:가맹점,02:고객',
  `NOTI_NM_EN` varchar(127) DEFAULT NULL COMMENT '공지제목(영어)',
  `NOTI_NM_KO` varchar(127) DEFAULT NULL COMMENT '공지제목(한국어)',
  `NOTI_NM_ZH` varchar(127) DEFAULT NULL COMMENT '공지제목(간체)',
  `NOTI_CONTENTS_EN` varchar(1024) DEFAULT NULL COMMENT '공지내용(영어)',
  `NOTI_CONTENTS_KO` varchar(1024) DEFAULT NULL COMMENT '공지내용(한국어)',
  `NOTI_CONTENTS_ZH` varchar(1024) DEFAULT NULL COMMENT '공지내용(간체)',
  `NOTI_PATH` varchar(255) DEFAULT NULL COMMENT '공지사항 이미지 파일경로',
  `NOTI_FILE_NM` varchar(128) DEFAULT NULL COMMENT '공지사항 이미지 파일명',
  `NOTI_ORG_FILE_NM` varchar(128) DEFAULT NULL COMMENT 'NEW_FILENAME',
  `NOTI_IMG_SIZE_H` int(11) DEFAULT '0' COMMENT '공지사항 이미지 파일 사이즈_H',
  `NOTI_IMG_SIZE_V` int(11) DEFAULT '0' COMMENT '공지사항 이미지 파일 사이즈_V',
  PRIMARY KEY (`NOTICE_DET_SEQNO`),
  KEY `FK_TB_NOTICE_INFO_TO_TB_NOTICE_DET` (`NOTICE_CODE`),
  CONSTRAINT `FK_TB_NOTICE_INFO_TO_TB_NOTICE_DET` FOREIGN KEY (`NOTICE_CODE`) REFERENCES `TB_NOTICE_INFO` (`NOTICE_CODE`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 COMMENT='공지(NOTI)_상세정보';

