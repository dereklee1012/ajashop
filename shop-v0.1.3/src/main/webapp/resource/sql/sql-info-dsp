DROP TABLE IF EXISTS `TB_COMPANY_RS_INFO`;
CREATE TABLE `TB_COMPANY_RS_INFO` (
  `RS_SEQNO` int(11) NOT NULL AUTO_INCREMENT COMMENT 'SEQNO',
  `COMPANY_CODE` char(10) NOT NULL COMMENT '업체코드',
  `START_DATE` date NOT NULL COMMENT 'START_DATE',
  `END_DATE` date NOT NULL COMMENT 'END_DATE',
  `RS_RATE` decimal(6,3) DEFAULT '0.000' COMMENT '상품판매당 수수료율',
  `RS_RATE_APPL_DAYS` int(11) DEFAULT NULL COMMENT '최초 주문 시 여행사 선택한 날 기준',
  PRIMARY KEY (`RS_SEQNO`),
  KEY `FK_TB_COMPANY_INFO_TO_TB_COMPANY_RS_INFO` (`COMPANY_CODE`),
  CONSTRAINT `FK_TB_COMPANY_INFO_TO_TB_COMPANY_RS_INFO` FOREIGN KEY (`COMPANY_CODE`) REFERENCES `TB_COMPANY_INFO` (`COMPANY_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='업체 수익쉐어 정보';

